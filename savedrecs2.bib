@article{ ISI:000605369900011,
Author = {Marsiglio, Simone and Privileggi, Fabio},
Title = {{On the economic growth and environmental trade-off: a multi-objective
   analysis}},
Journal = {{ANNALS OF OPERATIONS RESEARCH}},
Year = {{2021}},
Volume = {{296}},
Number = {{1-2, SI}},
Pages = {{263-289}},
Month = {{JAN}},
Note = {{International Conference on Multiple Objective Programming and Goal
   Programming (MOPGP), Univ Lorraine, LCOMS Lab, Metz, FRANCE, OCT, 2017}},
Abstract = {{We develop a multicriteria approach, based on both scalarization and
   goal programming techniques, in order to analyze the trade off between
   economic growth and environmental outcomes in a framework in which the
   economy and environment relation is bidirectional. On the one hand,
   economic growth by stimulating production activities gives rise to
   emissions of pollutants which deteriorate the environment. On the other
   hand, the environment affects economic activities since pollution
   generates a production externality determining how much output the
   economy can produce and reducing welfare. In this setting we show that
   optimality dictates an initial overshooting followed by economic
   degrowth and rising pollution. This implies that independently of the
   relative importance of economic and environmental factors, it is
   paradoxically optimal for the economy to asymptotically reach the
   maximum pollution level that the environment is able to bear.}},
