@article{ ISI:000596187000001,
Author = {Meng, Fanao and Liang, Xiujuan and Xiao, Changlai and Wang, Ge},
Title = {{Geothermal resource potential assessment utilizing GIS - based multi
   criteria decision analysis method}},
Journal = {{GEOTHERMICS}},
Year = {{2021}},
Volume = {{89}},
Month = {{JAN}},
Abstract = {{The accurate determination of potential geothermal resource locations is
   of great significance for efficient exploration and development. We
   identified five contributing criteria relevant to geothermal resources
   in northeastern China's Changbai Mountain region include shallow ground
   temperature, fault lines, hot springs location, basalt distribution, and
   isogravity lines. These criteria were used to generate five grid maps:
   temperature distribution, distance to fault lines, distance to hot
   springs, distance to basalts, and distance to grabens. After mapping
   each criterion, the weights of each were determined using an analytic
   hierarchy process and entropy weight method, and the geothermal
   potential area was generated using geographic information system based
   multi-criteria decision analysis method. The map of the geothermal
   favorable area was divided into six grades. One geothermal well is
   located at the junction of the extremely high favorable area and very
   high favorable area; the other two geothermal wells are located in the
   very high favorable area. These three geothermal wells are helpful in
   verifying the correctness of the results.}},
