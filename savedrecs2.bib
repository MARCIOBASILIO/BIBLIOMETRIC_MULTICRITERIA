
@article{ ISI:000607976100005,
Author = {Jain, Vineet and Chand, Mahesh},
Title = {{Decision making in FMS by COPRAS approach}},
Journal = {{INTERNATIONAL JOURNAL OF BUSINESS PERFORMANCE MANAGEMENT}},
