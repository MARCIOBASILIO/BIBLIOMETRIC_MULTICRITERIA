@article{ ISI:000603212100001,
Author = {Yang, Chih-Hao and Lee, Hsiu-Li and Tsai, Wen-Hsien and Chuang, Sophia},
Title = {{Sustainable Smart Healthcare Information Portfolio Strategy Evaluation:
   An Integrated Activity-Based Costing Decision Model}},
Journal = {{SUSTAINABILITY}},
