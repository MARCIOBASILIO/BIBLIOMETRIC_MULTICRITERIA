@article{ ISI:000581906200001,
Author = {Pourjavad, Ehsan and Shahin, Arash},
Title = {{A hybrid model for analyzing the risks of green supply chain in a fuzzy
   environment}},
Journal = {{JOURNAL OF INDUSTRIAL AND PRODUCTION ENGINEERING}},
Year = {{2020}},
Volume = {{37}},
Number = {{8}},
Pages = {{422-433}},
Month = {{NOV 16}},
Abstract = {{Effective adoption and implementation of green initiatives in greening a
   supply chain are threatened by several risks. Hence, investigating
   appropriate responses to mitigate risks is needed by most companies. The
   aim of this study is to propose a new approach for prioritizing the
   risks of Green Supply Chain (GSC) in a fuzzy environment. For this
   purpose, a fuzzy Multi-criteria Decision-Making (MCDM) approach is
   proposed for which, a systematic methodology is conducted by integrating
   a fuzzy decision-making trial and evaluation laboratory (DEMATEL) and
   fuzzy technique for order of preference by similarity to ideal solution
   (TOPSIS). The proposed approach makes the use of fuzzy DEMATEL in order
   to evaluate the weights of risk criteria. Then, the responses of GSC'
   risks are assessed and ranked based on six main risk categories and 12
   criteria. To highlight the real-life applicability of the proposed
   approach, an empirical case study is conducted.}},
