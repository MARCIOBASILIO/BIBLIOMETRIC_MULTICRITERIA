@article{ ISI:000607093800007,
Author = {Sun, Chu},
Title = {{Evaluation of the Consumer Interaction Capability of the Virtual Brand
   Community of Chinese Mobile Internet Enterprises Based on the Hybrid
   MCDM Method}},
Journal = {{JOURNAL OF INFORMATION \& KNOWLEDGE MANAGEMENT}},
