@article{ ISI:000587502700004,
Author = {Gupta, Ankita and Gupta, Chetna},
Title = {{A Collaborative Effort-Benefit-Value Analysis Model to Support
   Requirements Reuse for Software Requirements Prioritization}},
Journal = {{INTERNATIONAL JOURNAL OF SOFTWARE INNOVATION}},
Year = {{2021}},
Volume = {{9}},
Number = {{1}},
Pages = {{37-51}},
Month = {{JAN-MAR}},
Abstract = {{This paper proposes a collaborative method to prioritize the set of
   requirements backing the concept of requirement re usability through
   crowd sourcing platform. Both stakeholder and developer perspectives are
   captured and effort-benefit-value analysis model is presented.
   Stakeholder priority is captured using Fuzzy TOPSIS model, and developer
   priority is captured by analyzing inter-relationships among requirements
   in the form of dependency. Further, a provision is provided to resolve
   conflicts between requirement pairs in an interactive manner to produce
   final ranking accepted to both with minimum conflict. An experimental
   proof of concept is provided by comparing results of proposed approach
   with state of art, fuzzy analytic hierarchy process (FAHP), interactive
   genetic algorithm (IGA) techniques, and on a dataset. Results of
   experimentation conclude that presented approach is capable of producing
   accurate and comparable results in addition to handling issue of
   dependency, multi- criteria preferences, reliability, and robustness to
   errors.}},
