@article{ ISI:000613810400005,
Author = {Maulana, H. and Kanai, H.},
Title = {{MULTI-CRITERIA DECISION ANALYSIS FOR DETERMINING POTENTIAL AGRICULTURE
   COMMODITIES IN INDONESIA}},
Journal = {{JOURNAL OF ENGINEERING SCIENCE AND TECHNOLOGY}},
