@article{ ISI:000604990400003,
Author = {Phan-Nguyen Huu},
Title = {{Multi-objective optimization in titanium powder mixed electrical
   discharge machining process parameters for die steels}},
Journal = {{ALEXANDRIA ENGINEERING JOURNAL}},
