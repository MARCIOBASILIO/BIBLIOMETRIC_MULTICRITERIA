@article{ ISI:000597909100001,
Author = {Ortega, Jairo and Moslem, Sarbast and Toth, Janos and Peter, Tamas and
   Palaguachi, Juan and Paguay, Mario},
Title = {{Using Best Worst Method for Sustainable Park and Ride Facility Location}},
