@article{ ISI:000593228600001,
Author = {Reig-Mullor, Javier and Pla-Santamaria, David and Garcia-Bernabeu, Ana},
Title = {{Extended Fuzzy Analytic Hierarchy Process (E-FAHP): A General Approach}},
