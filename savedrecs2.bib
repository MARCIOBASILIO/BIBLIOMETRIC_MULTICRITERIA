@article{ ISI:000575874200018,
Author = {Sava, M. Gabriela and Vargas, Luis G. and May, Jerrold H. and Dolan,
   James G.},
Title = {{An analysis of the sensitivity and stability of patients' preferences
   can lead to more appropriate medical decisions}},
Journal = {{ANNALS OF OPERATIONS RESEARCH}},
Year = {{2020}},
Volume = {{293}},
Number = {{2, SI}},
Pages = {{863-901}},
Month = {{OCT}},
Note = {{24th International Conference on Multiple Criteria Decision Making
   (MCDM) - Creating a Sustainable Society, Ottawa, CANADA, JUL, 2017}},
Abstract = {{Patients are faced with multiple alternatives when selecting the
   preferred method for colorectal cancer screening, and there are multiple
   criteria to be considered in the decision-making process. We model
   patients' choices using a multicriteria decision model, specifically an
   Analytic Hierarchy Process-based model, and propose a new approach for
   characterizing the idiosyncratic preference regions for individual
   patients. The new approach involves the development of a personalized
   sensitivity and stability analysis of preferences that provides
   pertinent insights about the changes that occur in a patient's
