@article{ ISI:000577280000001,
Author = {Le Tellier, Mathilde and Berrah, Lamia and Cliville, Vincent and Audy,
   Jean-Francois and Stutz, Benoit and Barnabe, Simon},
Title = {{Using MACBETH for the performance expression of a mixeduse ecopark}},
Journal = {{JOURNAL OF MULTI-CRITERIA DECISION ANALYSIS}},
Year = {{2021}},
