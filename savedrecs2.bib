@article{ ISI:000594020600001,
Author = {Segura, Marina and Maroto, Concepcion and Segura, Baldomero and
   Casas-Rosal, Jose Carlos},
Title = {{Improving Food Supply Chain Management by a Sustainable Approach to
   Supplier Evaluation}},
