@article{ ISI:000585771300001,
Author = {Ayyildiz, Ertugrul and Gumus, Alev Taskin},
Title = {{Interval-valued Pythagorean fuzzy AHP method-based supply chain
   performance evaluation by a new extension of SCOR model: SCOR 4.0}},
Journal = {{COMPLEX \& INTELLIGENT SYSTEMS}},
