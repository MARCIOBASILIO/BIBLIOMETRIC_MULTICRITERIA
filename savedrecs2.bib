@article{ ISI:000576365200001,
Author = {Meshram, Sarita Gajbhiye and Singh, Vijay P. and Kahya, Ercan and
   Alvandi, Ehsan and Meshram, Chandrashekhar and Sharma, Shailesh Kumar},
Title = {{The Feasibility of Multi-Criteria Decision Making Approach for
   Prioritization of Sensitive Area at Risk of Water Erosion}},
