@article{ ISI:000594997000001,
Author = {Lu, Changli and Zhao, Ming and Khan, Imran and Uthansakul, Peerapong},
Title = {{Prospect Theory Based Hesitant Fuzzy Multi-Criteria Decision Making for
   Low Sulphur Fuel of Maritime Transportation}},
Journal = {{CMC-COMPUTERS MATERIALS \& CONTINUA}},
