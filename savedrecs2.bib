@article{ ISI:000582270100001,
Author = {Souza Rodrigues, Brunno and Martins Floriano, Carla and Pereira, Valdecy
   and Costa Roboredo, Marcos},
Title = {{An algorithm to elicitate ELECTRE II, III and IV parameters}},
Journal = {{DATA TECHNOLOGIES AND APPLICATIONS}},
