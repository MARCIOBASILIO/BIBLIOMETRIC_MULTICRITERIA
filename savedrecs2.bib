@article{ ISI:000605369900035,
Author = {Ferreira, Fernando A. F. and Santos, Sergio P.},
Title = {{Two decades on the MACBETH approach: a bibliometric analysis}},
Journal = {{ANNALS OF OPERATIONS RESEARCH}},
Year = {{2021}},
