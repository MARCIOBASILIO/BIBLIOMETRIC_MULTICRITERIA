@article{ ISI:000604906200004,
Author = {Awaga, A. L. and Xu, W. and Liu, L. and Zhang, Y.},
Title = {{Evolutionary game of green manufacturing mode of enterprises under the
   influence of government reward and punishment}},
Journal = {{ADVANCES IN PRODUCTION ENGINEERING \& MANAGEMENT}},
