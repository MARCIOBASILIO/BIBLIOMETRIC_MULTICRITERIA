@article{ ISI:000464486100011,
Author = {Kim, Jong Hyen and Ahn, Byeong Seok},
Title = {{Extended VIKOR method using incomplete criteria weights}},
Journal = {{EXPERT SYSTEMS WITH APPLICATIONS}},
