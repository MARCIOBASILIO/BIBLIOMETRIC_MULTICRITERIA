@article{ ISI:000475972000004,
Author = {Vogler, Sabine},
Title = {{Fair prices for medicines? Exploring competent authorities' and public
   payers' preferences on pharmaceutical policies}},
Journal = {{EMPIRICA}},
Year = {{2019}},
Volume = {{46}},
Number = {{3, SI}},
Pages = {{443-469}},
Month = {{AUG}},
