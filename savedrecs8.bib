@article{ ISI:000471010300188,
Author = {Kim, BumChoong and Kim, Juhan and Kim, Jinsoo},
Title = {{Evaluation Model for Investment in Solar Photovoltaic Power Generation
   Using Fuzzy Analytic Hierarchy Process}},
