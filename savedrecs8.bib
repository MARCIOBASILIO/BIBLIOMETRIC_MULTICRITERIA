@article{ ISI:000478164900001,
Author = {Yazdi, Mohammad},
Title = {{Introducing a heuristic approach to enhance the reliability of system
   safety assessment}},
Journal = {{QUALITY AND RELIABILITY ENGINEERING INTERNATIONAL}},
