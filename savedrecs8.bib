@article{ ISI:000458128800001,
Author = {Joshi, Rajesh and Kumar, Satish},
Title = {{A dissimilarity measure based on Jensen Shannon divergence measure}},
Journal = {{INTERNATIONAL JOURNAL OF GENERAL SYSTEMS}},
Year = {{2019}},
Volume = {{48}},
Number = {{3}},
Pages = {{280-301}},
Month = {{APR 3}},
Abstract = {{The need of suitable measures to find the distance between two
   probability distributions arises as they play an eminent role in
   problems based on discrimination and inferences. In this communication,
   we have introduced one such divergence measure based on well-known
   Shannon entropy and established its existence. In addition to this, a
   new dissimilarity measure for intuitionistic fuzzy sets corresponding to
   proposed divergence measure is also introduced and validated. Some major
   properties of the proposed dissimilarity measure are also discussed.
   Further, a new multiple attribute decision-making ( MADM) method based
   on the proposed dissimilarity measure is introduced by using the concept
   of TOPSIS and is thoroughly explained with the help of an illustrated
   example on supplier selection problem. Finally, the application of
   proposed dissimilarity measure is given in pattern recognition and the
   performance is compared with some existing divergence measures in the
   literature.}},
