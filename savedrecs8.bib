@article{ ISI:000472547700008,
Author = {Riccioli, F. and Gabbrielli, E. and Casini, L. and Marone, E. and El
   Asmar, J. P. and Fratini, R.},
Title = {{Geographical Analysis of Agro-Environmental Measures for Reduction of
   Chemical Inputs in Tuscany}},
