@article{ ISI:000481468700016,
Author = {Yucenur, G. Nilay and Subasi, Anil Sezer},
Title = {{An integrated solution for space shuttle launching ramp}},
Journal = {{AIRCRAFT ENGINEERING AND AEROSPACE TECHNOLOGY}},
Year = {{2019}},
