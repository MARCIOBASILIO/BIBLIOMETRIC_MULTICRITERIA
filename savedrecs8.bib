Title = {{Some 2-tuple linguistic Pythagorean Heronian mean operators and their
   application to multiple attribute decision-making}},
Journal = {{JOURNAL OF EXPERIMENTAL \& THEORETICAL ARTIFICIAL INTELLIGENCE}},
Year = {{2019}},
Volume = {{31}},
