@article{ ISI:000489722700003,
Author = {Eslamkhah, Mahdi and Seno, Seyed Amin Hosseini},
Title = {{Identifying and Ranking Knowledge Management Tools and Techniques
   Affecting Organizational Information Security Improvement}},
