@article{ ISI:000466455800006,
Author = {Zeng, Shouzhen and Chen, Shyi-Ming and Kuo, Li-Wei},
Title = {{Multiattribute decision making based on novel score function of
   intuitionistic fuzzy values and modified VIKOR method}},
Journal = {{INFORMATION SCIENCES}},
Year = {{2019}},
Volume = {{488}},
Pages = {{76-92}},
Month = {{JUL}},
Abstract = {{In this paper, we propose a new multiattribute decision making (MADM)
   method based on the proposed new score function of intuitionistic fuzzy
   values (IFVs) and the proposed modified VIKOR method. The proposed new
   score function can overcome the drawbacks of the existing score
   functions, the existing accuracy functions and the existing measure
   methods for ranking IFVs. The proposed modified VIKOR method is based on
   the proposed new score function of IFVs, which not only calculates the
   closest ideal score value of each alternative, but also calculates the
   farthest worst score value of each alternative. Moreover, the proposed
   MADM method uses the concept of the TOPSIS method to rank alternatives.
   The smaller the degree of relative closeness, the better the preference
   order of the alter-native. The proposed MADM method can overcome the
   drawbacks of the existing MADM methods for MADM in intuitionistic fuzzy
   environments. (C) 2019 Elsevier Inc. All rights reserved.}},
