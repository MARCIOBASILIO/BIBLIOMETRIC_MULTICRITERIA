@article{ ISI:000465054900008,
Author = {Watrobski, Jaroslaw and Jankowski, Jaroslaw and Ziemba, Pawel and
   Karczmarczyk, Artur and Ziolo, Magdalena},
Title = {{Generalised framework for multi-criteria method selection}},
