@article{ ISI:000465250500036,
Author = {Xu, Gongyue and Ding, Huafeng and Feng, Zemin},
Title = {{Optimal Design of Hydraulic Excavator Shovel Attachment Based on
   Multiobjective Evolutionary Algorithm}},
