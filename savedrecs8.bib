@article{ ISI:000471064400005,
Author = {Amin, Fazli and Fahmi, Aliya and Abdullah, Saleem},
Title = {{Dealer using a new trapezoidal cubic hesitant fuzzy TOPSIS method and
   application to group decision-making program}},
Journal = {{SOFT COMPUTING}},
