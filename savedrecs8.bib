@article{ ISI:000468824600002,
Author = {Bukhsh, Zaharah Allah and Stipanovic, Irina and Klanker, Giel and O'
   Connor, Alan and Doree, Andre G.},
Title = {{Network level bridges maintenance planning using Multi-Attribute Utility
   Theory}},
Journal = {{STRUCTURE AND INFRASTRUCTURE ENGINEERING}},
