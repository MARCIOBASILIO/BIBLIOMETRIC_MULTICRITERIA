@article{ ISI:000469154100017,
Author = {Stirbanovic, Zoran and Stanujkic, Dragisa and Miljanovic, Igor and
   Milanovic, Dragan},
Title = {{Application of MCDM methods for flotation machine selection}},
Journal = {{MINERALS ENGINEERING}},
Year = {{2019}},
Volume = {{137}},
Pages = {{140-146}},
Month = {{JUN 15}},
Abstract = {{MCDM methods can be used in many areas when decision making process is
   weighed on the basis of many influential parameters. Results of this
   study show that MCDM methods, such as TOPSIS and VIKOR, can also be
   applied for flotation machine selection.
   Five flotation machines were evaluated by three experts using ten
   criteria divided in three groups: constructional, economical and
   technical and the results of their evaluation were the basis for
   application of VIKOR and TOPSIS methods. In the first step of the
   selection the results obtained with these two MCDM methods were
   different: machine A(4) was seen as the best alternative by TOPSIS,
   while VIKOR recognized machine A(3) as the best alternative. In order to
   determine which of these two machines was really the most appropriate
   one, the machines were re-evaluated based only on two groups of
   criteria: constructional and economical. The results of the additional
   evaluation showed that the machine A(4) was the best ranked by both
   methods.}},
