@article{ ISI:000463801600003,
Author = {Mishra, Shraddha and Singh, Surya Prakash and Johansen, John and Cheng,
   Yang and Farooq, Sami},
Title = {{Evaluating indicators for international manufacturing network under
   circular economy}},
