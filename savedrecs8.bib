@article{ ISI:000473642200015,
Author = {Rashid, Tabasam and Faizi, Shahzad and Zafar, Sohail},
Title = {{Outranking method for intuitionistic 2-tuple fuzzy linguistic
   information model in group decision making}},
Journal = {{SOFT COMPUTING}},
Year = {{2019}},
Volume = {{23}},
