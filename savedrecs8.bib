@article{ ISI:000472691300013,
Author = {Zhang, Haowei and Xie, Junwei and Lu, Wenlong and Zhang, Zhaojian and
   Fu, Xiaolong},
Title = {{Novel ranking method for intuitionistic fuzzy values based on
   information fusion}},
Journal = {{COMPUTERS \& INDUSTRIAL ENGINEERING}},
Year = {{2019}},
Volume = {{133}},
Pages = {{139-152}},
Month = {{JUL}},
Abstract = {{In this paper, a novel ranking method for intuitionistic fuzzy values is
   proposed and applied to multi-attribute decision making problems. At
   first, a new intuitionistic fuzzy entropy (IFE) is designed to fully
   express the two facets of uncertainty expressed by intuitionistic fuzzy
   sets (IFSs): the fuzziness and the unknown. Then, after considering the
   distance to the positive ideal point and the negative ideal point
   simultaneously, the closeness degree of technique for order preference
   by similarity to ideal solution (TOPSIS) is used to characterize the
   amount of information conveyed by IFSs. The closeness degree is merged
   with the designed IFE by DMs' risk attitude, and a novel single-time
   ranking method is constructed. After that, by minimizing the distance to
   the objectivity and subjectivity simultaneously, the optimal attribute
   weights are derived by solving the optimization model. Furthermore, a
   normalized time sequence weight model is proposed to aggregate
   information from multiple time periods. Finally, a benchmark example is
   used to illustrate the flexibility and rationality of the proposed
   method.}},
