@article{ ISI:000473105700121,
Author = {Afifi, Zulfahmi and Chu, Hone-Jay and Kuo, Yen-Lien and Hsu, Yung-Chia
   and Wong, Hock-Kiet and Ali, Muhammad Zeeshan},
Title = {{Residential Flood Loss Assessment and Risk Mapping from High-Resolution
   Simulation}},
