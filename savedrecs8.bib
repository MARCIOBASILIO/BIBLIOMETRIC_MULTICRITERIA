@article{ ISI:000467752200052,
Author = {Erdogan, Seyit Ali and Saparauskas, Jonas and Turskis, Zenonas},
Title = {{A Multi-Criteria Decision-Making Model to Choose the Best Option for
   Sustainable Construction Management}},
