@article{ ISI:000480311600004,
Author = {Jiang, Haibo and Zhan, Jianming and Chen, Degang},
Title = {{Covering-Based Variable Precision (I, T)-Fuzzy Rough Sets With
   Applications to Multiattribute Decision-Making}},
