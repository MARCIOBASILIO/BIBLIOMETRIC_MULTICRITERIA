@article{ ISI:000461531700016,
Author = {Xu, Zhou and Qin, Jindong and Liu, Jun and Martinez, Luis},
Title = {{Sustainable supplier selection based on AHPSort II in interval type-2
   fuzzy environment}},
Journal = {{INFORMATION SCIENCES}},
