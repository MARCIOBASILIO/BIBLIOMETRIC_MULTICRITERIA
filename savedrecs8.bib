@article{ ISI:000477051900041,
Author = {Milosevic, Mimica R. and Milosevic, Dusan M. and Stevic, Dragan M. and
   Stanojevic, Ana D.},
Title = {{Smart City: Modeling Key Indicators in Serbia Using IT2FS}},
Journal = {{SUSTAINABILITY}},
