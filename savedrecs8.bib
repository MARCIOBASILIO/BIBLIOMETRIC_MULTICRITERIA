@article{ ISI:000476914800001,
Author = {Duric, Goran and Todorovic, Gordana and Dordevic, Aleksandar and Tisma,
   Ankica Borota},
Title = {{A new fuzzy risk management model for production supply chain economic
   and social sustainability}},
Journal = {{ECONOMIC RESEARCH-EKONOMSKA ISTRAZIVANJA}},
Year = {{2019}},
Volume = {{32}},
Number = {{1}},
Pages = {{1697-1715}},
Month = {{JUL 23}},
Abstract = {{The issues of operational, organisational and process risk assessment in
   supply chains (SCs) are the most usually analysed, while other risk
   groups (like economic and social risks) are not taken into account, even
   though they have a critical effect on the competitive advantage and SCs
   sustainability over long time periods. The determination of risk value
   that may arise due to the materialisation of each defined risk factor
   (RF) is based on the assessment of the severity of RF consequences and
   frequency of RF occurrence. These judgments are obtained by decision
   makers and modelled by using fuzzy set theory. The relative importance
   of RFs are stated by fuzzy pair-wise comparison matrices in compliance
   with fuzzy analytical hierarchy process (FAHP). The risk level of SCs
   could be obtained in an exact way by applying fuzzy logic. The proposed
   model, to be presented in this paper, provides a possibility to easily
   and simply determine risk level from the automotive industry SC and to
   propose appropriate management initiatives that should lead to a
   reduction or elimination of RF influence.}},
