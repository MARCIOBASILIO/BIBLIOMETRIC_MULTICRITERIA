@article{ ISI:000475519300008,
Author = {Zhang, HongGuang and Wang, Rui and Liu, HuaJian and Luo, Han and Liu,
   Yuanan},
Title = {{MCDMSR: multicriteria decision making selection/replacement based on
   agility strategy for real optimization problems}},
Journal = {{APPLIED INTELLIGENCE}},
