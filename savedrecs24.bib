@inproceedings{ ISI:000333936300029,
Author = {Kiszova, Zuzana and Nevima, Jan},
Editor = {{Kocourek, A}},
Title = {{Evaluation of Czech NUTS 2 Competitiveness Using AHP and Group Decision
   Making}},
Booktitle = {{PROCEEDINGS OF THE 11TH INTERNATIONAL CONFERENCE ON LIBEREC ECONOMIC
   FORUM 2013}},
Year = {{2013}},
