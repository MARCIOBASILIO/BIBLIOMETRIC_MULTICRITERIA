Title = {{Integrating fuzzy DEMATEL and fuzzy hierarchical TOPSIS methods for
   truck selection}},
Journal = {{EXPERT SYSTEMS WITH APPLICATIONS}},
Year = {{2013}},
Volume = {{40}},
