@article{ ISI:000325899400014,
Author = {Madhuri, Bhagat and Aniruddha, Ghare and Rahul, Ralegaonkar},
Title = {{Identification and classification of flood prone areas using AHP, GIS
   and GPS}},
Journal = {{DISASTER ADVANCES}},
