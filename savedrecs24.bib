@article{ ISI:000325810500010,
Author = {Danielson, Mats and Ekenberg, Love and Larsson, Aron and Riabacke, Mona},
Title = {{Weighting Under Ambiguous Preferences and Imprecise Differences in a
   Cardinal Rank Ordering Process}},
Journal = {{INTERNATIONAL JOURNAL OF COMPUTATIONAL INTELLIGENCE SYSTEMS}},
Year = {{2014}},
Volume = {{7}},
Number = {{1, SI}},
Pages = {{105-112}},
Month = {{JAN 1}},
Abstract = {{The limited amount of good tools for supporting elicitation of
   preference information in multi-criteria decision analysis (MCDA) causes
   practical problem. In our experiences, this can be remedied by allowing
   more relaxed input statements from decision-makers, causing the
   elicitation process to be less cognitively demanding. Furthermore, it
   should not be too time consuming and must be able to actually use of the
   information the decision-maker is able to supply. In this paper, we
   propose a useful weight elicitation method for MAVT/MAUT decision
   making, which builds on the ideas of rank-order methods, but increases
   the precision by adding numerically imprecise cardinal information as
   well.}},
