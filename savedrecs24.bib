@article{ ISI:000316774700032,
Author = {Yang, Yu-Ping Ou and Shieh, How-Ming and Tzeng, Gwo-Hshiung},
Title = {{A VIKOR technique based on DEMATEL and ANP for information security risk
   control assessment}},
Journal = {{INFORMATION SCIENCES}},
Year = {{2013}},
