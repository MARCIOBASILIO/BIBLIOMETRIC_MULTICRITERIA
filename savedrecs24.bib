@article{ ISI:000330342900010,
Author = {Pereira, Javier and Autran Monteiro Gomes, Luiz Flavio and Paredes,
   Fernando},
Title = {{ROBUSTNESS ANALYSIS IN A TODIM-BASED MULTICRITERIA EVALUATION MODEL OF
   RENTAL PROPERTIES}},
Journal = {{TECHNOLOGICAL AND ECONOMIC DEVELOPMENT OF ECONOMY}},
Year = {{2013}},
Volume = {{19}},
Number = {{1, SI}},
Pages = {{S176-S190}},
Abstract = {{A new robustness analysis framework is proposed where robustness of a
   solution in a decision aiding process is measured as the distance from
   that solution to an expected outcome, chosen by the decision-aiding
   analyst. The framework is explained by the application of the TODIM
   method of multicriteria decision aiding to the problem of predicting
   rental ranges for properties in a Chilean city. Therefore, the
   robustness concern concentrates on changes in criteria weights as well
   as in trade-off rates, as they are defined in the method. Two main
   contributions are introduced: a local robustness measure, defined in
   terms of a distance among rankings; and a global robustness measure, as
   an adaptation of the minimax-regret rule to select a global robust
   solution, i.e. a ranking produced by TODIM.}},
