@article{ ISI:000310424200026,
Author = {Jimenez, Antonio and Mateos, Alfonso and Sabio, Pilar},
Title = {{Dominance intensity measure within fuzzy weight oriented MAUT: An
   application}},
Journal = {{OMEGA-INTERNATIONAL JOURNAL OF MANAGEMENT SCIENCE}},
Year = {{2013}},
Volume = {{41}},
