@article{ ISI:000318571600005,
Author = {Wang, Yuhong and Deng, Xiaomei and Marcucci, Daniel J. and Le, Yuen},
Title = {{Sustainable Development Planning of Protected Areas near Cities: Case
   Study in China}},
Journal = {{JOURNAL OF URBAN PLANNING AND DEVELOPMENT-ASCE}},
Year = {{2013}},
Volume = {{139}},
Number = {{2}},
Pages = {{133-143}},
Month = {{JUN}},
Abstract = {{The regional level is believed to be an important platform to carry out
   sustainable development (SD) strategies. In China, the rapid
   urbanization process causes city expansions. To protect environmentally
   sensitive areas between cities, some protected areas (PAs) have been
   established. This paper proposes a framework of translating general SD
   principles into specific practices at the regional level, particularly
   for the PAs. The framework is based on and illustrated by a detailed
   case study of the comprehensive SD planning of an environmentally
   sensitive area in China. A multicriteria decision-making (MCDM)
   technique, the analytic network process (ANP), was used to weigh the
   evaluation criteria and rank the proposed capital and social development
   projects. It was found that the framework was helpful in guiding the SD
   planning process and the ANP method provided insights on the interaction
   of various aspects of sustainability. (C) 2013 American Society of Civil
   Engineers.}},
