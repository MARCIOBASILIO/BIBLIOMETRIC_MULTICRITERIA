@article{ ISI:000314039300002,
Author = {Behzadian, Majid and Hosseini-Motlagh, Seyyed-Mahdi and Ignatius, Joshua
   and Goh, Mark and Sepehri, Mohammad Mehdi},
Title = {{PROMETHEE Group Decision Support System and the House of Quality}},
