@article{ ISI:000330041500002,
Author = {Senol, Mehmet Burak and Dagdeviren, Metin and Kurt, Mustafa},
Title = {{A MULTI CRITERIA APPROACH FOR AIRCRAFT COCPIT INTERFACE EVALUATION}},
