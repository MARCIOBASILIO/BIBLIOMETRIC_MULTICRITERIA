@article{ ISI:000313592300006,
Author = {Kalogeras, Nikos and Pennings, Joost M. E. and Benos, Theo and Doumpos,
   Michael},
Title = {{Which Cooperative Ownership Model Performs Better? A Financial-Decision
   Aid Approach}},
Journal = {{AGRIBUSINESS}},
