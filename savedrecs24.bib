@article{ ISI:000326106300009,
Author = {Azzopardi, Brian and Martinez-Cesena, Eduardo A. and Mutale, Joseph},
Title = {{Decision support system for ranking photovoltaic technologies}},
Journal = {{IET RENEWABLE POWER GENERATION}},
