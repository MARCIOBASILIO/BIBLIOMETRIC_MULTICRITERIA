@article{ ISI:000318007100022,
Author = {Zeng, Qiang-Lin and Li, Dan-Dan and Yang, Yi-Bin},
Title = {{VIKOR Method with Enhanced Accuracy for Multiple Criteria Decision
   Making in Healthcare Management}},
