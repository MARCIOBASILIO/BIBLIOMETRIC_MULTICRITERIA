@article{ ISI:000317968200023,
Author = {Mousavi, S. Meysam and Tavakkoli-Moghaddam, R. and Heydar, M. and
   Ebrahimnejad, S.},
Title = {{Multi-Criteria Decision Making for Plant Location Selection: An
   Integrated Delphi-AHP-PROMETHEE Methodology}},
