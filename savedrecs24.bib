@article{ ISI:000320493400006,
Author = {Kadzinski, Milosz and Tervonen, Tommi},
Title = {{Stochastic ordinal regression for multiple criteria sorting problems}},
Journal = {{DECISION SUPPORT SYSTEMS}},
