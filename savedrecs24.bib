@article{ ISI:000315569200021,
Author = {Vinodh, S. and Varadharajan, Ashwin R. and Subramanian, Abhishek},
Title = {{Application of fuzzy VIKOR for concept selection in an agile environment}},
Journal = {{INTERNATIONAL JOURNAL OF ADVANCED MANUFACTURING TECHNOLOGY}},
Year = {{2013}},
Volume = {{65}},
Number = {{5-8}},
Pages = {{825-832}},
Month = {{MAR}},
Abstract = {{Due to the increasing competition, modern manufacturing organisations
   are adopting agile manufacturing (AM) paradigm. AM enables the speedy
   production of customised, high-quality products in varying lot sizes.
   Concept selection in the context of AM is a typical multi-criterion
   decision making problem as it involves several agility criteria. The
   case organisation needs to select the best concept among five
   alternative designs. VIKOR (VlseKriterijumskaOptimizacija I
   KompromisnoResenje in Serbian, meaning multi-criteria optimization and
   compromise solution) has been used to select the best concept design.
   Based on the inputs gathered from decision makers, concept design (A3)
   has been found the best concept design from the perspective of agility.
   The result derived from Fuzzy VIKOR has been compared with fuzzy TOPSIS
   (technique for order preference by similarity to an ideal solution)
   methodology. The selected concept design has been subjected to
   implementation in the case organisation.}},
