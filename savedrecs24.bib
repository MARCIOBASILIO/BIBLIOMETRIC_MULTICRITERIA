@article{ ISI:000310424200029,
Author = {Ramanathan, Usha},
Title = {{Aligning supply chain collaboration using Analytic Hierarchy Process}},
Journal = {{OMEGA-INTERNATIONAL JOURNAL OF MANAGEMENT SCIENCE}},
Year = {{2013}},
