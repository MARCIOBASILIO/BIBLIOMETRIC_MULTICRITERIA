@article{ ISI:000313133700024,
Author = {Amiri, M. J. and Mahiny, A. S. and Hosseini, S. M. and Jalali, S. Gh.
   and Ezadkhasty, Z. and Karami, Sh},
Title = {{OWA Analysis for Ecological Capability Assessment in Watersheds}},
Journal = {{INTERNATIONAL JOURNAL OF ENVIRONMENTAL RESEARCH}},
