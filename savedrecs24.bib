@article{ ISI:000322326300052,
Author = {Ordoobadi, Sharon M.},
Title = {{Application of AHP and Taguchi loss functions in evaluation of advanced
   manufacturing technologies}},
Journal = {{INTERNATIONAL JOURNAL OF ADVANCED MANUFACTURING TECHNOLOGY}},
