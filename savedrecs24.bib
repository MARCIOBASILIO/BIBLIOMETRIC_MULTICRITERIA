@article{ ISI:000320692300010,
Author = {Ic, Yusuf Tansel and Yildirim, Sebla},
Title = {{MOORA-based Taguchi optimisation for improving product or process
   quality}},
Journal = {{INTERNATIONAL JOURNAL OF PRODUCTION RESEARCH}},
Year = {{2013}},
Volume = {{51}},
Number = {{11}},
Pages = {{3321-3341}},
Month = {{JUN 1}},
Abstract = {{In this study a multi-objective optimisation on the basis of ratio
   analysis (MOORA)-based Taguchi application is used to solve
   multi-response optimisation problems. In this application, the MOORA
   method is integrated with the Taguchi method to convert the
   multi-response problem into a single-response problem. Four examples are
   considered in this paper for illustrative purposes. The MOORA-based
   Taguchi method is simple and robust compared to the other MADM methods,
   such as TOPSIS, VIKOR and GRA. The proposed model reduces the time
   associated with the amount of calculation steps significantly. We found
   that solution results of the MOORA-based Taguchi application and other
