@article{ ISI:000329048600012,
Author = {Kremer, Gul E. Okudan and Akman, Gulsen},
Title = {{A TOOL FOR PRODUCT DEVELOPMENT PERFORMANCE MONITORING (PDPM) FOR
   ALIGNMENT WITH COMPETITIVE PRIORITIES}},
Journal = {{INTERNATIONAL JOURNAL OF INFORMATION TECHNOLOGY \& DECISION MAKING}},
Year = {{2013}},
Volume = {{12}},
