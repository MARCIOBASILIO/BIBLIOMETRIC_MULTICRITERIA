@inproceedings{ ISI:000333750100022,
Author = {Milbradt, Rafael G. and Canha, Luciane N. and Zorrilla, Pedro B. and
   Abaide, Alzenira R. and Pereira, Paulo R. and Schmaedecke, Sandro M.},
