@article{ ISI:000328309400022,
Author = {Liu, Hu-Chen and Wu, Jing and Li, Ping},
Title = {{Assessment of health-care waste disposal methods using a VIKOR-based
   fuzzy multi-criteria decision making method}},
Journal = {{WASTE MANAGEMENT}},
