@inproceedings{ ISI:000342235500007,
Author = {AlOmary, Rolla Y. and Khan, Salman A.},
Editor = {{Sadikin, R and Subekti, A}},
Title = {{Goal Programming based Multi-criteria Decision-making for Distributed
   Denial of Service Attacks in Wireless Sensor Networks}},
Booktitle = {{2013 INTERNATIONAL CONFERENCE ON COMPUTER, CONTROL, INFORMATICS AND ITS
   APPLICATIONS (IC3INA)}},
Year = {{2013}},
Pages = {{33-37}},
