@inproceedings{ ISI:000251543000100,
Author = {Mendez, Maximo and Galvan, Blas},
Editor = {{Diaz, RM and Pichler, F and Arencibia, AQ}},
Title = {{Multi-objective evolutionary algorithms using the working point and the
   TOPSIS method}},
