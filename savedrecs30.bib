@article{ ISI:000236500400001,
Author = {Ayag, Z and Ozdemir, RG},
Title = {{A fuzzy AHP approach to evaluating machine tool alternatives}},
Journal = {{JOURNAL OF INTELLIGENT MANUFACTURING}},
