@article{ ISI:000228513800018,
Author = {Kwak, NK and Lee, CW and Kim, JH},
Title = {{An MCDM model for media selection in the dual consumer/industrial market}},
Journal = {{EUROPEAN JOURNAL OF OPERATIONAL RESEARCH}},
Year = {{2005}},
Volume = {{166}},
Number = {{1}},
Pages = {{255-265}},
Month = {{OCT 1}},
Note = {{15th Annual Meeting of the European Chapter on Combinatorial
   Optimization, Univ Lugano, Lugano, SWITZERLAND, MAY 30-JUN 01, 2002}},
Abstract = {{An optimal advertising media selection is a strategic factor for the
   operations of both traditional and e-business environments. This paper
   presents a case study that considers two options: industrial and
   consumer products. In order to resolve the strategic decision-making
   about dual market high technology products, a mixed integer goal
   programming model is developed to facilitate the advertising media
   selection process.
   Preemptive priority ordering is established by the analytic hierarchy
   process. The problem is solved lexicographically using a software
   package, based on the data obtained from the e-business industry in
   Korea. The satisfying solution is identified and analyzed, and
   sensitivity analyses are performed for model flexibility. (c) 2004
   Elsevier B.V. All rights reserved.}},
