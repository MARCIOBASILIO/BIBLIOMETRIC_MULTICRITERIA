@article{ ISI:000228513800008,
Author = {Olcer, AI and Odabasi, AY},
Title = {{A new fuzzy multiple attributive group decision making methodology and
   its application to propulsion/manoeuvring system selection problem}},
Journal = {{EUROPEAN JOURNAL OF OPERATIONAL RESEARCH}},
Year = {{2005}},
Volume = {{166}},
Number = {{1}},
Pages = {{93-114}},
