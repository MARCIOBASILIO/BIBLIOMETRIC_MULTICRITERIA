@inproceedings{ ISI:000240904600031,
Author = {Lamontagne, Luc and Abi-Zeid, Irene},
Editor = {{RothBerghofer, TR and Goker, MH and Guvenir, HA}},
Title = {{Combining multiple similarity metrics using a multicriteria approach}},
Booktitle = {{ADVANCES IN CASE-BASED REASONING, PROCEEDINGS}},
