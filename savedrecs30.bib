@article{ ISI:000229008600003,
Author = {Mau-Crimmins, T and de Steiguer, JE and Dennis, D},
Title = {{AHP as a means for improving public participation: a pre-post experiment
   with university students}},
Journal = {{FOREST POLICY AND ECONOMICS}},
