@inproceedings{ ISI:000249887900055,
Author = {Bi Wen-jie and Chen Xiao-hong},
Editor = {{Lan, H}},
Title = {{An extended dominance-based rough set approach to group sorting decision
   making}},
Booktitle = {{PROCEEDINGS OF 2007 INTERNATIONAL CONFERENCE ON MANAGEMENT SCIENCE \&
   ENGINEERING (14TH) VOLS 1-3}},
