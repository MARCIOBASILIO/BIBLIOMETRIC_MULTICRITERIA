@inproceedings{ ISI:000242507000186,
Author = {Zhang, Yong and Zhang, ShenSheng and Han, SongQiao},
Editor = {{Chen, Y and Abraham, A}},
Title = {{A proactive service mechanism based on linguistic variable and multiple
   attribute decision making}},
