@article{ ISI:000238301800001,
Author = {Kao, Hsing-Pei and Wang, Brian and Dong, James and Ku, Kuo-Cheng},
Title = {{An event-driven approach with makespan/cost tradeoff analysis for
   project portfolio scheduling}},
