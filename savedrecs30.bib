@article{ ISI:000242829000010,
Author = {Sun, L. and Greenberg, B. S.},
Title = {{Multicriteria group decision making: Optimal priority synthesis from
   pairwise comparisons}},
Journal = {{JOURNAL OF OPTIMIZATION THEORY AND APPLICATIONS}},
Year = {{2006}},
Volume = {{130}},
Number = {{2}},
Pages = {{317-338}},
Month = {{AUG}},
Abstract = {{We propose an optimization based hybrid approach for multicriteria group
   decision making that takes advantage of aggregating individual judgments
   and aggregating individual priority methods. Different error structures
   and criteria with particular emphasis on weighted least square measure
   and weighted least logarithmic square measure for additive and
   multiplicative error structures are studied. We evaluate also the
   statistical properties of the derived group preference and new measures
   to assess consistency. The proposed methods provide an iterative and
   flexible framework for guiding group decision making.}},
