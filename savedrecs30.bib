@article{ ISI:000255334800001,
Author = {Tikniouine, Abdessadek and Elfazziki, Abdelaziz},
Title = {{A hybrid approach of the MCDA: Application to the project of the
   localization of a site for the implantation of a dam in the western
   region of Morocco}},
Journal = {{INFORMATION-AN INTERNATIONAL INTERDISCIPLINARY JOURNAL}},
Year = {{2007}},
Volume = {{10}},
Number = {{2}},
Pages = {{151-160}},
Month = {{MAR}},
Abstract = {{The localization of a site for the implantation project of a dam
   constitutes a problem of a multicriterion choice. These criteria are not
   only financial but also linked to the regional development, to the
   environment, to the diversification of the cultures and to the
   socioeconomic impacts. However, these criteria are often qualitative or
   linguistic and that makes their use difficult for the analysis and
   decision-making. In this article, and in order to take into account the
   qualitative criteria, we propose a hybrid approach for their modelling
   to facilitate their insertion in the multicriterion decision aid (MCDA)
   models. This approach is based on the fuzzy set theory that permits to
   represent the linguistic values through the precise ones rather than by
   the classic intervals. Then, in the multicriterion analysis part, we
   have used the Analytic Hierarchy Process (AHP) that permits to determine
   the weights of importance of the different sites of the localization a
   site for the implantation considered by the decision makers, while
   taking in account the quantitative and qualitative criteria of the
   decision.}},
