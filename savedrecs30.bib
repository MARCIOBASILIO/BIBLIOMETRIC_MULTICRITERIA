@inproceedings{ ISI:000247093600078,
Author = {Kang, Seunglim and Lee, Seongkwan Mark},
Editor = {{Kim, SS and Park, JH and Pissinou, N and Kim, TH and Fang, WC and Slezak, D and Arabnia, HR and Howard, D}},
