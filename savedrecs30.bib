@article{ ISI:000238752000004,
Author = {Neaupane, K. M. and Piantanakulchai, M.},
Title = {{Analytic network process model for landslide hazard zonation}},
Journal = {{ENGINEERING GEOLOGY}},
Year = {{2006}},
Volume = {{85}},
