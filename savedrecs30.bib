@inproceedings{ ISI:000246477100017,
Author = {Zaras, Kazimierz and Thibault, Jules},
Editor = {{Yao, JT and Lingras, P and Wu, WZ and Szczuka, M and Cercone, NJ and Slezak, D}},
Title = {{Ranking by rough approximation of preferences for decision engineering
