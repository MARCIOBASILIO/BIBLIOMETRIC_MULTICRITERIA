@article{ ISI:000226733200024,
Author = {Chen, WH and Tsai, MS and Kuo, HL},
Title = {{Distribution system restoration using the hybrid fuzzy-grey method}},
Journal = {{IEEE TRANSACTIONS ON POWER SYSTEMS}},
Year = {{2005}},
Volume = {{20}},
Number = {{1}},
Pages = {{199-205}},
Month = {{FEB}},
Abstract = {{This paper proposes an effective and fast approach based on the hybrid
   fuzzy-grey method to evaluate restoration plans for power distribution
   systems. The proposed method consists of two stages: the fuzzy
   multicriteria evaluation and the grey relational analysis. The fuzzy
   multicriteria were adopted to deal with imprecise linguistic
   descriptions in the operators' heuristic rules, while the grey
   relational analysis was used to judge the ranking of preference for each
   feasible restoration plan. To demonstrate the effectiveness of the
   proposed method, a typical distribution system of Taiwan Power Company
   was examined. The test results showed that the proposed method was
   efficient and expeditious in obtaining a preferable restoration plan by
   using simple numerical operations.}},
