Author = {Xing Liqing and Sun Shaorong},
Editor = {{Li, JL and Zhu, DH and Porter, A and Wu, DD}},
Title = {{The application of fuzzy theory in MCDM-a multilevel evaluation model of
   TOPSIS based on entropy weight and fuzzy set}},
Booktitle = {{TIRMDCM 2007: PROCEEDINGS OF THE FIRST INTERNATIONAL CONFERENCE ON
