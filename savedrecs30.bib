@inproceedings{ ISI:000246355900051,
Author = {Loken, Espen and Botterud, Audun and Holen, Arne T.},
Book-Group-Author = {{IEEE}},
Title = {{Decision analysis and uncertainties in planning local energy systems}},
Booktitle = {{2006 INTERNATIONAL CONFERENCE ON PROBABILISTIC METHODS APPLIED TO POWER
   SYSTEMS, VOLS 1 AND 2}},
Year = {{2006}},
Pages = {{340-347}},
Note = {{9th International Conference on Probabilistic Methods Applied to Power
   Systems, KTH, Stockholm, SWEDEN, JUN 11-15, 2006}},
Abstract = {{In this paper, we discuss how uncertainties can be represented in two of
   the most well-known multi-criteria decision analysis (MCDA) methods, the
   multi-attribute utility theory (MAUT) and the analytical hierarchy
   process (AHP). There are distinct differences in the treatment of
   uncertainties in the two methods. MAUT is specially developed for
   handling uncertainties. In contrast, the AHP method has, in its general
   form, no systematic approach to integrate uncertainty aspects. However,
   various procedures for applying AHP in situations involving
   uncertainties have been proposed in the literature. In a case study, we
   illustrate how the two methods can be used to assist in decision making
   under uncertainty in a local energy planning problem. We conclude that
   MAUT is clearly more suited for handling uncertainties than AHP.
   However, also many other criteria should be considered when choosing
   which MCDA method to apply for local energy planning purposes.}},
