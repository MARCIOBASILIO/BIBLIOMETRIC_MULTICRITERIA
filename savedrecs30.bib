Author = {Zhang, Ling and Zhou, Dequn and Zhu, Peifeng and Li, Hongwei},
Book-Group-Author = {{IEEE}},
Title = {{Comparison analysis of MAUT expressed in terms of Choquet integral and
   utility axioms}},
Booktitle = {{ISSCAA 2006: 1ST INTERNATIONAL SYMPOSIUM ON SYSTEMS AND CONTROL IN
   AEROSPACE AND ASTRONAUTICS, VOLS 1AND 2}},
