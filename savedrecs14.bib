@article{ ISI:000417551800036,
Author = {Zahedi, Sina},
Title = {{Modification of expected conflicts between Drinking Water Quality Index
   and Irrigation Water Quality Index in water quality ranking of shared
   extraction wells using Multi Criteria Decision Making. techniques}},
Journal = {{ECOLOGICAL INDICATORS}},
