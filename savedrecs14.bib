@article{ ISI:000418970400008,
Author = {Argyriou, Athanasios V. and Teeuw, Richard M. and Soupios, Pantelis and
   Sarris, Apostolos},
Title = {{Neotectonic control on drainage systems: GIS-based geomorphometric and
   morphotectonic assessment for Crete, Greece}},
Journal = {{JOURNAL OF STRUCTURAL GEOLOGY}},
Year = {{2017}},
Volume = {{104}},
Pages = {{93-111}},
Month = {{NOV}},
Abstract = {{Geomorphic indices can be used to examine the geomorphological and
   tectonic processes responsible for the development of the drainage
   basins. Such indices can be dependent on tectonics, erosional processes
   and other factors that control the morphology of the landforms. The
   inter-relationships between geomorphic indices can determine the
   influence of regional tectonic activity in the shape development of
   drainage basins. A Multi Criteria Decision Analysis (MCDA) procedure has
   been used to perform an integrated cluster analysis that highlights
   information associated with the dominant regional tectonic activity.
   Factor Analysis (FA) and Analytical Hierarchy Process (AHP) were
   considered within that procedure, producing a representation of the
   distributed regional tectonic activity of the drainage basins studied.
   The study area is western Crete, located in the outer fore-arc of the
   Hellenic subduction zone, one of the world's most tectonically active
   regions. The results indicate that in the landscape evolution of the
   study area (especially the western basins) tectonic controls dominate
   over lithological controls.}},
