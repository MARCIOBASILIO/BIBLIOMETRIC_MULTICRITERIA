@article{ ISI:000409457000002,
Author = {Nunes, Luiz Henrique and Estrella, Julio Cezar and Perera, Charith and
   Reiff-Marganiec, Stephan and Botazzo Delbem, Alexandre Claudio},
Title = {{Multi-criteria IoT resource discovery: a comparative analysis}},
Journal = {{SOFTWARE-PRACTICE \& EXPERIENCE}},
