@article{ ISI:000407655400098,
Author = {Li, Yuan and Lin, Changqing and Wang, Yuebin and Gao, Xiaoping and Xie,
   Tao and Hai, Reti and Wang, Xiaohui and Zhang, Xiaowei},
Title = {{Multi-criteria evaluation method for site selection of industrial
   wastewater discharge in coastal regions}},
