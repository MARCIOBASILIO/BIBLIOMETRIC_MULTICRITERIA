@article{ ISI:000416805700033,
Author = {Stevic, Zeljko and Pamucar, Dragan and Vasiljevic, Marko and Stojic,
   Gordan and Korica, Sanja},
Title = {{Novel Integrated Multi-Criteria Model for Supplier Selection: Case Study
   Construction Company}},
