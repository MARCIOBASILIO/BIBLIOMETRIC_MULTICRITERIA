@article{ ISI:000408789300030,
Author = {Yazdani, Morteza and Zarate, Pascale and Coulibaly, Adama and Zavadskas,
   Edmundas Kazimieras},
Title = {{A group decision making support system in logistics and supply chain
   management}},
