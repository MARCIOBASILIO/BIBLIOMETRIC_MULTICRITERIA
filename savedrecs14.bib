@article{ ISI:000416216300004,
Author = {Bolukbas, Ufuk and Guneri, Ali Fuat},
Title = {{A FUZZY MULTI-CRITERIA DECISION APPROACH FOR MEASURING TECHNOLOGY
   COMPETENCY PERFORMANCE OF SMEs}},
