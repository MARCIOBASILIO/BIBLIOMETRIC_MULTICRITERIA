@article{ ISI:000406915100009,
Author = {Wang, Jing and Wang, Jian-Qiang and Zhang, Hong-Yu and Chen, Xiao-Hong},
Title = {{Distance-Based Multi-Criteria Group Decision-Making Approaches with
   Multi-Hesitant Fuzzy Linguistic Information}},
