@article{ ISI:000406709500179,
Author = {Lombardi, Patrizia and Abastante, Francesca and Torabi Moghadam, Sara
   and Toniolo, Jacopo},
Title = {{Multicriteria Spatial Decision Support Systems for Future Urban Energy
   Retrofitting Scenarios}},
Journal = {{SUSTAINABILITY}},
