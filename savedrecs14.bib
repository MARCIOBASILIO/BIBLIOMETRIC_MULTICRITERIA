Author = {Voronova, Irina and Shatrevich, Vladimirs and Freimane, Gita},
Editor = {{Soliman, KS}},
Title = {{The Impact of Digital Transformation on Development of Latvian Insurance
   Companies' Digitalization Strategies and Shift of Perception Values}},
Booktitle = {{VISION 2020: SUSTAINABLE ECONOMIC DEVELOPMENT AND APPLICATION OF
   INNOVATION MANAGEMENT}},
