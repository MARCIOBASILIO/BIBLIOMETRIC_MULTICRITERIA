@inproceedings{ ISI:000518205900020,
Author = {Ighravwe, D. E. and Babatunde, O. M. and Adedoja, O. S. and Okharedia,
   T. E.},
Book-Group-Author = {{IEEE}},
Title = {{Evaluation and Selection of Hybrid Renewable Energy Systems for
   Healthcare Centres In Rural Areas: A Techno-economic Approach}},
Booktitle = {{2018 IEEE 7TH INTERNATIONAL CONFERENCE ON ADAPTIVE SCIENCE \& TECHNOLOGY
   (IEEE ICAST)}},
Series = {{IEEE International Conference on Adaptive Science and Technology}},
Year = {{2018}},
Note = {{7th IEEE International Conference on Adaptive Science and Technology
   (IEEE ICAST), Accra, GHANA, AUG 22-24, 2018}},
Organization = {{IEEE; IEEE Ghana Sect; Cross Check; iThenticate}},
Abstract = {{The current global development goal programme embeds the need for access
   to quality health services and reliable clean energy. Researchers and
   practitioners often pursue these requirements separately in order to
   satisfy human beings needs. This has created a knowledge gap on how to
   combine these goals in making an informed decision. Thus, this study
   identifies and analyse Hybrid Renewable Energy Systems (HRES) for
   sustainable healthcare services. Technical, economic, and environmental
   criteria were used to identify optimal HRES for health-care centres.
   Health care centres energy needs were simulated using HOMER software.
   The results obtained were analysed using WASPAS (weighted aggregated sum
   product assessment) method, respectively. The case study least and most
   important criteria were the operational cost and the total net present
   cost, respectively. The applicability of the proposed framework was
   tested using a case study of six communities in Nigeria. The best HRES
