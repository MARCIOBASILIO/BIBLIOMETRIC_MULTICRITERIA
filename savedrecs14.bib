@article{ ISI:000412607100102,
Author = {Asefi, Hossein and Lim, Samsung},
Title = {{A novel multi-dimensional modeling approach to integrated municipal
   solid waste management}},
Journal = {{JOURNAL OF CLEANER PRODUCTION}},
