@article{ ISI:000405252000106,
Author = {Xiao, Yangfan and Yi, Shanzhen and Tang, Zhongqian},
Title = {{Integrated flood hazard assessment based on spatial ordered weighted
   averaging method considering spatial heterogeneity of risk preference}},
Journal = {{SCIENCE OF THE TOTAL ENVIRONMENT}},
Year = {{2017}},
