@article{ ISI:000414756000002,
Author = {Margeta, Jure and Durin, Bojan},
Title = {{Multi-criteria approach in solar urban water supply systems}},
Journal = {{PROCEEDINGS OF THE INSTITUTION OF CIVIL ENGINEERS-WATER MANAGEMENT}},
Year = {{2017}},
Volume = {{170}},
Number = {{6}},
Pages = {{273-286}},
Month = {{DEC}},
Abstract = {{An innovative method for sizing solar photovoltaic (PV) pumping systems
   in urban water supply systems (UWSSs) is presented. The methodology is
   based on various critical period approaches (the critical period
   method). A multicriteria analysis was carried out to evaluate the
   effects of the critical period duration on the system characteristics.
   It was found that the critical period length is a design variable that
   significantly affects the economic, environmental and social aspects of
   the solution, with a lengthening of the critical period resulting in
   improvement of the overall sustainability of the solution. The results
   obtained indicate that a solar PV system is a promising solution to the
   energy sustainability of UWSSs. This form of energy supply does not emit
   greenhouse gases and furthermore the `social cost' of carbon dioxide
   estimates decrease. The concept of a stand-alone PV energy system is a
   particularly promising sustainable energy solution.}},
