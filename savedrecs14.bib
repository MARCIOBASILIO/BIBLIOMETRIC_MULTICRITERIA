@article{ ISI:000413041000005,
Author = {Azadfallah, Mohammad},
Title = {{Supplier Selection by Extended TOPSIS to Obtain the Ideal Compromise
   Solution in Group Decision Making}},
Journal = {{INTERNATIONAL JOURNAL OF BUSINESS ANALYTICS}},
Year = {{2017}},
Volume = {{4}},
Number = {{3}},
Pages = {{71-85}},
Month = {{JUL-SEP}},
Abstract = {{In the current literature, there are several studies, which the supplier
   selection is typically a Multi Criteria Group Decision Making problem.
   Several solutions for the above problem are proposed (from simple
   approaches; like, Borda, Condorcet, etc., to complex ones; like,
   Multiple Criteria Decision Making model combined with intuitionistic
   fuzzy set, etc.). To solve this problem, different method (particularly,
   extended TOPSIS method) are proposed in this paper. Firstly, we have
   used TOPSIS to find the individual preference ordering, then, we have
   used the extended version of this method to find the collective
