Author = {Tuysuz, Fatih and Simsek, Berna},
Title = {{A hesitant fuzzy linguistic term sets-based AHP approach for analyzing
   the performance evaluation factors: an application to cargo sector}},
Journal = {{COMPLEX \& INTELLIGENT SYSTEMS}},
Year = {{2017}},
Volume = {{3}},
Number = {{3, SI}},
Pages = {{167-175}},
Month = {{OCT}},
Abstract = {{One of the important points in performance measurement and evaluation is
   determining the relative importance of the metrics used. The aim of this
   study is to present a fuzzy multi-criteria decision-making approach for
   evaluating the factors used for performance evaluation. Hesitant fuzzy
   sets-based analytic hierarchy process method, which is an effective and
   successful approach to deal with uncertainty, is proposed for analyzing
   the factors affecting the performance of the branches of a cargo company
   in Turkey and also for prioritizing them according to their level of
   importance.}},
