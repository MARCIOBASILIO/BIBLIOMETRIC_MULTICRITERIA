@article{ ISI:000407518300024,
Author = {Guo, Zhi-Lian and Liu, Yan-Ling and Yang, Hai-Long},
Title = {{A Novel Rough Set Model in Generalized Single Valued Neutrosophic
   Approximation Spaces and Its Application}},
Journal = {{SYMMETRY-BASEL}},
Year = {{2017}},
Volume = {{9}},
Number = {{7}},
Month = {{JUL}},
Abstract = {{In this paper, we extend the rough set model on two different universes
   in intuitionistic fuzzy approximation spaces to a single-valued
   neutrosophic environment. Firstly, based on the (alpha, beta, gamma)-cut
   relation (R) over tilde\{(alpha, beta, gamma)\}, we propose a rough set
   model in generalized single-valued neutrosophic approximation spaces.
   Then, some properties of the new rough set model are discussed.
   Furthermore, we obtain two extended models of the new rough set
   model-the degree rough set model and the variable precision rough set
   model-and study some of their properties. Finally, we explore an example
   to illustrate the validity of the new rough set model.}},
