@article{ ISI:000405417600039,
Author = {Paidar, Maryam and Chaharborj, Sarkhosh Seddighi and Harounabadi, Ali},
Title = {{Identifying Top-k Most Influential Nodes by using the Topological
   Diffusion Models in the Complex Networks}},
Journal = {{INTERNATIONAL JOURNAL OF ADVANCED COMPUTER SCIENCE AND APPLICATIONS}},
Year = {{2017}},
Volume = {{8}},
