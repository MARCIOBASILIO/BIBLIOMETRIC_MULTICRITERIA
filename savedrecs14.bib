@article{ ISI:000402364000004,
Author = {Sahin, Bekir},
Title = {{Consistency control and expert consistency prioritization for FFTA by
   using extent analysis method of trapezoidal FAHP}},
Journal = {{APPLIED SOFT COMPUTING}},
