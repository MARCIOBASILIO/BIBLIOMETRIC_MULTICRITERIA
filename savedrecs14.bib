@article{ ISI:000414788400022,
Author = {Garg, Harish},
Title = {{Some Picture Fuzzy Aggregation Operators and Their Applications to
   Multicriteria Decision-Making}},
Journal = {{ARABIAN JOURNAL FOR SCIENCE AND ENGINEERING}},
Year = {{2017}},
Volume = {{42}},
Number = {{12}},
Pages = {{5275-5290}},
Month = {{DEC}},
Abstract = {{The objective of the work is to present some series of the aggregation
   operators for the picture fuzzy sets (PFSs). As PFSs have been an
   extended version of the intuitionistic fuzzy set theory which not only
   considers the degree of acceptance or rejection but also taken into the
   account the degree of refusal during the analysis. Thus, by considering
   all these degrees, some aggregation operators, namely picture fuzzy
   weighted average, picture fuzzy ordered weighted average, and picture
   fuzzy hybrid average aggregation operators, have been proposed along
   with their desirable properties. A decision-making approach based on
   these operators has also been presented. Finally, an illustrative
   example has been given for demonstrating the approach.}},
