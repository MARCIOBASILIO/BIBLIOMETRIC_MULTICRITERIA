@inproceedings{ ISI:000399455300050,
Author = {Bakhouyi, Abdellah and Dehbi, Rachid and Talea, Mohamed},
Book-Group-Author = {{IEEE}},
Title = {{Multiple Criteria Comparative Evaluation on the Interoperability of LMS
   by Applying COPRAS Method}},
Booktitle = {{PROCEEDINGS OF 2016 FUTURE TECHNOLOGIES CONFERENCE (FTC)}},
Year = {{2016}},
Pages = {{361-366}},
Note = {{Future Technologies Conference (FTC), San Francisco, CA, DEC 06-07, 2016}},
Organization = {{HPCC Syst; IEEE; IEEE Power \& Energy Soc; IBM Res; IBM Watson AI
   XPRIZE; Brown Univ; IEEE San Francisco PES Chapter; Univ Michigan}},
Abstract = {{Today Education through ICT is one of the approaches that are currently
   regarded as an essential means of learning, The Learning Management
   Systems LMS provide an exchange of information and sharing of services
   between them to support e-learning such a way that these systems are
   subjected to a process of standardization and interoperability. This
   article speculates on a multi-criteria analysis of LMS interoperability,
   the most relevant in use today. We first define the interoperability
