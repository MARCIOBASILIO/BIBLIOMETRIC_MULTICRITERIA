@article{ ISI:000389342100002,
Author = {Guelfi, Silvano and Norese, Maria Franca and Saluto, Paolo},
Title = {{An integration of theoretical knowledge, day by day experience and
   multicriteria methods to support the entrepreneurial learning process}},
Journal = {{MEASURING BUSINESS EXCELLENCE}},
Year = {{2016}},
