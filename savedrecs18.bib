@inproceedings{ ISI:000406677200041,
Author = {Tamosiunas, Andrius},
Editor = {{Stankeviciene, J and Lankauskiene, T}},
Title = {{MANAGING STAKEHOLDERS IN COMPLEX INVESTMENTS PROJECTS}},
Booktitle = {{9TH INTERNATIONAL SCIENTIFIC CONFERENCE - BUSINESS AND MANAGEMENT 2016}},
Series = {{Business and Management-Spausdinta}},
Year = {{2016}},
Note = {{9th International Scientific Conference on Business and Management,
   Vilnius Gediminas Tech Univ, Fac Business Management, Vilnius,
   LITHUANIA, FEB 06-JUL 06, 2016}},
Abstract = {{The article presents the stakeholder management model for complex
   investment projects. The proposed model is aimed at strengthening in
   essence the justification of managerial decisions subject to the choice
   of the stakeholders and their subsequent coaction throughout the whole
   investment project management cycle. Respectively the model concerned is
   regarded as multiple criteria ranking task of possible variants of
   choice of stakeholders (as a one of indispensable factors) needed when
   seeking for effective implementation of complex investments. In order to
   solve this task the cooperation-target-homogeneity function and
   three-stage-criteria system is adapted. The latter measures are also
   supported by the quantitative methods to be used when assessing
   potential stakeholders and forming basis for objective decision making.}},
