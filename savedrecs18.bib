@article{ ISI:000404969200004,
Author = {Rocco, Claudio M. and Hernandez-Perdomo, Elvis and Barker, Kash},
Title = {{Multicriteria Decision Analysis Approach for Stochastic Ranking with
   Application to Network Resilience}},
Journal = {{ASCE-ASME JOURNAL OF RISK AND UNCERTAINTY IN ENGINEERING SYSTEMS PART
   A-CIVIL ENGINEERING}},
