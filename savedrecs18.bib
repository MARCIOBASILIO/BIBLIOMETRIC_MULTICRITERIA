Title = {{The TOPSIS Method in the Interval Type-2 Fuzzy Setting}},
Booktitle = {{PARALLEL PROCESSING AND APPLIED MATHEMATICS, PPAM 2015, PT II}},
Series = {{Lecture Notes in Computer Science}},
Year = {{2016}},
Volume = {{9574}},
