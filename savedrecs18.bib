Title = {{A Correlation based Intuitionistic Fuzzy TOPSIS Method on Supplier
   Selection Problem}},
Booktitle = {{2016 IEEE INTERNATIONAL CONFERENCE ON FUZZY SYSTEMS (FUZZ-IEEE)}},
Series = {{IEEE International Fuzzy Systems Conference Proceedings}},
Year = {{2016}},
Pages = {{2106-2112}},
