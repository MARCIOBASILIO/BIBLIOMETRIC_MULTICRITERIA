@article{ ISI:000388593300011,
Author = {Sakthivel, Gnanasekaran and Sivakumar, R. and Ilangkumaran, Mani and
   Ikua, Bernard W.},
Title = {{Selection of optimum fish oil fuel blend to reduce the greenhouse gas
   emissions in an IC engine-A hybrid multiple criteria decision aid
   approach}},
