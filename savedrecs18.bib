@article{ ISI:000379866600001,
Author = {Yodmun, Satit and Witayakiattilerd, Wichai},
Title = {{Stock Selection into Portfolio by Fuzzy Quantitative Analysis and Fuzzy
   Multicriteria Decision Making}},
Journal = {{ADVANCES IN OPERATIONS RESEARCH}},
Year = {{2016}},
Volume = {{2016}},
Abstract = {{This paper presents a stock selection approach assisted by fuzzy
   procedures. In this approach, stocks are classified into groups
   according to business types. Within each group, the stocks are screened
   and then ranked according to their investment weight obtained from fuzzy
   quantitative analysis. Groups were also ranked according to their group
   weight obtained from fuzzy analytic hierarchy process (FAHP) and
   technique for order preference by similarity to ideal solution method
   (TOPSIS). The overall weight for each stock was then derived from both
   of these weights and used for selecting a stock into the portfolio. As a
   demonstration, our analysis procedures were applied to a test set of
   data.}},
