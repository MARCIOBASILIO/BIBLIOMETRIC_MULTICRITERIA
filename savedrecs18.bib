@inproceedings{ ISI:000390786400073,
Author = {Devade, K. D. and Pise, A. T.},
Editor = {{Kyriakopoulos, GL}},
Title = {{Optimization of thermal performance of Ranque Hilsch Vortex Tube: MADM
   techniques}},
Booktitle = {{2016 INTERNATIONAL CONFERENCE ON NEW ENERGY AND FUTURE ENERGY SYSTEM
   (NEFES 2016)}},
Series = {{IOP Conference Series-Earth and Environmental Science}},
Year = {{2016}},
Volume = {{40}},
Note = {{International Conference on New Energy and Future Energy System (NEFES),
   Beijing, PEOPLES R CHINA, AUG 19-22, 2016}},
Abstract = {{Thermal performance of vortex tube is noticeably influenced by its
   geometrical and operational parameters. In this study effect of various
   geometrical (L/D ratio: 15, 16, 17, 18; exit valve angle; 30 degrees, 45
   degrees, 60 degrees, 75 degrees, 90 degrees; cold end orifice diameter:
   5, 6 and 7mm, tube divergence angle: 0 degrees, 2 degrees, 3 degrees, 4
   degrees) and operational parameters (inlet pressure: 2 to 6 bars) on the
   performance of vortex tube have been investigated experimentally.
   Multiple Attribute Decision Making (MADM) techniques are applied to
   determine the optimum combination of the vortex tube. Performance of
   vortex tube was analysed with optimum temperature difference on cold
   end, COP for cooling. The MADM (Multiple Attribute Decision Making)
   methods, namely WSM (Weighted Sum Method), WPM (Weighted Power Method),
   TOPSIS (Technique for Order Preference by Similarity to Ideal Solution)
   and AHP (Analytical Hierarchy Process) are applied. Experimental best
   performing combinations are obtained for Length to Diameter ratios 15,
   16, 17 with exit valve angle as 45 degrees,75 degrees and 90 degrees at
   orifice diameter 5mm for inlet pressure of 5 and 6 bar pressure. Best
   COP, efficiency and cold end temperature difference are 0.245, 40.6\%
   and 38.3K respectively for the combination of 15 L/D, 45 degrees valve
   angle, 5mm orifice diameter and 2 bar pressure by MADM techniques.}},
