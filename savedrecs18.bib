@article{ ISI:000391161100005,
Author = {Keshavarz Ghorabaee, Mehdi and Zavadskas, Edmundas Kazimieras and Amiri,
   Maghsoud and Antucheviciene, Jurgita},
Title = {{EVALUATION BY AN AREA-BASED METHOD OF RANKING INTERVAL TYPE-2 FUZZY SETS
   (EAMRIT-2F) FOR MULTI-CRITERIA GROUP DECISION-MAKING}},
Journal = {{TRANSFORMATIONS IN BUSINESS \& ECONOMICS}},
Year = {{2016}},
Volume = {{15}},
Number = {{3}},
Pages = {{76-95}},
Abstract = {{A type-2 fuzzy set, which is characterised by a fuzzy membership
   function, can provide us with more degrees of freedom to represent the
   uncertainty and vagueness of the real-world problems. Interval type-2
   fuzzy sets are the most widely used type-2 fuzzy sets and have
   significant applications in modelling of the multi-criteria decision
   making problems. In this paper, we present a new method of Evaluation by
   an Area-based Method of Ranking Interval Type-2 Fuzzy sets (EAMRIT-2F)
   for multi-criteria group decision-making. First, we propose a new method
   for ranking interval type-2 fuzzy sets based on the area under the upper
   and lower membership functions and the possibility degree of fuzzy sets.
   The ranking values of the proposed method are also compared with some
   existing methods. Then, the EAMRIT-2F method is presented based on the
   proposed ranking method of interval type-2 fuzzy sets for handling the
   fuzzy multi-criteria group decision-making problems. Applying the
   proposed method to an example concerning the selection of a facility
   location, we illustrate the process of the proposed method in the fuzzy
   multi-criteria group decision-making. The result of the EAMRIT-2F method
   is compared with five other methods. The comparison shows the
   consistency between the proposed method and other methods.}},
