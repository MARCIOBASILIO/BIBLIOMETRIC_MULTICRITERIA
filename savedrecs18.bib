Author = {Keshavarz Ghorabaee, Mehdi and Zavadskas, Edmundas Kazimieras and Amiri,
   Maghsoud and Antucheviciene, Jurgita},
Title = {{A NEW METHOD OF ASSESSMENT BASED ON FUZZY RANKING AND AGGREGATED WEIGHTS
   (AFRAW) FOR MCDM PROBLEMS UNDER TYPE-2 FUZZY ENVIRONMENT}},
Journal = {{ECONOMIC COMPUTATION AND ECONOMIC CYBERNETICS STUDIES AND RESEARCH}},
Year = {{2016}},
Volume = {{50}},
