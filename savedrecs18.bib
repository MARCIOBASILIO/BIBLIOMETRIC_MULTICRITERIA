@inproceedings{ ISI:000382042200108,
Author = {Obayiuwana, Enoruwa and Falowo, Olabisi},
Book-Group-Author = {{IEEE}},
Title = {{A New Network Selection Algorithm for Group Calls over Heterogeneous
   Wireless Networks with Dynamic Multi-Criteria}},
Booktitle = {{2016 13TH IEEE ANNUAL CONSUMER COMMUNICATIONS \& NETWORKING CONFERENCE
   (CCNC)}},
Year = {{2016}},
Note = {{13th IEEE Annual Consumer Communications and Networking Conference
   (CCNC), Las Vegas, NV, JAN 06-13, 2016}},
Organization = {{IEEE}},
Abstract = {{Communicating multimode mobile nodes (MNs) in HWNs are capable of
   supporting different classes of calls simultaneously. This is also
   referred to as group calls. Vertical handover (VHO) decision problem for
   group calls in HWNs is a complex multi-criteria group decision making
   (MCGDM) problem, very little attempt has been made to address this
   problem. Existing works in literature on network selection problem for
   group calls treat the multi-criteria for VHO as static criteria, however
   in NGWNs, criteria such as: MN's speed and network traffic load
   variation are dynamic and vital for wireless network selection for group
   calls. In this paper, we propose MULTIMOORA, a new decision making
   algorithm, for VHO decision and address the problem of network selection
   for group calls in HWNs. We investigate the impact of dynamic criterion
   and the degree of importance of class of call in group calls. The
   performance of the MULTIMOORA is validated by comparing it with TOPSIS,
   a well known MCGDM algorithm, however the performances of TOPSIS are
   seen to be unstable at high speed region, unlike MULTIMOORA.}},
