@inproceedings{ ISI:000382762000015,
Author = {Premarathne, Uthpala. S.},
Book-Group-Author = {{IEEE}},
Title = {{PROAASEL: Prospect Theory based Continuous Authentication Attribute
   Selection Model}},
Booktitle = {{2ND INTERNATIONAL MERCON 2016 MORATUWA ENGINEERING RESEARCH CONFERENCE}},
