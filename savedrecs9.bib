@article{ ISI:000504500600001,
Author = {Kraujaliene, Lidija},
Title = {{COMPARATIVE ANALYSIS OF MULTICRITERIA DECISION-MAKING METHODS EVALUATING
   THE EFFICIENCY OF TECHNOLOGY TRANSFER}},
