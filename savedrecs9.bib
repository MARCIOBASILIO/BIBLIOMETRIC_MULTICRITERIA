Editor = {{Nathanail, EG and Karakikes, ID}},
Title = {{Development of a Methodology, Using Multi-Criteria Decision Analysis
   (MCDA), to Choose Between Full Pedestrianization and Traffic Calming
   Area (Woonerf Zone Type)}},
Booktitle = {{DATA ANALYTICS: PAVING THE WAY TO SUSTAINABLE URBAN MOBILITY}},
