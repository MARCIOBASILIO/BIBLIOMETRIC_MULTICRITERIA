@inproceedings{ ISI:000589182700074,
Author = {Kiousis, Vasileios and Nathanail, Eftihia and Karakikes, Ioannis},
Editor = {{Nathanail, EG and Karakikes, ID}},
Title = {{Assessing Traffic and Environmental Impacts of Smart Lockers Logistics
   Measure in a Medium-Sized Municipality of Athens}},
Booktitle = {{DATA ANALYTICS: PAVING THE WAY TO SUSTAINABLE URBAN MOBILITY}},
