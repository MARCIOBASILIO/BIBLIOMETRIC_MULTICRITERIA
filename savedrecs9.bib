@inproceedings{ ISI:000591565600010,
Author = {Maciol, Andrzej and Rebiasz, Bogdan},
Editor = {{Arai, K and Kapoor, S and Bhatia, R}},
Title = {{Classical, Rule-Based and Fuzzy Methods in Multi-Criteria Decision
   Analysis (MCDA) for Life Cycle Assessment}},
Booktitle = {{INTELLIGENT COMPUTING, VOL 1}},
