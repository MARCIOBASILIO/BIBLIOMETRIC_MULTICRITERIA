Author = {Farahbakhsh, Amin and Forghani, Mohammad Ali},
Title = {{Sustainable location and route planning with GIS for waste sorting
   centers, case study: Kerman, Iran}},
Journal = {{WASTE MANAGEMENT \& RESEARCH}},
