@article{ ISI:000505154500015,
Author = {Liu, Mengling and Zhang, Chonghui and Fan, Yuanhang and Jin, Huanhuan},
Title = {{CLOUD-MODEL-BASED MULTIMOORA COMPREHENSIVE EVALUATION METHOD AND ITS
   APPLICATION IN STATISTICS DISCIPLINE ASSESSMENT}},
Journal = {{TRANSFORMATIONS IN BUSINESS \& ECONOMICS}},
Year = {{2019}},
Volume = {{18}},
Number = {{3}},
Pages = {{229-245}},
Abstract = {{The conventional MULTIMOORA method is not widely used as it has
   drawbacks in the characterization of the uncertainty of evaluation
   information and the importance of decision indicators. In this paper, a
   MULTIMOORA method based on the cloud model (C-MULTIMOORA) is proposed.
   This method transforms uncertain evaluation information quantitatively
   using the cloud model. Subsequently, the Square Root of the
   Jensen-Shannon Divergence (SRJSD) distance is employed to determine the
   importance weights of the experts and construct the weighted evaluation
   cloud. Moreover, the C-MULTIMOORA approach is used to rank different
   solutions and select the most appropriate one. Finally, a case study of
   subsidized assessment of Statistics discipline is presented to
   demonstrate the feasibility and validity of the proposed approach.}},
