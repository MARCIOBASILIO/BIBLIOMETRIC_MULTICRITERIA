Title = {{A Novel Hybrid Multi-criteria Decision-Making Model to Solve UA-FLP}},
Booktitle = {{DISTRIBUTED COMPUTING AND ARTIFICIAL INTELLIGENCE}},
Series = {{Advances in Intelligent Systems and Computing}},
Year = {{2019}},
Volume = {{800}},
Pages = {{292-299}},
