@article{ ISI:000463832400001,
Author = {Motieyan, Hamid and Mesgari, Mohammad Saadi},
Title = {{A novel spatial index using spatial analyses and hierarchical fuzzy
   expert system for obtaining green TOD: a case study in Tehran city}},
Journal = {{GEOCARTO INTERNATIONAL}},
