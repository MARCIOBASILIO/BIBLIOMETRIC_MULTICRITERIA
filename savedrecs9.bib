@article{ ISI:000460812900004,
Author = {Aydin, Serhat and Kahraman, Cengiz},
Title = {{Evaluation of firms applying to Malcolm Baldrige National Quality Award:
   a modified fuzzy AHP method}},
Journal = {{COMPLEX \& INTELLIGENT SYSTEMS}},
