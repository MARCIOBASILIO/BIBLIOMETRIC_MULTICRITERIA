@article{ ISI:000498834900098,
Author = {Naeem, Khalid and Riaz, Muhammad and Peng, Xindong and Afzal, Deeba},
Title = {{Pythagorean fuzzy soft MCGDM methods based on TOPSIS, VIKOR and
   aggregation operators}},
Journal = {{JOURNAL OF INTELLIGENT \& FUZZY SYSTEMS}},
Year = {{2019}},
Volume = {{37}},
Number = {{5}},
Pages = {{6937-6957}},
Abstract = {{We study, in this paper, some notions related to Pythagorean fuzzy soft
   sets (PFSSs) along with their algebraic structures. We present
   operations on PFSSs and their peculiar characteristics and elaborate
   them with real life examples and tabular representation to develop the
   affluence of linguistic variables based on Pythagorean fuzzy soft (PFS)
   information. We present an application of PFSSs to the multi-criteria
   group decision-making (MCGDM) related to site selection, accompanied by
   Algorithm and flow chart. We develop PFS TOPSIS method and PFS VIKOR
   method as extensions of the TOPSIS (Technique for Order of Preference by
   Similarity to Ideal Solution) and VIKOR (Vlse Kriterijumska Optimizacija
   Kompromisno Resenje) respectively. Finally, we rendered an application
   in stock exchange investment problem and tackled it by both PFS TOPSIS
   and PFS VIKOR methods.}},
