@article{ ISI:000458592200031,
Author = {Chen, Vivien Yi-Chun and Lin, Jerry Chao-Lee and Tzeng, Gwo-Hshiung},
Title = {{Assessment and improvement of wetlands environmental protection plans
   for achieving sustainable development}},
