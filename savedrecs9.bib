@article{ ISI:000464850800043,
Author = {Liu, Hu-Chen and Zhao, Hao and You, Xiao-Yue and Zhou, Wen-Yong},
Title = {{Robot Evaluation and Selection Using the Hesitant Fuzzy Linguistic
   MULTIMOORA Method}},
Journal = {{JOURNAL OF TESTING AND EVALUATION}},
