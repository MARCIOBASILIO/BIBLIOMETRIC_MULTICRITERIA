@inproceedings{ ISI:000507570400051,
Author = {Jan, Bartoska and Tereza, Jedlanova and Rydval, Jan},
Editor = {{Houda, M and Remes, R}},
Title = {{Semantic Model of Project Management in Corporate practice}},
