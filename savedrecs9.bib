@article{ ISI:000457499200012,
Author = {Chakraborty, Tilottama and Majumder, Mrinmoy},
Title = {{Application of statistical charts, multi-criteria decision making and
   polynomial neural networks in monitoring energy utilization of wave
   energy converters}},
Journal = {{ENVIRONMENT DEVELOPMENT AND SUSTAINABILITY}},
