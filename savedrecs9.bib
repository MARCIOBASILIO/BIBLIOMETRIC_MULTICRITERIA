Title = {{Modeling the Conflict Within Group Decision Making: A Comparison Between
   Methods that Require and Do Not Require the Use of Preference
   Aggregation Techniques}},
Booktitle = {{GROUP DECISION AND NEGOTIATION: BEHAVIOR, MODELS, AND SUPPORT, GDN 2019}},
Series = {{Lecture Notes in Business Information Processing}},
