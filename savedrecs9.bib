@inproceedings{ ISI:000591586200038,
Author = {Yin, Yubo and Xian, Sidong},
Book-Group-Author = {{IEEE}},
Title = {{Multi-criteria Intuitionistic Fuzzy Decision Making Method with Unknown
   Weights Based on Experts Classification}},
Booktitle = {{2019 2ND INTERNATIONAL CONFERENCE ON ARTIFICIAL INTELLIGENCE AND BIG
   DATA (ICAIBD 2019)}},
Year = {{2019}},
Pages = {{229-237}},
Note = {{2nd International Conference on Artificial Intelligence and Big Data
   (ICAIBD), Chengdu, PEOPLES R CHINA, MAY 25-28, 2019}},
Organization = {{IEEE; Sichuan Prov Comp Federat}},
Abstract = {{The multiple-criteria group decision making (MCGDM) is an important
   research topic in decision theory. The highlight of this paper is to
   develop a novel decision-making formula for intuitionistic fuzzy MCGDM
   problems with the consideration of the deviation of experts' knowledge,
   experience and preference. Firstly, the grey relational similarity
   matrix (GRSM) and the close degree matrix (CDM) are proposed to describe
   the similarity of experts' assessments from two perspectives: the
   geometric shape of data and the cosine similarity. Then, combining GRSM
   with CDM, an intuitionistic fuzzy similarity matrix (IFSM) is
   constructed for experts classification. Importantly, a performance index
   is presented for choosing the optimal clustering result. Subsequently,
   the group conflict degree and familiarity degree are constructed for
   determining the weights. Finally, an earthquake emergency decision is
   proposed to reveal the availability and practicability of the novel
   method.}},
Publisher = {{IEEE}},
Address = {{345 E 47TH ST, NEW YORK, NY 10017 USA}},
Type = {{Proceedings Paper}},
Language = {{English}},
