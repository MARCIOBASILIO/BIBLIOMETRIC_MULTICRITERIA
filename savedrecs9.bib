@article{ ISI:000503058900010,
Author = {Brauers, Willem K. M. and Lepkova, Natalija},
Title = {{IS CREDIT RATING RESERVED TERRITORY FOR CREDIT RATING AGENCIES? A
   MULTIMOORA APPROACH FOR EUROPEAN FIRMS AND COUNTRIES}},
Journal = {{TECHNOLOGICAL AND ECONOMIC DEVELOPMENT OF ECONOMY}},
Year = {{2019}},
