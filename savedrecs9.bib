@article{ ISI:000458380000004,
Author = {Ahmad, Md. Tanweer and Mondal, Sandeep},
Title = {{Dynamic supplier selection approach for mining equipment company}},
Journal = {{JOURNAL OF MODELLING IN MANAGEMENT}},
