@inproceedings{ ISI:000567002200035,
Author = {Purwita, Anggraeni Widya and Subriadi, Apol Pribadi},
Editor = {{Younus, A and Herdiyanti, A}},
Title = {{Information Technology Investment: In Search of The Closest Accurate
   Method}},
Booktitle = {{FIFTH INFORMATION SYSTEMS INTERNATIONAL CONFERENCE}},
Series = {{Procedia Computer Science}},
Year = {{2019}},
Volume = {{161}},
Pages = {{300-307}},
Note = {{5th Information Systems International Conference (ISICO) on Role of
   Information Systems in Industry 4.0, Surabaya, INDONESIA, JUL 23-24,
   2019}},
Organization = {{Inst Teknologi Sepuluh Nopember, Dept Informat Syst}},
Abstract = {{In order to improve performance, company allocates significant amount of
   expenditures for IT investment. Regardless IT productivity paradox
   phenomenon, the companies certainly expect investment that have been
   made able to gain various benefits to the company. This research aims to
   compare five method of IT investment evaluation and find the closest
   accurate method according to selected literatures. Analysis was done by
   comparing: i) tangible and intangible ii) advantage iii) disadvantage
   iv) object and v) how to use. The result showed that Return on
   Investment (ROI), and Cost Benefit Analysis (CBA) focus on tangible.
   While Multi Criteria Decision Making (MCDM) focus on intangible. The
   method must justify tangible and intangible to get accurate result.
   Information Economics (IE) and Real Options Analysis (ROA) focus on both
   tangible and intangible. From both methods, IE was the closest accurate
   method because it could calculate tangible, quasi-tangible, and
   intangible, and also calculates the value of an investment based on
   enhanced ROI, business factors, and risk criteria. (C) 2019 The Authors.
   Published by Elsevier B.V.}},
