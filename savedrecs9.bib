Editor = {{Becherif, M and Ramadan, HS and Benchouia, MT and Khaled, M and Ramadan, M and Hilairet, M}},
Title = {{Site selection methodology for the wind-powered hydrogen refueling
   station based on AHP-GIS in Adrar, Algeria}},
Booktitle = {{EMERGING AND RENEWABLE ENERGY: GENERATION AND AUTOMATION}},
Series = {{Energy Procedia}},
