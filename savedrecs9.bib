@article{ ISI:000499475200253,
Author = {Yadav, Sasmit and Verma, Rajesh Kumar},
Title = {{Multi Criteria Optimization Approaches in Electric Discharge Machining}},
Journal = {{MATERIALS TODAY-PROCEEDINGS}},
