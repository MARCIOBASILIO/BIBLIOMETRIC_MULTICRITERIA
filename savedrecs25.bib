@article{ ISI:000300648300001,
Author = {Greco, Salvatore and Kadzinski, Milosz and Mousseau, Vincent and
   Slowinski, Roman},
Title = {{Robust ordinal regression for multiple criteria group decision:
   UTA(GMS)-GROUP and UTADIS(GMS)-GROUP}},
Journal = {{DECISION SUPPORT SYSTEMS}},
Year = {{2012}},
Volume = {{52}},
Number = {{3}},
Pages = {{549-561}},
Month = {{FEB}},
Abstract = {{We introduce the principle of robust ordinal regression to multiple
   criteria group decision, and we present two new methods using a set of
   additive value functions as a preference model, called UTA(GMS)-GROUP
   and UTADIS(GMS)-GROUP. With respect to the set of decision makers (DMs),
   we consider two levels of certainty for the results. The first level is
   related to the necessary or possible consequences of indirect preference
   information provided by each DM, whereas the other refers to the subset
   of DMs agreeing for a specific outcome. In this way, we investigate
   spaces of consensus and disagreement between the DMs. The proposed
   methods are illustrated by examples showing how they can support
   real-world group decision. (C) 2011 Elsevier B.V. All rights reserved.}},
