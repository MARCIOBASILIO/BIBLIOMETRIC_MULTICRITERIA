@article{ ISI:000324039300009,
Author = {Wilkens (Nee Braune), Ines and Schmuck, Peter},
Title = {{Transdisciplinary Evaluation of Energy Scenarios for a German Village
   Using Multi-Criteria Decision Analysis}},
