@inproceedings{ ISI:000346067900236,
Author = {Spyridi, D. and Vlachokostas, Ch and Moussiopoulos, N. and Sioutas, C.},
Editor = {{Lekkas, TD}},
Title = {{EVALUATION OF MEASURES TO ADDRESS CLIMATE CHANGE USING MULTICRITERIA
   ANALYSIS}},
Booktitle = {{PROCEEDINGS OF THE 13TH INTERNATIONAL CONFERENCE ON ENVIRONMENTAL
   SCIENCE AND TECHNOLOGY}},
