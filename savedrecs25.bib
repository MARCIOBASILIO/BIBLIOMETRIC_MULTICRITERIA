@inproceedings{ ISI:000326810700005,
Author = {Su, Chiu-Hung and Tzeng, Gwo-Hshiung and Tseng, Hao-Lin},
Book-Group-Author = {{IEEE}},
Title = {{Improving cloud computing service in fuzzy environment - Combining fuzzy
   DANP and fuzzy VIKOR with a new hybrid FMCDM model}},
Booktitle = {{2012 INTERNATIONAL CONFERENCE ON FUZZY THEORY AND ITS APPLICATIONS
   (IFUZZY2012)}},
Year = {{2012}},
Pages = {{30-35}},
Note = {{International Conference on Fuzzy Theory and Its Applications (iFUZZY),
   Natl Chung Hsing Univ, Taichung, TAIWAN, NOV 16-18, 2012}},
Organization = {{Taiwan Fuzzy Syst Assoc; Natl Sci Council; Natl Taiwan Univ Sci \&
   Technol; IEEE Syst, Man \& Cybernet Soc, Taipei, Taichung \& Tainan
   Chapters; Chinese Automat Control Soc; Smart Grid Program Off, Natl Sci
   \& Technol Project Energy; IEEE Syst, Man \& Cybernet Soc; Int Fuzzy
   Syst Assoc; Korean Inst Intelligent Syst; Int Journal Fuzzy Syst; Int
   Journal Intelligent Syst Sci \& Technol; Int Journal Computat
   Intelligence Res \& Applicat}},
Abstract = {{Understanding user intentions and behaviors with regards to online cloud
   computing service applications will be helpful in predicting whether an
   application will be adopted by enterprises and guiding the improvement
   processes of providers. A wide range of criteria is used to assess the
   quality of cloud computing services. However, most of these criteria
   have interdependent or interactive characteristics, which can make it
   difficult to effectively analyze and increase usage intentions of cloud
   computing services. This study aims to address this issue by using a
   hybrid fuzzy MCDM approach that includes the fuzzy DEMATEL, fuzzy DANP
   and fuzzy VIKOR methods to improve service levels and achieve the goal
   of meeting user needs in fuzzy environments. By exploring the
