@article{ ISI:000318970400004,
Author = {Sabri, Soheil and Ludin, Ahmad Nazri Muhammad M. and Ho, Chin Siong},
Title = {{Conceptual Design for an Integrated Geosimulation and Analytic Network
   Process (ANP) in Gentrification Appraisal}},
Journal = {{APPLIED SPATIAL ANALYSIS AND POLICY}},
