@article{ ISI:000311016400009,
Author = {Hung, Ying-Hsun and Huang, Tsong-Liang and Hsieh, Jing-Chzi and Tsuei,
   Hung-Jia and Cheng, Chun-Chuan and Tzeng, Gwo-Hshiung},
Title = {{Online reputation management for improving marketing by using a hybrid
   MCDM model}},
Journal = {{KNOWLEDGE-BASED SYSTEMS}},
Year = {{2012}},
Volume = {{35}},
Pages = {{87-93}},
Month = {{NOV}},
Abstract = {{Online reputation management (ORM) has been considered as a significant
   tool of internet marketing. The purpose of this paper is to construct a
   decision model for evaluating performances and improving professional
   services of marketing. To investigate the interrelationship and
   influential weights among criteria, this study uses a hybrid MCDM model
   including decision-making trial and evaluation laboratory (DEMATEL).
