@inproceedings{ ISI:000392509700006,
Author = {Sousa, Bruno and Pentikousis, Kostas and Curado, Marilia},
Editor = {{Pesch, D and TimmGiel, A and Calvo, RA and Wenning, BL and Pentikousis, K}},
Title = {{Enhancing Path Selection in Multihomed Nodes}},
Booktitle = {{MOBILE NETWORKS AND MANAGEMENT}},
