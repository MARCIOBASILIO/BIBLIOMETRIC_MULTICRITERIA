@article{ ISI:000304094900008,
Author = {Chai, Junyi and Liu, James N. K. and Xu, Zeshui},
Title = {{A NEW RULE-BASED SIR APPROACH TO SUPPLIER SELECTION UNDER INTUITIONISTIC
   FUZZY ENVIRONMENTS}},
Journal = {{INTERNATIONAL JOURNAL OF UNCERTAINTY FUZZINESS AND KNOWLEDGE-BASED
   SYSTEMS}},
