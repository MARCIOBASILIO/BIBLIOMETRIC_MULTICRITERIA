@article{ ISI:000325240800002,
Author = {Saeed, Mohamed Osman and Ahamad, Mohd Sanusi S. and Aziz, Hamidi Abdul
   and Ahmad, Siti Zubaidah},
Title = {{An integrated AHP-GIS technique for landfill siting: A case study in
   Malaysia}},
