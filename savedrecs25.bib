@article{ ISI:000319713700009,
Author = {Streimikiene, Dalia},
Title = {{ASSESSMENT OF ROAD TRANSPORT TECHNOLOGIES BASED ON GHG EMISSION
   REDUCTION POTENTIAL AND COSTS}},
