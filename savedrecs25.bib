@article{ ISI:000313069000013,
Author = {Tahernejad, Mohammad Mehdi and Khalokakaie, Reza and Ataei, Mohammad},
Title = {{Determining proper strategies for Iran's dimensional stone mines: a
   SWOT-AHP analysis}},
Journal = {{ARABIAN JOURNAL OF GEOSCIENCES}},
