@article{ ISI:000297823300106,
Author = {Chen, Yueh-Hsiang and Chao, Ru-Jen},
Title = {{Supplier selection using consistent fuzzy preference relations}},
Journal = {{EXPERT SYSTEMS WITH APPLICATIONS}},
Year = {{2012}},
Volume = {{39}},
Number = {{3}},
Pages = {{3233-3240}},
Month = {{FEB 15}},
Abstract = {{Supplier selection is usually involved a multi-criteria evaluation to
   assist decision-making. This study proposed a simple method for vendor
   selection. The proposed procedure utilizes the structure of criteria in
   analytic hierarchy process (AHP) model and employs consistent fuzzy
   preference relations (CFPR) to construct the decision matrices. The
   advantage of CFPR is computational simplicity and efficiency. The
   computation can be done by using an Excel spreadsheet or writing a
   simple computer program. Also, the CFPR method guarantees the
   consistency of decision matrices. In this paper, a step-by-step
   empirical example demonstrates the usage of this proposed method. (C)
   2011 Elsevier Ltd. All rights reserved.}},
