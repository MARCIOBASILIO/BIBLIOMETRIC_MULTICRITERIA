@article{ ISI:000324415200001,
Author = {Wu, Chung-Min and Hsieh, Ching-Lin and Chang, Kuei-Lun},
Title = {{A Hybrid Multiple Criteria Decision Making Model for Supplier Selection}},
Journal = {{MATHEMATICAL PROBLEMS IN ENGINEERING}},
Year = {{2013}},
Volume = {{2013}},
Abstract = {{The sustainable supplier selection would be the vital part in the
   management of a sustainable supply chain. In this study, a hybrid
   multiple criteria decision making (MCDM) model is applied to select
   optimal supplier. The fuzzy Delphi method, which can lead to better
   criteria selection, is used to modify criteria. Considering the
   interdependence among the selection criteria, analytic network process
   (ANP) is then used to obtain their weights. To avoid calculation and
   additional pairwise comparisons of ANP, a technique for order preference
   by similarity to ideal solution (TOPSIS) is used to rank the
   alternatives. The use of a combination of the fuzzy Delphi method, ANP,
   and TOPSIS, proposing an MCDM model for supplier selection, and applying
   these to a real case are the unique features of this study.}},
