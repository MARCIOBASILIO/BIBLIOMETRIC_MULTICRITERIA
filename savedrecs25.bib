@article{ ISI:000311422300013,
Author = {Liu, Kuang-Sheng and Hsueh, Sung-Lin and Wu, Wen-Chen and Chen, Yu-Lung},
Title = {{A DFuzzy-DAHP Decision-Making Model for Evaluating Energy-Saving Design
   Strategies for Residential Buildings}},
