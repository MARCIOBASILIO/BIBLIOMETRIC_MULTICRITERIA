@inproceedings{ ISI:000346483800177,
Author = {Hu Moufa and Gao Lei and Zhang Zhiyong},
Book-Group-Author = {{IEEE}},
Title = {{Comprehensive evaluation on ISAR anti-jamming effectiveness via
   ELECTRE-IIII}},
Booktitle = {{2013 IEEE INTERNATIONAL CONFERENCE ON INFORMATION AND AUTOMATION (ICIA)}},
