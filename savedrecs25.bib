@article{ ISI:000304218100006,
Author = {Lin, Rong-Ho},
Title = {{An integrated model for supplier selection under a fuzzy situation}},
Journal = {{INTERNATIONAL JOURNAL OF PRODUCTION ECONOMICS}},
Year = {{2012}},
Volume = {{138}},
Number = {{1}},
Pages = {{55-61}},
Month = {{JUL}},
Abstract = {{Supplier selection as an essential component of supply chain management
   is usually a multi-criteria decision problem which, in actual business
   contexts, may have to be solved in the absence of precise information.
   To tackle with the multiple criteria and the inherent uncertainty in
   supplier selection, this study propose to adopt the fuzzy analytic
   network process (FANP) approach first to identify top suppliers by
   considering the effects of interdependence among selection criteria and
   to handle inconsistent and uncertain judgments. FANP is then integrated
   with fuzzy multi-objective linear programming (FMOLP) in selecting the
   best suppliers for achieving optimal order allocation under fuzzy
   conditions. (C) 2012 Elsevier B.V. All rights reserved.}},
