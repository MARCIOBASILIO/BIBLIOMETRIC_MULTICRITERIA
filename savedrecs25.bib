@inproceedings{ ISI:000335578000161,
Author = {Tomaskova, Hana and Gavalec, Martin},
Editor = {{Vojackova, H}},
Title = {{Decision making based on tropical algebra}},
Booktitle = {{MATHEMATICAL METHODS IN ECONOMICS 2013, PTS I AND II}},
Year = {{2013}},
Pages = {{950-955}},
Note = {{31st International Conference on Mathematical Methods in Economics,
   Jihlava, CZECH REPUBLIC, SEP 11-13, 2013}},
Abstract = {{In AHP approach to multi-criteria decision problem, the relative
   importance of alternatives is computed from preference matrices, which
   come from experience and can possibly be inconsistent. Standardly, the
   preference vector is computed as the eigenvector of the preference
   matrix by methods of linear algebra. Alternative use of non-standard
   methods in tropical algebra is considered in this paper. The preference
   matrix will be processed by the methods used in max-prod algebra and
   other tropical algebras. Given preference matrix will be transformed by
   the tropical operations, until a steady state is reached. The
   eigenvector of the matrix then describes the steady state preferences
   and respects all preference relations contained in the original matrix.
   Efficient algorithms for computing eigenvectors in the tropical algebra
   are described. The method is illustrated by numerical examples and
   compared with the linear algebra approach. The consistent and
   inconsistent cases are considered.}},
