@article{ ISI:000297823300081,
Author = {Buyukozkan, Gulcin and Cifci, Gizem},
Title = {{A novel hybrid MCDM approach based on fuzzy DEMATEL, fuzzy ANP and fuzzy
   TOPSIS to evaluate green suppliers}},
Journal = {{EXPERT SYSTEMS WITH APPLICATIONS}},
Year = {{2012}},
