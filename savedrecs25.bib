@article{ ISI:000305129500005,
Author = {Ye, Jun},
Title = {{Multicriteria Group Decision-Making Method Using Vector Similarity
   Measures For Trapezoidal Intuitionistic Fuzzy Numbers}},
Journal = {{GROUP DECISION AND NEGOTIATION}},
