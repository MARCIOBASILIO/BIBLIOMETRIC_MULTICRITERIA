@article{ ISI:000310739800005,
Author = {Akay, D. and Boran, F. E. and Yilmaz, M. and Atak, M.},
Title = {{The Evaluation of Power Plants Investment Alternatives with Grey
   Relational Analysis Approach for Turkey}},
Journal = {{ENERGY SOURCES PART B-ECONOMICS PLANNING AND POLICY}},
Year = {{2013}},
Volume = {{8}},
Number = {{1}},
Pages = {{35-43}},
Abstract = {{The evaluation of investments on the expansion of power-generating
   capacities is a challenging one that requires consideration of many
   conflicting criteria and objectives. Consequently, these sorts of
   problems are quite suited to the use of multi-criteria decision making
   methods. The aim of this study is to select the best power plants among
   six different energy investment options, currently being planned for
   construction in Turkey. Decisions are made subject to a set of 17
   criteria relating to technical, economic, and environmental
   considerations. As a methodological tool, the grey relational analysis
   (GRA) approach is applied to solve the presented problem. The analysis
   is also supported through scenarios by giving different weights to the
   criteria in the GRA in order to improve the quality of decision given by
   policy makers.}},
Publisher = {{TAYLOR \& FRANCIS INC}},
Address = {{530 WALNUT STREET, STE 850, PHILADELPHIA, PA 19106 USA}},
Type = {{Article}},
