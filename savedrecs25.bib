@article{ ISI:000304236900002,
Author = {Yang, Wu-E and Wang, Jian-Qiang and Wang, Xin-Fan},
Title = {{An outranking method for multi-criteria decision making with duplex
   linguistic information}},
Journal = {{FUZZY SETS AND SYSTEMS}},
Year = {{2012}},
Volume = {{198}},
Pages = {{20-33}},
Month = {{JUL 1}},
Abstract = {{An outranking method for duplex linguistic multi-criteria decisions is
   presented. We define a duplex linguistic set that is especially suitable
   for describing a decision-maker's preference, and assess each
   alternative with respect to criteria using duplex linguistic sets in a
   multi-criteria decision-making problem. An outranking relation between
   the alternatives is introduced. This novel outranking relation is based
   on systematic comparison of assessments of the alternatives for each
   criterion. The dominance of the duplex linguistic sets plays a crucial
