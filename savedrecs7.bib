Language = {{English}},
Affiliation = {{Musumba, GW (Corresponding Author), Univ Free State, Dept Comp Sci \& Informat, Private Bag X13, ZA-9866 Kestell, South Africa.
   Musumba, George Wamamu; Wario, Ruth Diko, Univ Free State, Dept Comp Sci \& Informat, Private Bag X13, ZA-9866 Kestell, South Africa.}},
DOI = {{10.1080/20421338.2019.1655212}},
Early Access Date = {{SEP 2019}},
ISSN = {{2042-1338}},
EISSN = {{2042-1346}},
