@article{ ISI:000488037000004,
Author = {Negi, Arvind Singh and Katiyar, Jitendra Kumar and Kumar, Sandeep and
   Kumar, Nitin and Patel, Vinay Kumar},
Title = {{Physicomechanical and abrasive wear properties of hemp/Kevlar/carbon
   reinforced hybrid epoxy composites}},
Journal = {{MATERIALS RESEARCH EXPRESS}},
