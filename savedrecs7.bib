@article{ ISI:000470947000082,
Author = {Lo, Huai-Wei and Liou, James J. H. and Tzeng, Gwo-Hshiung},
Title = {{Comments on ``Sustainable recycling partner selection using fuzzy
   DEMATEL-AEW-FVIKOR: A case study in small-and-medium enterprises{''}}},
Journal = {{JOURNAL OF CLEANER PRODUCTION}},
Year = {{2019}},
Volume = {{228}},
Pages = {{1011-1012}},
Month = {{AUG 10}},
Abstract = {{This paper discusses a previously published paper titled ``sustainable
   recycling partner selection using fuzzy DEMATEL-AEW-FVIKOR: A case study
   in small-and-medium enterprises (SMEs){''}, by Zhou et al. (2018).
   Authors of the aforementioned paper have discussed sustainable recycling
   partners for SMEs and contributed to the multiple-criteria
   decision-making (MCDM) modelling. However, in their study, the crucial
   weights generated using the decision-making trial and evaluation
   laboratory (DEMATEL) technique are questionable, may not be accurate for
   obtaining the subjective weights of criteria. In addition, using VIKOR
   together with an aspiration level concept is recommended for determining
   management implications. The present discussion includes several
   opinions and suggestions for improving the article and can be considered
   an attempt to complete the original paper. (C) 2019 Elsevier Ltd. All
   rights reserved.}},
