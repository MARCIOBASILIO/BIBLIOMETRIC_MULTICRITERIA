@article{ ISI:000486100800001,
Author = {Huang, Shufang and Zhang, Honghao},
Title = {{Green supply chain management of automotive manufacturing industry
   considering multiperspective indices}},
Journal = {{IEEJ TRANSACTIONS ON ELECTRICAL AND ELECTRONIC ENGINEERING}},
