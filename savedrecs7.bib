@article{ ISI:000500012900003,
Author = {Shumaiza and Akram, Muhammad and Al-Kenani, Ahmad N.},
Title = {{Multiple-Attribute Decision Making ELECTRE II Method under Bipolar Fuzzy
   Model}},
