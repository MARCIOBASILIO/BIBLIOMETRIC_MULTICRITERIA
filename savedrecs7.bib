@article{ ISI:000477784000023,
Author = {Kheybari, Siamak and Rezaie, Fariba Mahdi and Naji, S. Ali and Najafi,
   Fatemeh},
Title = {{Evaluation of energy production technologies from biomass using
   analytical hierarchy process: The case of Iran}},
Journal = {{JOURNAL OF CLEANER PRODUCTION}},
Year = {{2019}},
Volume = {{232}},
Pages = {{257-265}},
Month = {{SEP 20}},
Abstract = {{The purpose of this research was to evaluate technologies converting
   biomass to biofuels through the multi-criteria decision-making (MCDM)
   approach. To this end, first a comprehensive framework of criteria
   affecting the evaluation of technologies was presented. The proposed
   framework was employed to evaluate five technologies, including
   gasification, bioethanol, biogas (anaerobic digestion), biodiesel, and
   combustion, based on the condition of Iran in the second step. To
   determine both the weight of criteria presented in the framework and the
   performance of technologies in the criteria, the opinion of a sample of
   experts was sought through a questionnaire designed by the analytical
   hierarchy process (AHP). The results indicated that water use with the
   weight of 0.1425 (out of 1) among 22 criteria is the most important
   factor in the framework for evaluating the five technologies and biogas
   (anaerobic digestion) is the first option for energy generation from
   biomass in Iran. (C) 2019 Elsevier Ltd. All rights reserved.}},
