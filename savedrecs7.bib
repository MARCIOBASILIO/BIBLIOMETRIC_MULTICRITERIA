@article{ ISI:000469155600012,
Author = {Hafezalkotob, Arian and Hafezalkotob, Ashkan and Liao, Huchang and
   Herrera, Francisco},
Title = {{An overview of MULTIMOORA for multi-criteria decision-making: Theory,
   developments, applications, and challenges}},
