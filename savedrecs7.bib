@article{ ISI:000491022300001,
Author = {Pena, Luis E. and Zapata, Mario A. and Barrios, Miguel},
Title = {{Analytic hierarchy process approach for the selection of stream-gauging
   sites}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
