@article{ ISI:000498497700001,
Author = {Wang, Jie and Wang, Ping and Wei, Guiwu and Wei, Cun and Wu, Jiang},
Title = {{Some power Heronian mean operators in multiple attribute decision-making
   based on q-rung orthopair hesitant fuzzy environment}},
Journal = {{JOURNAL OF EXPERIMENTAL \& THEORETICAL ARTIFICIAL INTELLIGENCE}},
Year = {{2020}},
Volume = {{32}},
