@article{ ISI:000491675200001,
Author = {Khalid, Muhammad Abdullah and Ali, Yousaf},
Title = {{Economic impact assessment of natural disaster with multi-criteria
   decision making for interdependent infrastructures}},
Journal = {{ENVIRONMENT DEVELOPMENT AND SUSTAINABILITY}},
Year = {{2020}},
Volume = {{22}},
