@article{ ISI:000495404700003,
Author = {Braun, Adeli Beatriz and Trentin, Adan William da Silva and Visentin,
   Caroline and Thome, Antonio},
Title = {{Proposal for an optimized method for sustainable remediation evaluation
   and application: implementation of a multi-criteria process}},
