Author = {Zhao, Rong and Ge, Peng and Jin, Maozhu and Ren, Peiyu and
   Fernandez-Martinez, Manuel},
Title = {{A discrete method based on distance measure in linguistic space and its
   application for CMADM problem}},
Journal = {{JOURNAL OF DIFFERENCE EQUATIONS AND APPLICATIONS}},
