@article{ ISI:000494884000077,
Author = {Liu, Jicheng and Yin, Yu},
Title = {{An integrated method for sustainable energy storing node optimization
   selection in China}},
Journal = {{ENERGY CONVERSION AND MANAGEMENT}},
Year = {{2019}},
Volume = {{199}},
Month = {{NOV 1}},
Abstract = {{Energy storage has developed into a key solution to the problem of
   energy abandonment, which has been paid close attention. In the era of
   energy sharing, there is a general trend of cooperation between energy
   storage companies and power suppliers. This paper evaluates energy
   storage node enterprises to make their alliance more valuable with power
   generation node enterprises. First, this paper expounds the importance
   of sustainable energy storing node optimization selection and identifies
   23 criteria from the perspective of benefits, opportunities, costs and
   risks to establish the evaluation index system. Then a hybrid ANP-BOCR
   -TOPSIS method is proposed to evaluate the index system including
   determining the integrated criteria weights and ranking four
   alternatives by TOPSIS method. Next, a case study is used to verify the
   accuracy and feasibility of the hybrid method. Through the validation
   with DEMATEL and comparative analysis of three integrated methods, it is
   concluded that the hybrid ANP-BOCR-TOPSIS method can be applied to the
   evaluation of clean energy further research. Finally, the suggestions
   and conclusions are given in the end.}},
