@article{ ISI:000482478500002,
Author = {Dobrovolskiene, Nomeda and Tamosiuniene, Rima and Banaitis, Audrius and
   Ferreira, Fernando A. F. and Banaitiene, Nerija and Taujanskaite, Kamile
   and Meidute-Kavaliauskiene, Ieva},
Title = {{Developing a composite sustainability index for real estate projects
   using multiple criteria decision making}},
Journal = {{OPERATIONAL RESEARCH}},
Year = {{2019}},
