@article{ ISI:000487074800012,
Author = {Fulzele, Vijayta and Shankar, Ravi and Choudhary, Divya},
Title = {{A model for the selection of transportation modes in the context of
   sustainable freight transportation}},
Journal = {{INDUSTRIAL MANAGEMENT \& DATA SYSTEMS}},
