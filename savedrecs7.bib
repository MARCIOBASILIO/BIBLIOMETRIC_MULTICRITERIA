@article{ ISI:000503277900257,
Author = {Morkunaite, Zydrune and Bausys, Romualdas and Zavadskas, Edmundas
   Kazimieras},
Title = {{Contractor Selection for Sgraffito Decoration of Cultural Heritage
   Buildings Using the WASPAS-SVNS Method}},
Journal = {{SUSTAINABILITY}},
Year = {{2019}},
Volume = {{11}},
Number = {{22}},
Month = {{NOV}},
Abstract = {{Sgraffito is an ancient decorative technique applied in walls, produced
   by using layers of cement plaster tinted in contrasting colours to a
   moistened surface. This decoration originated in Italy and has been
   expanded in different European cities or villages. Nowadays, this
   decoration technique has almost disappeared because of decomposition,
   inexperience, or lack of knowledge. The preservation and restoration of
   the sgraffito technique is the most challenging and vital task of
   cultural heritage buildings' preservation. Consequently, the
   conservation of sgraffito depends not only on the historical and
   architectural knowledge, studies, and conservation projects of the
   cultural heritage buildings, but also demands the experience, proper
   qualification, and knowledge of the contractor. This paper presents six
   principal criteria. Three possible variants for contractor selection for
   sgraffito decoration of cultural heritage buildings are proposed, and
   six principal criteria for their evaluation are suggested. The research
   employs the Analytic Hierarchy Process (AHP), SWARA (Step-Wise Weights
   Assessment Ratio Analysis), and WASPAS-SVNS (Weight Aggregated Sum
   Product Assessment-Single- Valued Neutrosophic Set) methods.}},
