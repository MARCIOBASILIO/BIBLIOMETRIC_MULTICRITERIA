@article{ ISI:000488994300032,
Author = {Gong, Kaixin and Chen, Chunfang},
Title = {{Multiple-attribute decision making based on equivalence consistency
   under probabilistic linguistic dual hesitant fuzzy environment}},
Journal = {{ENGINEERING APPLICATIONS OF ARTIFICIAL INTELLIGENCE}},
Year = {{2019}},
Volume = {{85}},
Pages = {{393-401}},
Month = {{OCT}},
Abstract = {{Due to the complexity of the real problems and the limitations of
   expert's knowledge, it is often difficult for decision makers to give
   the exact decision information. How to accurately and adequately express
   the evaluation information of decision makers becomes the primary
   problem of multi-attribute decision making(MADM). Therefore, a large
   number of fuzzy sets(such as linguistic hesitant fuzzy sets(LHFS),
   probabilistic hesitant fuzzy sets(PHFS), dual hesitant fuzzy sets(DHFS))
   that express decision makers' judgment information have been widely used
   in multi-attribute decision making. Inspired by these fuzzy sets, this
   paper proposes a new set called probabilistic linguistic dual hesitant
   fuzzy sets(PLDHFS). Based on the PLDHFS, the probabilistic linguistic
   dual hesitant fuzzy preference relation(PLDHFPR) is further given. In
   order to obtain a reasonable ranking weight vector, the equivalence
   consistency between the PLDHFPR and the interval-valued fuzzy preference
   relation(IVFPR) is proposed, and we present the objective programming
   model(OPM) based on the multiplicative consistency of the IVFPR.
   Moreover, we establish a consistency improvement model for inconsistent
   PLDHFPR. Finally, an example of venture capital is analyzed to
   demonstrate the feasibility and effectiveness of the proposed method.}},
Publisher = {{PERGAMON-ELSEVIER SCIENCE LTD}},
