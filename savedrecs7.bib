@article{ ISI:000465200500005,
Author = {Hosseinian, Amir Hossein and Baradaran, Vahid},
Title = {{An Energy-efficient Mathematical Model for the Resource-constrained
   Project Scheduling Problem: An Evolutionary Algorithm}},
