@article{ ISI:000490263600001,
Author = {Wang, Yuhong and Xu, Guangcheng and Zhang, Wanting and Zhou, Zhixiang},
Title = {{Location Analysis of Earthquake Relief Warehouses: Evaluating the
   Efficiency of Location Combinations by DEA}},
