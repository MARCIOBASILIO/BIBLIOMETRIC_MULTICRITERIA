@article{ ISI:000485890300006,
Author = {Garg, Harish and Rani, Dimple},
Title = {{Exponential, Logarithmic and Compensative Generalized Aggregation
   Operators Under Complex Intuitionistic Fuzzy Environment}},
Journal = {{GROUP DECISION AND NEGOTIATION}},
