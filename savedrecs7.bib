@article{ ISI:000471674400005,
Author = {Kadoic, Nikola and Divjak, Blazenka and Redep, Nina Begicevic},
Title = {{Integrating the DEMATEL with the analytic network process for effective
   decision-making}},
Journal = {{CENTRAL EUROPEAN JOURNAL OF OPERATIONS RESEARCH}},
