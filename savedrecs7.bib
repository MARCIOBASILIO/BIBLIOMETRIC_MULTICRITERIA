@article{ ISI:000511013100001,
Author = {Varshney, Ayush and Khan, Arshad H. and Yasin, M. Yaqoob and Khan, Zahid
   A. and Asjad, Mohammad},
Title = {{On the optimal dynamic design of laminated composite folded plates: a
   multi-criteria decision analysis}},
Journal = {{MULTIDISCIPLINE MODELING IN MATERIALS AND STRUCTURES}},
