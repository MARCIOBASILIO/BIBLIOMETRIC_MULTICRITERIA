@article{ ISI:000479054800026,
Author = {Jamshidi, Afshin and Jamshidi, Farzad and Ait-Kadi, Daoud and Ramudhin,
   Amar},
Title = {{A review of priority criteria and decision-making methods applied in
   selection of sustainable city logistics initiatives and collaboration
   partners}},
