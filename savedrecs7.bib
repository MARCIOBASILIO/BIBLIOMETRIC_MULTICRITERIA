@article{ ISI:000482491500105,
Author = {Chen, Yong and Jin, Qiuxia and Fang, Hui and Lei, Hui and Hu, Jiarui and
   Wu, Yanqi and Chen, Jie and Wang, Cheng and Wan, Yuehua},
Title = {{Analytic network process: Academic insights and perspectives analysis}},
