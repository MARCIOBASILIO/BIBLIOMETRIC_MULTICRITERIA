@article{ ISI:000487205800052,
Author = {Dong, Yilin and Li, Xinde and Dezert, Jean and Ge, Shuzhi Sam},
Title = {{A Novel Multi-Criteria Discounting Combination Approach for Multi-Sensor
   Fusion}},
Journal = {{IEEE SENSORS JOURNAL}},
Year = {{2019}},
Volume = {{19}},
