Author = {Gupta, Shubham and Soni, Umang and Kumar, Girish},
Title = {{Green supplier selection using multi-criterion decision making under
   fuzzy environment: A case study in automotive industry}},
Journal = {{COMPUTERS \& INDUSTRIAL ENGINEERING}},
Year = {{2019}},
