@article{ ISI:000489686700001,
Author = {Bagdanaviciute, Ingrida and Kelpsaite-Rimkiene, Loreta and Galiniene,
   Jelena and Soomere, Tarmo},
Title = {{Index based multi-criteria approach to coastal risk assesment}},
