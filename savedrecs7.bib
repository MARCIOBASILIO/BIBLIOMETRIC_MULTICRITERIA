@article{ ISI:000482478500003,
Author = {Bilbao-Terol, Amelia and Arenas-Parra, Mar and Onopko-Onopko, Vitali},
Title = {{Measuring regional sustainable competitiveness: a multi-criteria
   approach}},
Journal = {{OPERATIONAL RESEARCH}},
