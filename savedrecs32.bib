@article{ ISI:A1993LW74000003,
Author = {BANAI, R},
Title = {{FUZZINESS IN GEOGRAPHICAL INFORMATION-SYSTEMS - CONTRIBUTIONS FROM THE
   ANALYTIC HIERARCHY PROCESS}},
Journal = {{INTERNATIONAL JOURNAL OF GEOGRAPHICAL INFORMATION SYSTEMS}},
Year = {{1993}},
Volume = {{7}},
Number = {{4}},
Pages = {{315-329}},
Month = {{JUL-AUG}},
Abstract = {{Recent developments in geographical information systems have drawn upon
   concepts of fuzzy set theory and multi-criteria methodology. In this
   paper we argue that there is a method, Saaty's Analytic Hierarchy
   Process (AHP), that is compatible with both these research directions.
   The contributions of the AHP are highlighted in the light of recent
   developments in GIS, with particular attention to the concept of fuzzy
   set theory. An example of a GIS application is provided to show how the
   AHP can deal operationally with fuzziness, factor diversity and
   complexity in problems of land evaluation involving the location of a
   public facility.}},
