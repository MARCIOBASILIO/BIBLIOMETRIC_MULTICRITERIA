@article{ ISI:A1991GX31300115,
Author = {SASAKI, M and NAKAHARA, Y and GEN, M and IDA, K},
Title = {{AN EFFICIENT ALGORITHM FOR SOLVING FUZZY MULTIOBJECTIVE 0-1
   LINEAR-PROGRAMMING PROBLEM}},
Journal = {{COMPUTERS \& INDUSTRIAL ENGINEERING}},
Year = {{1991}},
Volume = {{21}},
Number = {{1-4}},
Pages = {{647-651}},
Abstract = {{In this paper, we propose a new method for solving fuzzy multiple
   objective 0-1 LP (0-1 FMLP) problem that extend the efficient algorithm
   considering the generalized upper bounding (GUB) structure, which arise
   the case that nonlinear programming problem is transformed to 0-1 linear
   programming problem, to more general multiple objective decision making
   (MODM) problem.  And also, the fuzzyness of the decision maker(DM) due
   to his/her subjective judgement is able to take into account by
   introducing fuzzy set theory to the goal values.  Also, this method
   enables to cope with the change of condition about system resources or
