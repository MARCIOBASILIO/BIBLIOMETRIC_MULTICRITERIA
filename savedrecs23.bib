@article{ ISI:000333999000010,
Author = {Shen, Kao-Yi and Yan, Min-Ren and Tzeng, Gwo-Hshiung},
Title = {{Combining VIKOR-DANP model for glamor stock selection and stock
   performance improvement}},
Journal = {{KNOWLEDGE-BASED SYSTEMS}},
