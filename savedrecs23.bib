@article{ ISI:000333867900001,
Author = {Chakraborty, Shankar and Zavadskas, Edmundas Kazimieras},
Title = {{Applications of WASPAS Method in Manufacturing Decision Making}},
Journal = {{INFORMATICA}},
Year = {{2014}},
Volume = {{25}},
Number = {{1}},
Pages = {{1-20}},
Abstract = {{In order to survive in the present day global competitive environment,
   it now becomes essential for the manufacturing organizations to take
   prompt and correct decisions regarding effective use of their scarce
   resources. Various multi-criteria decision-making (MCDM) methods are now
   available to help those organizations in choosing the best decisive
   course of actions. In this paper, the applicability of weighted
   aggregated sum product assessment (WASPAS) method is explored as an
   effective MCDM tool while solving eight manufacturing decision making
   problems, such as selection of cutting fluid, electroplating system,
   forging condition, arc welding process, industrial robot, milling
   condition, machinability of materials, and electro-discharge
   micro-machining process parameters. It is observed that this method has
   the capability of accurately ranking the alternatives in all the
   considered selection problems. The effect of the parameter A. on the
   ranking performance of WASPAS method is also studied.}},
