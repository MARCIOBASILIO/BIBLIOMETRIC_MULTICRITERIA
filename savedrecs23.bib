@inproceedings{ ISI:000366837206064,
Author = {Zualkernan, Imran A.},
Editor = {{Chova, LG and Martinez, AL and Torres, IC}},
Title = {{AN ANALYTICAL NETWORK PROCESS MODEL TO RANK ALTERNATIVE LEARNING
   TECHNOLOGY INTERVENTIONS IN DEVELOPING COUNTRIES}},
