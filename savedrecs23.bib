@article{ ISI:000340262700004,
Author = {Li, Qian and Dou, Runliang and Chen, Fuzan and Nan, Guofang},
Title = {{A QoS-oriented Web service composition approach based on
   multi-population genetic algorithm for Internet of things}},
Journal = {{INTERNATIONAL JOURNAL OF COMPUTATIONAL INTELLIGENCE SYSTEMS}},
Year = {{2014}},
Volume = {{7}},
Number = {{2, SI}},
Pages = {{26-34}},
Abstract = {{Internet of things (IoT) will create new opportunities to build
   applications that better integrate real-time state of the industry. With
   Web services accomplishing similar function proliferated, industrial
   enterprises have to choose appropriate Web services according Quality of
   Service (QoS) properties. It introduces the problem of QoS-oriented
   service composition (QSC). This study formulates the QSC problem as a
   multi-criteria goal programming (MCGP) model, and develops a
   multi-population genetic algorithm (MGA) to solve the model. MCGP not
   only automatically assigns high quality Web services to combine a
   composite service, but also finds non inferior composite services by
   relaxing QoS constraints to satisfy users' QoS requirements. Empirical
   comparisons demonstrate MGA outperforms to the GA. Moreover, the
   experiments indicate MGA is capable to solve the large-scale QSC problem
   in terms of efficiency and scalability.}},
