@article{ ISI:000333867900005,
Author = {Stanujkic, Dragisa and Magdalinovic, Nedeljko and Milanovic, Dragan and
   Magdalinovic, Srdjana and Popovic, Gabrijela},
Title = {{An Efficient and Simple Multiple Criteria Model for a Grinding Circuit
   Selection Based on MOORA Method}},
Journal = {{INFORMATICA}},
Year = {{2014}},
Volume = {{25}},
Number = {{1}},
Pages = {{73-93}},
Abstract = {{The comminution process, particularly grinding, is very important in the
   mineral processing industry. Some characteristics of ore particles,
   which occur as a product of grinding process, have a significant impact
   on the effects of further ore processing. At the same time, this process
   requires a significant amount of energy which significantly affects the
   overall processing costs. Therefore, in this paper, we propose new
   multiple criteria decision making model, based on the Ratio system part
   of the MOORA method, which should enable an efficient selection of the
   adequate comminution circuit design.}},
