@article{ ISI:000335272500011,
Author = {Kubler, Sylvain and Voisin, Alexandre and Derigent, William and Thomas,
   Andre and Rondeau, Eric and Framling, Kary},
Title = {{Group fuzzy AHP approach to embed relevant data on ``communicating
   material{''}}},
