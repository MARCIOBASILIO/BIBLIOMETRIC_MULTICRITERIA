@inproceedings{ ISI:000350226700117,
Author = {Polednikova, Eva},
Editor = {{Loster, T and Pavelka, T}},
Title = {{MULTICRITERIA EVALUATION OF REGIONAL DISPARITIES IN VISEGRAD FOUR}},
Booktitle = {{8TH INTERNATIONAL DAYS OF STATISTICS AND ECONOMICS}},
