@article{ ISI:000348334200018,
Author = {Esmaeili, Ahmad and Kahnali, Reza Ahmadi and Rostamzadeh, Reza and
   Zavadskas, Edmundas Kazimieras and Sepahvand, Akbar},
Title = {{THE FORMULATION OF ORGANIZATIONAL STRATEGIES THROUGH INTEGRATION OF
   FREEMAN MODEL, SWOT, AND FUZZY MCDM METHODS: A CASE STUDY OF OIL
   INDUSTRY}},
Journal = {{TRANSFORMATIONS IN BUSINESS \& ECONOMICS}},
Year = {{2014}},
Volume = {{13}},
Number = {{3C}},
Pages = {{602-627}},
Abstract = {{Due to perpetual novelty in technology and change, increasing
   consumption of oil products and also a tendency toward privatization in
   Iran, it seems necessary to formulate a strategic plan for the oil
   industry. This research takes a quantitative approach to analytically
   determine the best strategies for the given industry. In order to
   respond accurately to the needs of organization, key stakeholders of an
   oil refining company was identified through the use of the Freeman's
   model. After developing a SWOT (Strengths, Weaknesses, Opportunities and
   Threats) matrix, the critical factors were determined, according to
   which the effective practical strategies were identified Based on the
