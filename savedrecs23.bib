@article{ ISI:000331503500010,
Author = {Boujelben, Mohamed Ayman and De Smet, Yves},
Title = {{A choice model with imprecise ordinal evaluations}},
Journal = {{INTERNATIONAL JOURNAL OF APPROXIMATE REASONING}},
Year = {{2014}},
Volume = {{55}},
Number = {{2}},
Pages = {{689-710}},
Month = {{JAN}},
Abstract = {{We consider multicriteria choice problems where the actions are
   evaluated on ordinal criteria and where they can be assessed
   imprecisely. In order to select the subset of best actions, the pairwise
   comparisons between the actions on each criterion are modeled by basic
   belief assignments (BBAs). Dempster's rule of combination is used for
   the aggregation of the BBAs of each pair of alternatives in order to
   express a global comparison between them on all the criteria. A model
   inspired by ELECTRE I is also proposed and illustrated by a pedagogical
   example. (C) 2013 Elsevier Inc. All rights reserved.}},
