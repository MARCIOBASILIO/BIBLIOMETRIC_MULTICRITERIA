Author = {Sawicki, Piotr and Sawicka, Hanna},
Editor = {{DeSousa, JF and DeSousa, JP and Costa, A and Farias, T and Melo, S}},
Title = {{Logistics process improvement using simulation and stochastic multiple
   criteria decision aiding}},
