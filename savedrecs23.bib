Series = {{Lecture Notes in Artificial Intelligence}},
Year = {{2014}},
Volume = {{8818}},
Pages = {{173-183}},
Note = {{9th International Conference on Rough Sets and Knowledge Technology
   (RSKT), Tongji Univ, Shanghai, PEOPLES R CHINA, OCT 24-26, 2014}},
Organization = {{Natl Nat Sci Fdn China; Int Rough Set Soc; Chinese Assoc Artificial
   Intelligence, Rough Sets \& Soft Comp Soc}},
Abstract = {{Dominance-based Rough Sets Approach (DRSA) is a useful tool for
   multi-criteria classification problems solving. Parallel computing is an
   efficient way to accelerate problems solving. Computation of
   approximations is a vital step to find the solutions with rough sets
   methodologies. In this paper, we propose a matrix-based approach for
