@inproceedings{ ISI:000368524100031,
Author = {Musakwa, W. and Makoni, E. N. and Kangethe, M. and Segooa, L.},
Editor = {{Li, S and Dragicevic, S}},
Title = {{DEVELOPING A DECISION SUPPORT SYSTEM TO IDENTIFY STRATEGICALLY LOCATED
   LAND FOR LAND REFORM IN SOUTH AFRICA}},
Booktitle = {{ISPRS TECHNICAL COMMISSION II SYMPOSIUM}},
