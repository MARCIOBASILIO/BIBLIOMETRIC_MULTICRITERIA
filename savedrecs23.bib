@inproceedings{ ISI:000342986500038,
Author = {Wang, Chen-Shu and Lin, Shiang-Lin},
Editor = {{Park, JJ and Chen, SC and Gil, JM and Yen, NY}},
Title = {{A Hybrid Multi-Criteria Decision Support Model: Combining DANP with MDS}},
