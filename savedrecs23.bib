@article{ ISI:000334088800005,
Author = {Kourtit, Katima and Macharis, Cathy and Nijkamp, Peter},
Title = {{A multi-actor multi-criteria analysis of the performance of global
   cities}},
Journal = {{APPLIED GEOGRAPHY}},
Year = {{2014}},
Volume = {{49}},
Number = {{SI}},
Pages = {{24-36}},
Month = {{MAY}},
Abstract = {{The strong world-wide urbanisation trend calls for a repositioning of
   cities, especially the large cities with a global impact. These cities
   tend to become economic, logistic and political powerhouses and are
   increasingly involved in a competition on their integral performance.
   The present paper aims to trace to what extent and why some cities
   outperform others. Starting from an extensive database on many important
   characteristics of global cities, this paper offers a multi-criteria
   methodology for identifying the relative position of various important
   cities on the basis of distinct assessment criteria. The explicit
   consideration of the perceptions of important classes of stakeholders on
   the performance outcomes of the various cities involved allows for an
   enhanced policy analysis. From a technical assessment perspective, the
   applied part of the paper employs the MAMCA and PROMETHEE multi-criteria
   methodology, which have proven their analytical power in various
   multi-criteria evaluation problems over the past years. The paper
   concludes with some policy perspectives and lessons. (C) 2013 Elsevier
   Ltd. All rights reserved.}},
