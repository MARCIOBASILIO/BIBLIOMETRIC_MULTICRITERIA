@inproceedings{ ISI:000366666800058,
Author = {Bisio, Igor and Braccini, Carlo and Delucchi, Stefano and Lavagetto,
   Fabio and Marchese, Mario},
Editor = {{Jamalipour, A and Deng, DJ}},
Title = {{Dynamic Multi-Attribute Network Selection Algorithm for Vertical
   Handover Procedures over Mobile Ad Hoc Networks}},
Booktitle = {{2014 IEEE INTERNATIONAL CONFERENCE ON COMMUNICATIONS (ICC)}},
