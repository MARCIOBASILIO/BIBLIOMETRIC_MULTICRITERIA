@inproceedings{ ISI:000347907600199,
Author = {Wang, Zhou-Jing and Li, Kevin W.},
Editor = {{Ning, B and Chen, J and Cai, X and Zhang, Z and Zhang, R and Zhang, J}},
Title = {{An approach to aggregating interval weights for hierarchical multiple
   criteria decision making}},
