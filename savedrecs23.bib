Title = {{Was the Construction Sector in 20 European Countries Anti-Cyclical
   during the Recession Years 2008-2009 as measured by Multicriteria
   Analysis (MULTIMOORA)?}},
Booktitle = {{2ND INTERNATIONAL CONFERENCE ON INFORMATION TECHNOLOGY AND QUANTITATIVE
   MANAGEMENT, ITQM 2014}},
Series = {{Procedia Computer Science}},
