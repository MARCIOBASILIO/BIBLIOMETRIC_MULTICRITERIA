@article{ ISI:000332921700007,
Author = {Kaya, Ihsan and Kahraman, Cengiz},
Title = {{A COMPARISON OF FUZZY MULTICRITERIA DECISION MAKING METHODS FOR
   INTELLIGENT BUILDING ASSESSMENT}},
Journal = {{JOURNAL OF CIVIL ENGINEERING AND MANAGEMENT}},
Year = {{2014}},
Volume = {{20}},
Number = {{1}},
Pages = {{59-69}},
Month = {{FEB}},
Abstract = {{The methodology, Multicriteria Decision Making (MCDM), refers to finding
   the best alternative from all of the feasible alternatives in the
   presence of multiple, usually conflicting, decision criteria. Nowadays,
   intelligent buildings' performance that is increasingly evidenced in
   building design and construction has been analyzed by using MCDM
   techniques. Intelligent buildings (IBs) are also under assessment
   according to their IB related characteristics and actual circumstances
   as a MCDM problem. In this paper, two most known MCDM methodologies,
   Analytic Hierarchy Process (AHP) and TOPSIS (Technique for Order
   Preference by Similarity to Ideal Solution), are used for intelligent
   building assessment under fuzzy environment for dealing with the
   evaluations' uncertainty and imprecision in which the expert's
   comparisons that are represented as fuzzy numbers. For this aim, three
   intelligent building alternatives for a business centre in Istanbul are
   evaluated by using these two fuzzy MCDM methods and the obtained ranking
   results are compared.}},
