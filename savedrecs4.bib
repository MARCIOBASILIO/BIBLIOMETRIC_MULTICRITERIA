@article{ ISI:000543400500006,
Author = {Abdollahi, Amir Ali and Babazadeh, Hossein and Yargholi, Bahman and
   Taghavi, Lobat},
Title = {{ZONING THE RATE OF POLLUTION IN DOMESTIC RIVER USING SPATIAL MULTI -
   CRITERIA EVALUATION MODEL}},
Journal = {{CIVIL AND ENVIRONMENTAL ENGINEERING}},
