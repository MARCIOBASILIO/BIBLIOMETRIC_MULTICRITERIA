@article{ ISI:000529365900010,
Author = {Verma, Jitendra Singh and Sharma, Sanjeev},
Title = {{Promethee Based Distributed Multihead Energy and Coverage Preserving
   Clustering Algorithm for Mobile Sensor Nodes}},
Journal = {{NATIONAL ACADEMY SCIENCE LETTERS-INDIA}},
Year = {{2020}},
Volume = {{43}},
Number = {{2}},
Pages = {{157-161}},
Month = {{APR}},
Abstract = {{In this paper, a preference ranking organization method for enrichment
   evaluations (PROMETHEE) based distributed multihead energy and coverage
   preserving clustering algorithm for mobile sensor nodes (PDMECCA) is
   proposed. The objective of proposed PDMECCA is to equally distribute the
   load (communication, computation) among Mobile Sensor Nodes using the
   multihead concept for balancing the energy consumption among nodes and
   increase the lifetime of network with the ability to persistently
   provide coverage and connectivity of the entire monitored area. This
   paper considers PROMETHEE method for optimal selection of cluster heads
   based on multi criteria. The results of simulation show that proposed
   algorithm effectively prolongs the network lifetime and provide constant
   coverage persistently.}},
