@article{ ISI:000566695100138,
Author = {Jiao, Hongbo},
Title = {{Selection of Resettlement Site in Reservoir Construction Using
   Pythagorean Fuzzy MULTIMOORA Multi-criteria Decision-making Method}},
Journal = {{JOURNAL OF COASTAL RESEARCH}},
Year = {{2020}},
Number = {{115}},
Pages = {{502-505}},
Month = {{SUM}},
Abstract = {{The resettlement is an important part of reservoir construction and the
   selection of resettlement site is directly related to the success or
   failure of resettlement work. Existing research don't show enough power
   to handle the complexity and uncertainty in selection of resettlement
   site in reservoir (RSR) problem. This study aims to create a theoretical
   evaluation model using decision making method for RSR selection.
   Firstly, the factors affecting RSR selection decision-making was
   constructed. Secondly, based on Pythagorean fuzzy sets, this study
   constructed Pythagorean fuzzy ratio system, Pythagorean fuzzy reference
   point approach and Pythagorean fuzzy full multiplicative form. Thirdly,
   Pythagorean fuzzy MULTIMOORA decision-making method through integrating
   Pythagorean fuzzy information and MULTIMOORA method was established.
   Finally, a case study on RSR selecting was presented.}},
