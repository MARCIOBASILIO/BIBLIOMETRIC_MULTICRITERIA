@article{ ISI:000531558100311,
Author = {Papapostolou, Aikaterini and Karakosta, Charikleia and Apostolidis,
   Georgios and Doukas, Haris},
Title = {{An AHP-SWOT-Fuzzy TOPSIS Approach for Achieving a Cross-Border RES
   Cooperation}},
Journal = {{SUSTAINABILITY}},
