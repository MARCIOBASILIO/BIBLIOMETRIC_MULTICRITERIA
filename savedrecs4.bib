@article{ ISI:000527262500001,
Author = {Gu, Jinheng and Liu, Changqing and Fu, Shenghui and Pang, Changle},
Title = {{Implementing product design assessment with sandwich-like architecture:
   A case study in an automotive company}},
Journal = {{PROCEEDINGS OF THE INSTITUTION OF MECHANICAL ENGINEERS PART C-JOURNAL OF
   MECHANICAL ENGINEERING SCIENCE}},
