@article{ ISI:000534085100027,
Author = {Bera, Ashoke Kumar and Jana, Dipak Kumar and Banerjee, Debamalya and
   Nandy, Titas},
Title = {{Supplier selection using extended IT2 fuzzy TOPSIS and IT2 fuzzy MOORA
   considering subjective and objective factors}},
