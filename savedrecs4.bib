@article{ ISI:000536516800002,
Author = {Huang, Show-Hui Sheree and Hsu, Wen-Kai Kevin and Chen, Jen-Wen},
Title = {{A safety evaluation system based on a revised fuzzy AHP for dangerous
   goods in airfreights}},
Journal = {{JOURNAL OF TRANSPORTATION SAFETY \& SECURITY}},
Year = {{2020}},
Volume = {{12}},
Number = {{5}},
Pages = {{611-627}},
Month = {{MAY 27}},
Abstract = {{With the rapid development of new electronic products, the styles and
   types of dangerous goods (DGs) in international trades get increasing
   significantly, leading to threatening the airfreight safety. The purpose
   of this article is to assess the safety of DG airfreights from the
   perspectives of operational procedures. Based on the relevant literature
   and the features of the operational procedures in DGs airfreights, the
   safety factors (SFs) were first investigated. A safety evaluation system
   (SES) based on a revised fuzzy analytical hierarchy process (RFAHP) was
   then developed to assess the SFs. Finally, to validate the SES in
   practical applications, the DG airfreights in Taiwan were empirically
   investigated. The results can provide practical information for
   airfreight operators to making policies in improving their safety
   performance in DG airfreights. Further, the RFAHP approach can provide a
   theoretical reference for relevant research in MCDM methodologies.}},
