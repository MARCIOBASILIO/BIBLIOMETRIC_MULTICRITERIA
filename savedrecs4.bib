@article{ ISI:000544346600001,
Author = {Sisay, Getahun and Gebre, Sintayehu Legesse and Getahun, Kefelegn},
Title = {{GIS-based potential landfill site selection using MCDM-AHP modeling of
   Gondar Town, Ethiopia}},
Journal = {{AFRICAN GEOGRAPHICAL REVIEW}},
Abstract = {{Selecting an appropriate landfill site is one of the major problems of
   towns and cities. This study was aimed to map the optimum suitable
   landfill sites in Gondar town, Ethiopia. To achieve the objectives, a
   GIS-based multi-criteria evaluation method was employed. Nine-criteria
   were considered to select appropriate landfill sites. The result of the
   suitability map shows that about 4.5\% of the study area is highly
   suitable for the landfill site. The result of this study could be used
   as an initial input for further detailed identification of landfill site
   selection to enhance the waste disposal management of Gondar town.}},
