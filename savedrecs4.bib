@article{ ISI:000544455800005,
Author = {Ishizaka, Alessio and Tasiou, Menelaos and Martinez, Luis},
Title = {{Analytic hierarchy process-fuzzy sorting: An analytic hierarchy
   process-based method for fuzzy classification in sorting problems}},
Journal = {{JOURNAL OF THE OPERATIONAL RESEARCH SOCIETY}},
