@article{ ISI:000550839500001,
Author = {Jocic, Kristina Jaukovic and Jocic, Goran and Karabasevic, Darjan and
   Popovic, Gabrijela and Stanujkic, Dragisa and Zavadskas, Edmundas
   Kazimieras and Phong Thanh Nguyen},
Title = {{A Novel Integrated PIPRECIA-Interval-Valued Triangular Fuzzy ARAS Model:
   E-Learning Course Selection}},
Journal = {{SYMMETRY-BASEL}},
Year = {{2020}},
Volume = {{12}},
Number = {{6}},
Month = {{JUN}},
Abstract = {{The development of information and communication technologies has
   revolutionized and changed the way we do business in various areas. The
   field of education did not remain immune to the mentioned changes; there
   was a gradual integration of the educational process and the mentioned
   technologies. As a result, platforms for distance learning, as well as
   the organization of e-learning courses of various types, have been
   developed. The rapid development of e-learning courses has led to the
   problem of e-learning course selection and evaluation. The problem of
   the e-learning course selection can be successfully solved by using
   multiple-criteria decision-making (MCDM) methods. Therefore, the aim of
   the paper is to propose an integrated approach based on the MCDM methods
   and symmetry principles for e-learning course selection. The pivot
   pairwise relative criteria importance assessment (PIPRECIA) method is
   used for determining the weights of criteria, and the interval-valued
   triangular fuzzy additive ratio assessment (ARAS) method is used for the
   ranking of alternatives i.e., e-learning courses. The suitability of the
   proposed integrated model is demonstrated through a numerical case
   study.}},
