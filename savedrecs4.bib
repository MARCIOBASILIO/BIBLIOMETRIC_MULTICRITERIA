@article{ ISI:000523561500030,
Author = {Maghsoodi, Abtin Ijadi and Riahi, Dara and Herrera-Viedma, Enrique and
   Zavadskas, Edmundas Kazimieras},
Title = {{An integrated parallel big data decision support tool using the
   W-CLUS-MCDA: A multi-scenario personnel assessment}},
Journal = {{KNOWLEDGE-BASED SYSTEMS}},
