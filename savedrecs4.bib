@article{ ISI:000523728900001,
Author = {Verma, Pradipika and Singh, Prafull and Srivastava, S. K.},
Title = {{Development of spatial decision-making for groundwater recharge
   suitability assessment by considering geoinformatics and field data}},
Journal = {{ARABIAN JOURNAL OF GEOSCIENCES}},
