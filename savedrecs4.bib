@article{ ISI:000517658600013,
Author = {Liu, Peide and Wang, Yumei},
Title = {{Multiple attribute decision making based on q-rung orthopair fuzzy
   generalized Maclaurin symmetic mean operators}},
Journal = {{INFORMATION SCIENCES}},
