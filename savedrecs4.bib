@article{ ISI:000542720900001,
Author = {Niu, Li-li and Li, Jian and Li, Feilong and Wang, Zhong-xing},
Title = {{Multi-criteria decision-making method with double risk parameters in
   interval-valued intuitionistic fuzzy environments}},
Journal = {{COMPLEX \& INTELLIGENT SYSTEMS}},
