@article{ ISI:000528424700008,
Author = {Rasheed, Rizwan and Javed, Hajra and Rizwan, Asfra and Yasar, Abdullah
   and Tabinda, Amtul Bari and Mahfooz, Yusra and Wang, Yubo and Su,
   Yuehong},
Title = {{Sustainability and CDM potential analysis of a novel vs conventional
   bioenergy projects in South Asia by multi-criteria decision-making
   method}},
Journal = {{ENVIRONMENTAL SCIENCE AND POLLUTION RESEARCH}},
Year = {{2020}},
Volume = {{27}},
Number = {{18}},
Pages = {{23081-23093}},
Month = {{JUN}},
Abstract = {{In this study, the multi-attribute utility theory model, one of the most
   widely utilized multi-criteria decision-making methods, is employed to
   analyze the relative sustainability and cleaner development mechanism
   (CDM) potential of a novel bioenergy project in Pakistan and four
   registered CDM bioenergy installations in China, India, Sri Lanka, and
   Thailand. Four major sustainability evaluation criteria selected for the
   study are technology, economy, environment, and social aspects, whereas
   ``simple multi-attribute rating technique{''} (SMART) and ``additive
   synthesis method{''} were utilized for the calculation of weighting
   factors. The most significant indicator was emission reduction, and
   least significant indicator was job creation with 24\% and 15\%
   respective weightages, in the sustainability scoring. Moreover, for
   three considered evaluation scenarios, (I) base case, (II) per kilowatt,
   and (III) per kilowatt hour, the bioenergy project in Pakistan has the
   highest comparative sustainability values of 0.735, 0.592, and 0.492
   respectively. This analysis will provide an insight assistance to
   developing countries for establishing sustainable energy installations
   under CDM.}},
