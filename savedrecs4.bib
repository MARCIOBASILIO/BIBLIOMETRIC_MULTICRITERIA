@article{ ISI:000526412600012,
Author = {Patil, Saurabh Dadasaheb and Ragha, Lata},
Title = {{Adaptive fuzzy-based message dissemination and micro-artificial bee
   colony algorithm optimised routing scheme for vehicular ad hoc network}},
Journal = {{IET COMMUNICATIONS}},
Year = {{2020}},
