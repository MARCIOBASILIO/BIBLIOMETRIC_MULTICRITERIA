@article{ ISI:000538253800001,
Author = {Fattahi, Reza and Tavakkoli-Moghaddam, Reza and Khalilzadeh, Mohammad
   and Shahsavari-Pour, Nasser and Soltani, Roya},
Title = {{A novel FMEA model based on fuzzy multiple-criteria decision-making
   methods for risk assessment}},
