Title = {{Application of TGRA-Based Optimisation for Machinability of
   High-Chromium Tool Steel in the EDM Process}},
Journal = {{ARABIAN JOURNAL FOR SCIENCE AND ENGINEERING}},
Year = {{2020}},
Volume = {{45}},
