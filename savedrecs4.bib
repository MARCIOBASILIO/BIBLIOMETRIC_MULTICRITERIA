@article{ ISI:000541512300016,
Author = {Agarski, Boris and Sokac, Mario and Santosi, Zeljko and Vukelic, Djordje
   and Budak, Igor},
Title = {{Multi-Criteria Evaluation of Design Complexity for Patient-Specific Bone
   Graft}},
Journal = {{TEHNICKI VJESNIK-TECHNICAL GAZETTE}},
