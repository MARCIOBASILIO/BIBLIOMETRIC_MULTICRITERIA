@article{ ISI:000544278400003,
Author = {Aguezzoul, Aicha and Pache, Gilles},
Title = {{AN AHP-ELECTRE I APPROACH FOR 3PL-PROVIDER SELECTION}},
Journal = {{INTERNATIONAL JOURNAL OF TRANSPORT ECONOMICS}},
