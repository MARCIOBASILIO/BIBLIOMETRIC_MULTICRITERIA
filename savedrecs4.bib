@article{ ISI:000531033800012,
Author = {Magibalan, Subramaniam and Senthilkumar, Palanisamy and Senthilkumar,
   Chinnamuthu and Prabu, Muthusamy},
Title = {{Multi-response optimization of AA 8011 and 12 wt.-\% fly ash composites}},
Journal = {{MATERIALS TESTING}},
