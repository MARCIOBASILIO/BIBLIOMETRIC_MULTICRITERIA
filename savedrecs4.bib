@article{ ISI:000532728000001,
Author = {Alsyouf, Imad and Hamdan, Sadeque and Shamsuzzaman, Mohammad and Haridy,
   Salah and Alawaysheh, Iyad},
Title = {{On preventive maintenance policies: a selection framework}},
Journal = {{JOURNAL OF QUALITY IN MAINTENANCE ENGINEERING}},
