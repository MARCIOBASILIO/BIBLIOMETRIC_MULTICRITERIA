@article{ ISI:000562062700010,
Author = {Hsu, Wen-Kai K. and Lian, Shu-Jun and Huang, Show-Hui S.},
Title = {{An assessment model based on a hybrid MCDM approach for the port choice
   of liner carriers}},
Journal = {{RESEARCH IN TRANSPORTATION BUSINESS AND MANAGEMENT}},
Year = {{2020}},
Volume = {{34}},
Number = {{SI}},
Month = {{MAR}},
Abstract = {{The purpose of this paper is to evaluate the port choice of liner
   carriers for ship calls. In this paper, based on the relevant literature
   and the operating features of liner carriers, the port choice factors
   (PCFs) for ship calls of liner carriers are first investigated. An
   assessment model based on a hybrid MCDM (Multiple Criteria Decision
   Making) approach is then proposed to assess the PCFs, by which port
   companies may make policies to improve their port managements. As an
   empirical study, the liner carriers in Kaohsiung Port were investigated
   to validate the model. The result may provide practical information for
   port companies to improve their performance in ship calls of carriers.
   Further, the proposed hybrid MCDM approach can provide theoretical
   reference for methodological research on multiple criteria decision
   makings.}},
