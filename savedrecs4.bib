@article{ ISI:000562590400002,
Author = {Dizdar, Ercument Neset and Kocar, Oguz},
Title = {{Fuzzy logic-based decision-making system design for safe forklift truck
   speed: cast cobblestone production application}},
Journal = {{SOFT COMPUTING}},
