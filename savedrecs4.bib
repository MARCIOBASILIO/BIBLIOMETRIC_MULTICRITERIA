@article{ ISI:000530043200041,
Author = {Sarkar, Biswajit and Biswas, Animesh},
Title = {{A unified method for Pythagorean fuzzy multicriteria group
   decision-making using entropy measure, linear programming and extended
   technique for ordering preference by similarity to ideal solution}},
Journal = {{SOFT COMPUTING}},
Year = {{2020}},
Volume = {{24}},
Number = {{7}},
Pages = {{5333-5344}},
Month = {{APR}},
Note = {{13th International Conference on Natural Computation, Fuzzy Systems and
