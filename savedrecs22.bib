@inproceedings{ ISI:000389523300080,
Author = {Dezert, Jean and Benameur, Kaouthar and Ratton, Laurent and Grandin,
   Jean-Francois},
Book-Group-Author = {{IEEE}},
Title = {{On the Quality Estimation of Optimal Multiple Criteria Data Association
   Solutions}},
