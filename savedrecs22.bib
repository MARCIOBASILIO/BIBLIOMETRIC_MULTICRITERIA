@article{ ISI:000337852900016,
Author = {Durbach, Ian and Lahdelma, Risto and Salminen, Pekka},
Title = {{The analytic hierarchy process with stochastic judgements}},
Journal = {{EUROPEAN JOURNAL OF OPERATIONAL RESEARCH}},
Year = {{2014}},
Volume = {{238}},
Number = {{2}},
Pages = {{552-559}},
Month = {{OCT 16}},
Abstract = {{The analytic hierarchy process (AHP) is a widely-used method for
   multicriteria decision support based on the hierarchical decomposition
   of objectives, evaluation of preferences through pairwise comparisons,
   and a subsequent aggregation into global evaluations. The current paper
   integrates the AHP with stochastic multicriteria acceptability analysis
   (SMAA), an inverse-preference method, to allow the pairwise comparisons
   to be uncertain. A simulation experiment is used to assess how the
   consistency of judgements and the ability of the SMAA-AHP model to
   discern the best alternative deteriorates as uncertainty increases.
   Across a range of simulated problems results indicate that, according to
   conventional benchmarks, judgements are likely to remain consistent
   unless uncertainty is severe, but that the presence of uncertainty in
   almost any degree is sufficient to make the choice of best alternative
   unclear. (C) 2014 Elsevier B.V. All rights reserved.}},
