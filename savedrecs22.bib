Editor = {{Loster, T and Pavelka, T}},
Title = {{RANKING OF EU COUNTRIES AND TURKEY IN TERMS OF SUSTAINABLE DEVELOPMENT
   INDICATORS: AN INTEGRATED APPROACH USING ENTROPY AND TOPSIS METHODS}},
Booktitle = {{9TH INTERNATIONAL DAYS OF STATISTICS AND ECONOMICS}},
Year = {{2015}},
Pages = {{22-32}},
