@article{ ISI:000338808000008,
Author = {Kou, Gang and Peng, Yi and Lu, Chen},
Title = {{MCDM APPROACH TO EVALUATING BANK LOAN DEFAULT MODELS}},
Journal = {{TECHNOLOGICAL AND ECONOMIC DEVELOPMENT OF ECONOMY}},
Year = {{2014}},
Volume = {{20}},
