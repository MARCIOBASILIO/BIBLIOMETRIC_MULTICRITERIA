Author = {Gavalec, Martin and Mls, Karel and Tomaskova, Hana},
Editor = {{Alonso, JM and Bustince, H and Reformat, M}},
Title = {{Optimization of the ordinal and cardinal consistency of a preference
   matrix in decision making}},
Booktitle = {{PROCEEDINGS OF THE 2015 CONFERENCE OF THE INTERNATIONAL FUZZY SYSTEMS
   ASSOCIATION AND THE EUROPEAN SOCIETY FOR FUZZY LOGIC AND TECHNOLOGY}},
Series = {{Advances in Intelligent Systems Research}},
