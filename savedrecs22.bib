@article{ ISI:000342862100021,
Author = {Milutinovic, Biljana and Stefanovic, Gordana and Dassisti, Michele and
   Markovic, Danijel and Vuckovic, Goran},
Title = {{Multi-criteria analysis as a tool for sustainability assessment of a
   waste management model}},
Journal = {{ENERGY}},
