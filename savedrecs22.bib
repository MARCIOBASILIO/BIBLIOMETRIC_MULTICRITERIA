@article{ ISI:000343712400002,
Author = {Lin, Sin-Jin and Wang, Chang-Sheng and Hsu, Ming-Fu},
Title = {{SELECTION OF MULTIPLE COMBINATION STRATEGIES FOR RISK ASSESSMENT}},
