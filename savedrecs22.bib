Author = {Khalili-Damghani, Kaveh and Aminzadeh-Goharrizi, Bahram and Rastegar,
   Saeed and Aminzadeh-Goharrizi, Babak},
Title = {{Solving land-use suitability analysis and planning problem by a hybrid
   meta-heuristic algorithm}},
