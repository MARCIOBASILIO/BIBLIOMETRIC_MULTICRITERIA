@article{ ISI:000342528900003,
Author = {Singh, Amol},
Title = {{Supplier evaluation and demand allocation among suppliers in a supply
   chain}},
Journal = {{JOURNAL OF PURCHASING AND SUPPLY MANAGEMENT}},
