@article{ ISI:000342316900006,
Author = {Mitra, Ashis and Majumdar, Abhijit and Ghosh, Anindya and Majumdar,
   Prabal Kumar and Bannerjee, Debamalya},
Title = {{Selection of Handloom Fabrics for Summer Clothing Using Multi-Criteria
   Decision Making Techniques}},
Journal = {{JOURNAL OF NATURAL FIBERS}},
Year = {{2015}},
Volume = {{12}},
Number = {{1}},
Pages = {{61-71}},
Abstract = {{Ranking and selection of textile fabrics for a particular end-use
   requirement is a complex task. In this article, an attempt has been made
   to develop a simple index of handloom fabric quality, which can be used
   for selecting fabrics for a specified end use. The Analytic Hierarchy
   Process (AHP) and Multiplicative Analytic Hierarchy Process (MAHP) of
   multi-criteria decision making (MCDM) have been used for ranking 25
   handloom cotton fabrics in terms of their overall quality value
   considering their applicability as summer clothing materials. The rank
   correlation between the rankings elicited from two MCDM methods was
   found to be 0.926 which implies that the rankings given by AHP and MAHP
   are in high degree of agreement with each other and any of the two
   methods can be chosen for ranking of fabrics.}},
