@article{ ISI:000337294700004,
Author = {Song, Wenyan and Ming, Xinguo and Wu, Zhenyong and Zhu, Baoting},
Title = {{A rough TOPSIS Approach for Failure Mode and Effects Analysis in
   Uncertain Environments}},
Journal = {{QUALITY AND RELIABILITY ENGINEERING INTERNATIONAL}},
