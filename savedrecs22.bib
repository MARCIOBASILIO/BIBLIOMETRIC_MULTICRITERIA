@article{ ISI:000352158200006,
Author = {Vesyropoulos, Nikos and Georgiadis, Christos K.},
Title = {{Customized QoS-based Mashups for the Web of Things: An Application of
   AHP}},
Journal = {{COMPUTER SCIENCE AND INFORMATION SYSTEMS}},
