@article{ ISI:000341183800004,
Author = {Baiocchi, Valerio and Lelo, Keti and Polettini, Alessandra and Pomi,
   Raffaella},
Title = {{Land suitability for waste disposal in metropolitan areas}},
Journal = {{WASTE MANAGEMENT \& RESEARCH}},
