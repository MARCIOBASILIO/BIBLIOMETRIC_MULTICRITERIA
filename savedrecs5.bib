@article{ ISI:000514483700045,
Author = {Kutlu Gundogdu, Fatma and Kahraman, Cengiz},
Title = {{A novel spherical fuzzy analytic hierarchy process and its renewable
   energy application}},
Journal = {{SOFT COMPUTING}},
Year = {{2020}},
Volume = {{24}},
Number = {{6}},
Pages = {{4607-4621}},
Month = {{MAR}},
Abstract = {{The extensions of ordinary fuzzy sets such as intuitionistic fuzzy sets,
   Pythagorean fuzzy sets, and neutrosophic sets, whose membership
   functions are based on three dimensions, aim at collecting experts'
   judgments more informatively and explicitly. In the literature,
   generalized three-dimensional spherical fuzzy sets have been introduced
   by Kutlu Gundogdu and Kahraman (J Intell Fuzzy Syst 36(1):337-352,
   2019a), including their arithmetic operations, aggregation operators,
   and defuzzification operations. In this paper, our aim is to extend
   classical analytic hierarchy process (AHP) to spherical fuzzy AHP
   (SF-AHP) method and to show its applicability and validity through a
   renewable energy location selection example and a comparative analysis
   between neutrosophic AHP and SF-AHP.}},
