@article{ ISI:000518595800040,
Author = {Rouyendegh, Babak Daneshvar and Yildizbasi, Abdullah and Ustunyer, Pelin},
Title = {{Intuitionistic Fuzzy TOPSIS method for green supplier selection problem}},
