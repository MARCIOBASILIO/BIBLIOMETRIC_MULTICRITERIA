@article{ ISI:000594722100010,
Author = {Gomes Pereira, Felipe George and Botter, Rui Carlos and Robles, Leo
   Tadeu},
Title = {{DECISION FACTORS IN THE DEPLOYMENT OF PUBLIC AND PRIVATE PORT TERMINALS
   IN THE ARCO NORTE EXPORT LOGISTIC CORRIDOR}},
Journal = {{REVISTA ELETRONICA DE ESTRATEGIA E NEGOCIOS-REEN}},
Year = {{2020}},
Volume = {{13}},
Number = {{2}},
Pages = {{225-247}},
Abstract = {{This article discusses the decision factors for the implementation of
   specialized port terminals in the region known as Arco Norte, ports
   located in the Northeast and North of the country. The Analytic
   Hierarchy Process (AHP) decision tool was used to apply a questionnaire
   with port specialists and managers. The results showed a great relevance
   for the Port Location criteria (54\%), followed by Economic and
   Financial (27.8\%), Performance and Dimensioning (11.2\%) and Contract
   and Legislation (6.9\%). The study concluded that the priority is for
   Private Use Terminals, considering the current situation in the country
   and the investments in private port terminals in force in recent years.}},
