@article{ ISI:000519054500027,
Author = {Qi, Lianyong and Chen, Yi and Yuan, Yuan and Fu, Shucun and Zhang, Xuyun
   and Xu, Xiaolong},
Title = {{A QoS-aware virtual machine scheduling method for energy conservation in
   cloud-based cyber-physical systems}},
