@article{ ISI:000607537100005,
Author = {Chang, Kuei-Lun},
Title = {{The Selection of Gamers for Professional Esports Teams}},
Journal = {{JOURNAL OF MULTIPLE-VALUED LOGIC AND SOFT COMPUTING}},
Year = {{2020}},
Volume = {{35}},
Number = {{3-4}},
Pages = {{325-346}},
Abstract = {{Professional gamers play an essential role in the esports industry, and
   their selection is a part of the multi-criteria decision-making (MCDM)
   problem to which an optimal solution can be found only through adequate
   selection criteria and processes. For this purpose, the present study
   applied a collaborated MCDM model, which incorporates the fuzzy Delphi
   method (FDM), the decision making trial and evaluation laboratory
   (DEMATEL), the analytic network process (ANP), and the technique for
   order preference by similarity to ideal solution (TOPSIS), to assist
   managers of Taiwanese professional esports teams in the selection of
   gamers. This critical topic has not been discussed in any study known to
   the author of this study.}},
