@article{ ISI:000497593000011,
Author = {Azzini, Ivan and Munda, Giuseppe},
Title = {{A new approach for identifying the Kemeny median ranking}},
Journal = {{EUROPEAN JOURNAL OF OPERATIONAL RESEARCH}},
