@article{ ISI:000522470900134,
Author = {Ardebili, Ali Aghazadeh and Padoano, Elio and Rahmani, Najmeh},
Title = {{Waste Reduction for Green Service Supply Chain-the Case Study of a
   Payment Service Provider in Iran}},
Journal = {{SUSTAINABILITY}},
