@article{ ISI:000519353900003,
Author = {Yildirim, Bahadir Fatih and Mercangoz, Burcu},
Title = {{Evaluating the logistics performance of OECD countries by using fuzzy
   AHP and ARAS-G}},
Journal = {{EURASIAN ECONOMIC REVIEW}},
