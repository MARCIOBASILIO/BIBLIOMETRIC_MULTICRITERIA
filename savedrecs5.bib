Editor = {{Cardoso, A and Alves, GR and Restivo, MT}},
Title = {{Evaluation of Decision-making Quality using Multi-attribute Decision
   Matrix and Design Thinking}},
Booktitle = {{PROCEEDINGS OF THE 2020 IEEE GLOBAL ENGINEERING EDUCATION CONFERENCE
   (EDUCON 2020)}},
Series = {{IEEE Global Engineering Education Conference}},
