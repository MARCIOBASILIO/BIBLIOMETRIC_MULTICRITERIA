@article{ ISI:000588349300001,
Author = {Yariyan, Peyman and Avand, Mohammadtaghi and Abbaspour, Rahim Ali and
   Torabi Haghighi, Ali and Costache, Romulus and Ghorbanzadeh, Omid and
   Janizadeh, Saeid and Blaschke, Thomas},
Title = {{Flood susceptibility mapping using an improved analytic network process
   with statistical models}},
