@article{ ISI:000508248700004,
Author = {Heidari-Fathian, Hassan and Davari-Ardakani, Hamed},
Title = {{Bi-objective optimization of a project selection and adjustment problem
   under risk controls}},
Journal = {{JOURNAL OF MODELLING IN MANAGEMENT}},
Year = {{2020}},
