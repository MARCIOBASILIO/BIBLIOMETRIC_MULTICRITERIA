@article{ ISI:000506742700001,
Author = {Sanau, Jaime and Gomez-Bahillo, Carlos and Maria Moreno-Jimenez, Jose},
Title = {{The economic valuation of social aspects: A multicriteria approach based
   on input-output models}},
Journal = {{JOURNAL OF MULTI-CRITERIA DECISION ANALYSIS}},
Year = {{2020}},
Volume = {{27}},
Number = {{1-2}},
Month = {{JAN}},
Abstract = {{Decision making on the suitability of public investment projects has
   traditionally been based on the study of their economic feasibility.
   Since the end of the 1970s, this has been complemented by a more
   holistic vision of reality in which social and environmental aspects are
   explicitly considered alongside economic factors. This paper proposes a
   new evaluation methodology that allows for the valuation of social and
   environmental factors in economic terms. The methodology is based on use
   values (direct, indirect, and potential) and nonuse values (bequest and
   existence) that are usually studied for an environmental evaluation. The
   resulting economic value is obtained by the utilization of (a)
   input-output models to calculate the direct use value in monetary terms
   and (b) multicriteria techniques (especially the analytic hierarchy
   process) and comparative models to estimate the remaining use (indirect
   and potential) and nonuse values (bequest and existence). The
   methodology is applied to a real-life problem in Aragon.}},
Publisher = {{WILEY}},
Address = {{111 RIVER ST, HOBOKEN 07030-5774, NJ USA}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Moreno-Jimenez, JM (Corresponding Author), Univ Zaragoza, Fac Econ \& Empresa, Zaragoza, Spain.
   Sanau, Jaime; Gomez-Bahillo, Carlos; Maria Moreno-Jimenez, Jose, Univ Zaragoza, Fac Econ \& Empresa, Zaragoza, Spain.}},
DOI = {{10.1002/mcda.1700}},
Early Access Date = {{JAN 2020}},
ISSN = {{1057-9214}},
EISSN = {{1099-1360}},
