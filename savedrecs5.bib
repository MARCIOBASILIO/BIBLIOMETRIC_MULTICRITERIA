@article{ ISI:000510109100001,
Author = {Kumari, Reetu and Mishra, Arunodaya Raj},
Title = {{Multi-criteria COPRAS Method Based on Parametric Measures for
   Intuitionistic Fuzzy Sets: Application of Green Supplier Selection}},
