@inproceedings{ ISI:000587768000001,
Author = {Li, Yinhao and Jha, Devki Nandan and Aujla, Gagangeet Singh and Morgan,
   Graham and Zomaya, Albert Y. and Ranjan, Rajiv},
Book-Group-Author = {{IEEE}},
Title = {{IoTWC: Analytic Hierarchy Process Based Internet of Things Workflow
   Composition System}},
