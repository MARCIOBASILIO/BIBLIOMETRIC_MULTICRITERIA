@article{ ISI:000544993600025,
Author = {Dogru, Ahmet Ozgur and Abdulhussein, Rawaa A. and Mercan, Selda and
   Kuloglu, Merve and Tekin, Tugba and Asicioglu, Faruk},
Title = {{A GIS assisted system for monitoring illicit drug use in wastewater}},
