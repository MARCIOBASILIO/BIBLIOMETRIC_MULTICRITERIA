@article{ ISI:000511596700028,
Author = {Joshi, Rajesh},
Title = {{A new multi-criteria decision-making method based on intuitionistic
   fuzzy information and its application to fault detection in a machine}},
Journal = {{JOURNAL OF AMBIENT INTELLIGENCE AND HUMANIZED COMPUTING}},
Year = {{2020}},
