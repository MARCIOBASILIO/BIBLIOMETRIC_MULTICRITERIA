@article{ ISI:000498396100011,
Author = {Fonseca, Juan D. and Latifi, Abderrazak M. and Orjuela, Alvaro and
   Rodriguez, Gerardo and Gil, Ivan D.},
Title = {{Modeling, analysis and multi-objective optimization of an industrial
   batch process for the production of tributyl citrate}},
Journal = {{COMPUTERS \& CHEMICAL ENGINEERING}},
Year = {{2020}},
Volume = {{132}},
Month = {{JAN 4}},
Abstract = {{This work describes the modeling and optimization of a trybutil citrate
   (TBC) production process at the industrial scale. The process comprises
   a batch reactive distillation for the esterification of citric acid with
   butanol. The dynamic model of the process was constructed based upon
   validated kinetic and thermodynamic models, implemented in gPROMS (R)
   software, verified with industrial scale experiments, and used for
   further optimization to minimize batch time and energy consumption.
   Manipulated process variables were the molar ratio of butanol to citric
   acid, the alcohol loading policy, and the heat supply profile, and a
   Pareto front of optimal solutions was obtained. Depending on the main
   goal, the optimization process allowed to obtain energy savings up to
   28\%, and the corresponding processing times were reduced by 36\%.
   Finally, multi-attribute utility theory (MAUT) approach was used as
   decision-making aid tool to select the preferred operating conditions
   for TBC production at the industrial scale. (C) 2019 Elsevier Ltd. All
   rights reserved.}},
Publisher = {{PERGAMON-ELSEVIER SCIENCE LTD}},
Address = {{THE BOULEVARD, LANGFORD LANE, KIDLINGTON, OXFORD OX5 1GB, ENGLAND}},
Type = {{Article}},
