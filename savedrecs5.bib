@article{ ISI:000511596600022,
Author = {Yang, Yu-Yao and Liu, Xin-Wang and Liu, Fang},
Title = {{Trapezoidal Interval Type-2 Fuzzy TOPSIS Using Alpha-Cuts}},
Journal = {{INTERNATIONAL JOURNAL OF FUZZY SYSTEMS}},
