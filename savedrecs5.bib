@article{ ISI:000582570100011,
Author = {Yazdi, Amir Karbassi and Hanne, Thomas and Osorio Gomez, Juan Carlos},
Title = {{EVALUATING THE PERFORMANCE OF COLOMBIAN BANKS BY HYBRID MULTICRITERIA
   DECISION MAKING METHODS}},
