@article{ ISI:000525287900009,
Author = {Abdel-Basset, Mohamed and Gamal, Abduallah and Son, Le Hoang and
   Smarandache, Florentin},
Title = {{A Bipolar Neutrosophic Multi Criteria Decision Making Framework for
   Professional Selection}},
Journal = {{APPLIED SCIENCES-BASEL}},
