@article{ ISI:000521279900007,
Author = {Oturakci, Murat and Dagsuyu, Cansu},
Title = {{Integrated environmental risk assessment approach for transportation
   modes}},
Journal = {{HUMAN AND ECOLOGICAL RISK ASSESSMENT}},
