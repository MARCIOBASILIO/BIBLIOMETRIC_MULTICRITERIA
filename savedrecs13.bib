@article{ ISI:000425198400014,
Author = {Buyukozkan, Gulcin and Feyzioglu, Orhan and Gocer, Fethullah},
Title = {{Selection of sustainable urban transportation alternatives using an
   integrated intuitionistic fuzzy Choquet integral approach}},
Journal = {{TRANSPORTATION RESEARCH PART D-TRANSPORT AND ENVIRONMENT}},
