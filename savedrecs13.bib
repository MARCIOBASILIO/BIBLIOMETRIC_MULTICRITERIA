Author = {Pourjavad, Ehsan and Mayorga, Rene V.},
Title = {{A fuzzy rule-based approach to prioritize third-party reverse logistics
   based on sustainable development pillars}},
Journal = {{JOURNAL OF INTELLIGENT \& FUZZY SYSTEMS}},
