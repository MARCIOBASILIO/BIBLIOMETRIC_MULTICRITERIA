@article{ ISI:000451062900001,
Author = {Hu, Shu-Kung and Liou, James J. H. and Chuang, Yen-Ching and Tzeng,
   Gwo-Hshiung},
Title = {{NEW HYBRID FMADM MODEL FOR MOBILE COMMERCE IMPROVEMENT}},
Journal = {{TECHNOLOGICAL AND ECONOMIC DEVELOPMENT OF ECONOMY}},
