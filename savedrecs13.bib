@article{ ISI:000442131100014,
Author = {Liou, James J. H. and Hsu, Chao-Che and Li, Chun-Sheng Joseph and
   Pineda, Pedro Jose Gudiel and Chang, Gin-Weng},
Title = {{DEVELOPING A SUCCESSFUL AEROTROPOLIS BY USING A HYBRID MODEL UNDER
   INFORMATION UNCERTAINTY}},
Journal = {{TECHNOLOGICAL AND ECONOMIC DEVELOPMENT OF ECONOMY}},
