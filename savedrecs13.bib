@article{ ISI:000423503400002,
Author = {Zhou, Wei and Yin, Wenying and Peng, Xiaoqing and Liu, Fuming and Yang,
   Fei},
Title = {{Comprehensive evaluation of land reclamation and utilisation schemes
   based on a modified VIKOR method for surface mines}},
Journal = {{INTERNATIONAL JOURNAL OF MINING RECLAMATION AND ENVIRONMENT}},
Year = {{2018}},
Volume = {{32}},
Number = {{2}},
Pages = {{93-108}},
Abstract = {{In view of the significance of evaluating reclamation schemes for
   surface mines, the authors propose an integrated evaluation method based
   on multi-attribute decision-making theory. An evaluation index system
   incorporating local conditions was constructed. Triangular fuzzy theory
   was applied to consider the fuzziness of indexes. Weights were
   calculated by combining the analytic hierarchy process and entropy
   method to reduce the influence of original data and subjective and
   objective factors. The programmes were sorted by the modified VIKOR
   method. Finally, farming, forestry and grass-animal husbandry schemes
   were evaluated for the Xiaolongtan surface mine to verify the
   effectiveness of the proposed approach.}},
