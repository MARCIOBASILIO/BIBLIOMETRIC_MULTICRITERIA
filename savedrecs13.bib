@article{ ISI:000451094800004,
Author = {Jakimavicius, Marius},
Title = {{ANALYSIS AND ASSESSMENT OF LITHUANIAN ROAD ACCIDENTS BY AHP METHOD}},
Journal = {{BALTIC JOURNAL OF ROAD AND BRIDGE ENGINEERING}},
