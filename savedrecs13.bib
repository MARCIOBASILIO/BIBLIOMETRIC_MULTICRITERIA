@article{ ISI:000454073800214,
Author = {Wang, Wenbao and Lin, Wenhe},
Title = {{Multi-criteria Decision Making Model for Evaluation of the Growth of
   Environmental Protection Enterprises}},
Journal = {{EKOLOJI}},
Year = {{2018}},
Volume = {{27}},
Number = {{106}},
Pages = {{1887-1894}},
Abstract = {{Establishing a scientific and reasonable growth evaluation model for
   environmental protection enterprises is of great significance. Aimed at
   the development of environmental protection enterprises, a growth
   evaluation index system that includes growth resources, growth ability
   and growth innovation was established, and a growth evaluation model
   that avoids subjective weighting of indicators was constructed by the
   Entropy-VIKOR algorithm. Based on the growth evaluation and analysis of
   the environmental protection enterprises listed, it is concluded that
   operating capacity, intangible resources, tangible resources and human
   resources are the key influencing factors for the growth of enterprises,
   and the stability and effectiveness of the model are verified. The
   results show that the evaluation model is reasonable and effective, and
