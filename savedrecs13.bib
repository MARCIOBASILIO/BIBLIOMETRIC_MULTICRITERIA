@article{ ISI:000430319500024,
Author = {Milosevic, L. and Mihajlovic, E. and Janackovic, G. and Vasovic, D. and
   Nikolic, J. Malenovic},
Title = {{NOVEL APPROACH TO LANDFILL FIRE PROTECTION ENGINEERING BASED ON
   MULTI-CRITERIA ANALYSIS AND PRINCIPLES OF SUSTAINABLE ENVIRONMENTAL
   MANAGEMENT}},
Journal = {{JOURNAL OF ENVIRONMENTAL PROTECTION AND ECOLOGY}},
Year = {{2018}},
Volume = {{19}},
Number = {{1}},
Pages = {{226-235}},
Abstract = {{Sustainable environmental management in the context of landfills
   involves the inclusion of sustainable planning in all landfill lifecycle
   phases, from initial design and location selection, through its use, to
   its closure. An important aspect in the planning process is the
   consideration of landfill fires, which are major problems from the
   viewpoint of efficient landfill use and environmental pollution.
   Sustainable landfill management is described by the set of indicators
   defining the processes of planning, maintenance, efficiency analysis,
   and raising awareness of the responsibility to protect the environment.
   The indicators describing landfill sustainability and fire issues are
   grouped into four categories, which define landfill organisation,
   environment, efficiency, and technology. A large number of indicators
   lead to difficulties in decision-making. Multiple-criteria decision
   analysis methods aid the decision-making process by selection of key
   indicators. In this paper, the method of group decision-making is
   applied during the selection of the key aspects that affect landfill
   fires and sustainable environmental management. From the perspective of
   fire protection, preventive controls, waste ignition potential, legal
   framework, landfill organisation, and landfill gas emissions are
   particularly important.}},
