@article{ ISI:000454962200005,
Author = {Estruch-Guitart, V and Valls-Civera, A.},
Title = {{An economic valuation of ecosystem services provided by the River Turia
   Natural Park (Valencia)}},
Journal = {{ECONOMIA AGRARIA Y RECURSOS NATURALES}},
Year = {{2018}},
Volume = {{18}},
Number = {{2}},
Pages = {{93-115}},
Abstract = {{The objective of this paper is to obtain the monetary value of ecosystem
   services provided by the Tuna Natural Park (TNP). This paper proposes
   the use of the Analytic Multicriteria Valuation Method (AMUVAM) by
   replacing the Analytical Hierarchy Process (AHP) with the Analytic
   Network Process (ANP). since the ANP method allows to analyze
   interdependence relationships between the services provided by a system.
   The results express that the economic value of the ecosystem services
   associated to the TNP ranges between 163,946,752(sic) and
   481,549,597(sic). The results reveal distinct patterns in the valuation
   of the existing services due to ethical issues.}},
