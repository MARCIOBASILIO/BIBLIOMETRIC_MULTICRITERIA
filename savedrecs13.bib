@article{ ISI:000451338400082,
Author = {Bolturk, Eda and Kahraman, Cengiz},
Title = {{Interval-valued intuitionistic fuzzy CODAS method and its application to
   wave energy facility location selection problem}},
Journal = {{JOURNAL OF INTELLIGENT \& FUZZY SYSTEMS}},
Year = {{2018}},
Volume = {{35}},
Number = {{4}},
Pages = {{4865-4877}},
Abstract = {{Intuitionistic Fuzzy Sets (IFSs) have extensively been employed in
   multi-criteria decision making (MCDM). COmbinative Distance-based
   Assessment (CODAS) method, which is relatively a new MCDM technique,
   aims to select the alternative having the largest Euclidean and Taxicab
   (Hamming) distances to the negative-ideal point. This paper develops an
   intuitionistic fuzzy CODAS method, namely Interval-Valued Intuitionistic
   Fuzzy CODAS (IVIF-CODAS), and introduces it to the literature first
   time. The objective of our paper is to make a multi-criteria decision
   under intuitionistic fuzzy uncertainty by taking the hesitancy of
   decision makers into consideration based on both Euclidean and Taxicab
   distances according to the negative-ideal point. To show the
   effectiveness of the developed method, an application is given for wave
   energy facility location selection in Turkey and the obtained results
   are compared with crisp CODAS method's.}},
Publisher = {{IOS PRESS}},
Address = {{NIEUWE HEMWEG 6B, 1013 BG AMSTERDAM, NETHERLANDS}},
Type = {{Article}},
