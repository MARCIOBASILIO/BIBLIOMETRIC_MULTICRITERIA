@inproceedings{ ISI:000437157500040,
Author = {Macuada, Claudio J. and Oddershede, Astrid M. and Quezada, Luis E.},
Editor = {{Dzitac, I and Filip, FG and Manolescu, MJ and Dzitac, S and Oros, H and Dzitac, D}},
Title = {{DM Methodology for Automating Technology System in Water Treatment
   Plants.}},
Booktitle = {{2018 7TH INTERNATIONAL CONFERENCE ON COMPUTERS COMMUNICATIONS AND
   CONTROL (ICCCC 2018)}},
