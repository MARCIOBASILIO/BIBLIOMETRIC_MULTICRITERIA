@article{ ISI:000449559900005,
Author = {Koseoglu, Burak and Buber, Muge and Toz, Ali C.},
Title = {{Optimum site selection for oil spill response center in the Marmara Sea
   using the AHP-TOPSIS method}},
Journal = {{ARCHIVES OF ENVIRONMENTAL PROTECTION}},
Year = {{2018}},
Volume = {{44}},
Number = {{4}},
Pages = {{38-49}},
Abstract = {{The aim of this study is to determine the optimum location for an oil
   spill response center in the Marmara Sea. The analytic hierarchy process
   (AHP) and technique for the order of preference by similarity to ideal
   solution (TOPSIS) method as the most preferred multi-criteria
   decision-making (MCDM) technique were used. The results reveal that the
   LOC criterion and PRA sub-criterion have the highest effects on the
   optimal location selection of the oil spill response center. According
   to the results. the most suitable location for the oil spill response
   center in the region is Izmit Bay Entrance. This location has been
   proposed for the response headquarters to manage the whole operation
   with the assistance of auxiliary installations in the area. In this
   study, only technical and operational variables are taken into account,
   but political and administrative criteria are excluded.}},
