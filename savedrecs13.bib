@article{ ISI:000429266000010,
Author = {Masoumi, Isa and Ahangari, Kaveh and Noorzad, Ali},
Title = {{Optimal monitoring instruments selection using innovative decision
   support system framework}},
Journal = {{SMART STRUCTURES AND SYSTEMS}},
