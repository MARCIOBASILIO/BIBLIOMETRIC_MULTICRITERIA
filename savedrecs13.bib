@article{ ISI:000436950800001,
Author = {Roig-Tierno, Norat and Baviera-Puig, Amparo and Buitrago-Vera, Juan and
   Escriba-Perez, Carmen},
Title = {{Assessing food retail competitors with a multi-criteria GIS-based method}},
Journal = {{ECONOMIA AGRARIA Y RECURSOS NATURALES}},
Year = {{2018}},
Volume = {{18}},
Number = {{1}},
Pages = {{5-22}},
Abstract = {{Given the importance of competition in the retail sector, this research
   builds on spatial interaction theory to develop the competition index
   (CI). For this, geographic information systems (GIS) and the analytic
   hierarchy process (AHP) were used. AHP results reveal that key factors
   to assess competitors relate to location and branding. The proposed
   method was tested by evaluating 45 supermarkets in the city of Castellon
