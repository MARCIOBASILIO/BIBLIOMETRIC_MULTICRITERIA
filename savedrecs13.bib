@inproceedings{ ISI:000454845100114,
Author = {Tiwari, Sushil and Jain, Vinod Kumar},
Book-Group-Author = {{IEEE}},
Title = {{Optimum Grid Size Estimation using Fuzzy-AHP for Radio Fingerprinting
   based Indoor Localization}},
Booktitle = {{2018 41ST INTERNATIONAL CONFERENCE ON TELECOMMUNICATIONS AND SIGNAL
   PROCESSING (TSP)}},
