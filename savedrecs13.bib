@inproceedings{ ISI:000452448400036,
Author = {Geri, Francesco and Sacchelli, Sandro and Bernetti, Iacopo and Ciolli,
   Marco},
Editor = {{Bisello, A and Vettorato, D and Laconte, P and Costa, S}},
Title = {{Urban-Rural Bioenergy Planning as a Strategy for the Sustainable
   Development of Inner Areas: A GIS-Based Method to Chance the Forest
   Chain}},
Booktitle = {{SMART AND SUSTAINABLE PLANNING FOR CITIES AND REGIONS, SSPCR 2017}},
