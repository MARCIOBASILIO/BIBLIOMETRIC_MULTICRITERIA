@article{ ISI:000426083200001,
Author = {Alinejad, Ensieh Ahmad and Pishvaee, Mir Saman and Naeini, Ali Bonyadi},
Title = {{Key success factors for logistics provider enterprises: an empirical
   investigation in Iran}},
