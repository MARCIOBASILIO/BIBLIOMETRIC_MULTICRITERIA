@article{ ISI:000445651000275,
Author = {Beemsterboer, D. J. C. and Hendrix, E. M. T. and Claassen, G. D. H.},
Title = {{On solving the Best-Worst Method in multi-criteria decision-making}},
Journal = {{IFAC PAPERSONLINE}},
Year = {{2018}},
