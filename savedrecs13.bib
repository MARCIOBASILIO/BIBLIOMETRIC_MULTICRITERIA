@article{ ISI:000430620000010,
Author = {Ou, Yue and Yi, Liangzhong and Zou, Bin and Pei, Zheng},
Title = {{The linguistic intuitionistic fuzzy set TOPSIS method for linguistic
   multi-criteria decision makings}},
Journal = {{INTERNATIONAL JOURNAL OF COMPUTATIONAL INTELLIGENCE SYSTEMS}},
