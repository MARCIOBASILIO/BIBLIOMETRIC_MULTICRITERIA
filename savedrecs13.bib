@article{ ISI:000429149000008,
Author = {Hsu, Wan-Chi Jackie and Tsai, Ming-Hone and Tzeng, Gwo-Hshiung},
Title = {{EXPLORING THE BEST STRATEGY PLAN FOR IMPROVING THE DIGITAL CONVERGENCE
