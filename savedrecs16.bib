@article{ ISI:000428200700013,
Author = {Mousavi, S. M.},
Title = {{Solving New Product Selection Problem by a New Hierarchical Group
   Decision-making Approach with Hesitant Fuzzy Setting}},
Journal = {{INTERNATIONAL JOURNAL OF ENGINEERING}},
