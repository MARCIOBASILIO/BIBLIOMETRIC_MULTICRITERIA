@inproceedings{ ISI:000452115800029,
Author = {Huang, Chi-Yo and Cheng, Jeng-Chieh},
Editor = {{Fujita, H and Selamat, A and Omatu, S}},
Title = {{A Derivation of Factors Influencing the Concept Development of Virtual
   Reality Products by Hybrid MADM Methods}},
