@article{ ISI:000399728300015,
Author = {Caiado, R. G. G. and Lima, G. B. A. and Gaviao, L. O. and Quelhas, O. L.
   G. and Paschoalino, F. F.},
Title = {{Sustainability Analysis in Electrical Energy Companies by Similarity
   Technique to Ideal Solution}},
Journal = {{IEEE LATIN AMERICA TRANSACTIONS}},
Year = {{2017}},
Volume = {{15}},
Number = {{4}},
Pages = {{675-681}},
Month = {{APR}},
Abstract = {{Nowadays, a tough problem for decision makers is to get a compromised
   solution for multi-objective functions taking into account various
   criteria for assessing corporate sustainability, according to the
   economic, environmental and social performance. The present study aims
   to propose a novel model for solving decision-making problems in the
   evaluation of corporate sustainability. The proposal was applied in the
   analysis of sustainability indicators of the energy sector in Brazil.
   The principle of the Technique for Order Preference by Similarity to
   Ideal Solution (TOPSIS) is based on the premise that the chosen solution
   should have the shortest distance from the positive ideal solution as
   well as the longest distance from the negative ideal solution. The
   applied model proved to be robust for the evaluation and ranking of the
   energy companies with respect to the observed aspects of sustainability.}},
