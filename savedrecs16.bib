@article{ ISI:000403331500002,
Author = {Zindani, Divya and Maity, Saikat R. and Bhowmik, Sumit and Chakraborty,
   Shankar},
Title = {{A material selection approach using the TODIM (TOmada de Decisao
   Interativa Multicriterio) method and its analysis}},
Journal = {{INTERNATIONAL JOURNAL OF MATERIALS RESEARCH}},
