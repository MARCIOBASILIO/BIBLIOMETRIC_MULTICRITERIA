@article{ ISI:000395116800001,
Author = {Keshavarz Ghorabaee, Mehdi and Amiri, Maghsoud and Zavadskas, Edmundas
   Kazimieras and Hooshmand, Reyhaneh and Antucheviciene, Jurgita},
Title = {{FUZZY EXTENSION OF THE CODAS METHOD FOR MULTI-CRITERIA MARKET SEGMENT
