Title = {{Multicriteria evaluation of the Internet of Things potential in health
   care: The case of dementia care}},
Booktitle = {{2017 IEEE 19TH CONFERENCE ON BUSINESS INFORMATICS (CBI), VOL 1}},
Series = {{Conference on Business Informatics}},
