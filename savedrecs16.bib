@article{ ISI:000404966900047,
Author = {Yousefi, Nazila and Mehralian, Gholamhossein and Rasekh, Hamid Reza and
   Yousefi, Mina},
Title = {{New Product Development in the Pharmaceutical Industry: Evidence from a
   generic market}},
