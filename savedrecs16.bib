@article{ ISI:000437053800003,
Author = {Hamurcu, Mustafa and Alagas, Huci Mehmet and Eren, Tamer},
Title = {{SELECTION OF RAIL SYSTEM PROJECTS WITH ANALYTIC HIERARCHY PROCESS AND
   GOAL PROGRAMMING}},
