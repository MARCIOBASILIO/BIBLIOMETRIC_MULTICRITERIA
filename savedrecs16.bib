Author = {Ben Alla, Hicham and Ben Alla, Said and Ezzati, Abdellah},
Editor = {{Sabir, E and GarciaArmada, A and Ghogho, M and Debbah, M}},
Title = {{A Priority Based Task Scheduling in Cloud Computing Using a Hybrid MCDM
   Model}},
