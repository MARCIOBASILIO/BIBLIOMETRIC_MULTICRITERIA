@inproceedings{ ISI:000427598701048,
Author = {Al Garni, Hassan Z. and Awasthi, Anjali},
Book-Group-Author = {{IEEE}},
Title = {{A Fuzzy AHP and GIS-based Approach to Prioritize Utility-Scale Solar PV
   Sites in Saudi Arabia}},
Booktitle = {{2017 IEEE INTERNATIONAL CONFERENCE ON SYSTEMS, MAN, AND CYBERNETICS
   (SMC)}},
