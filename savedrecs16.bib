@inproceedings{ ISI:000426449100006,
Author = {Kubler, Sylvain and Derigent, William and Voisin, Alexandre and Robert,
   Jeremy and Le Traon, Yves},
Book-Group-Author = {{IEEE}},
Title = {{Knowledge-based Consistency Index for Fuzzy Pairwise Comparison Matrices}},
Booktitle = {{2017 IEEE INTERNATIONAL CONFERENCE ON FUZZY SYSTEMS (FUZZ-IEEE)}},
Series = {{IEEE International Conference on Fuzzy Systems}},
Year = {{2017}},
Note = {{IEEE International Conference on Fuzzy Systems (FUZZ-IEEE), Naples,
   ITALY, JUL 09-12, 2017}},
Organization = {{IEEE; IEEE Computat Intelligence Soc; European Soc Fuzzy Log \& Technol;
   BT; Univ Degli Studi Napoli Frederico II; Univ Degli Studi Napoli
   Frederico II, Fisica Ettore Pancini}},
Abstract = {{Fuzzy AHP is today one of the most used Multiple Criteria
   Decision-Making (MCDM) techniques. The main argument to introduce fuzzy
   set theory within AHP lies in its ability to handle uncertainty and
   vagueness arising from decision makers (when performing pairwise
   comparisons between a set of criteria/alternatives). As humans usually
   reason with granular information rather than precise one, such pairwise
   comparisons may contain some degree of inconsistency that needs to be
   properly tackled to guarantee the relevance of the result/ranking. Over
   the last decades, several consistency indexes designed for fuzzy
   pairwise comparison matrices (FPCMs) were proposed, as will be discussed
   in this article. However, for some decision theory specialists, it
   appears that most of these indexes fail to be properly
   ``axiomatically{''} founded, thus leading to misleading results. To
   overcome this, a new index, referred to as KCI (Knowledge-based
   Consistency Index) is introduced in this paper, and later compared with
   an existing index that is axiomatically well founded. The comparison
   results show that (i) both indexes perform similarly from a consistency
   measurement perspective, but (ii) KCI contributes to significantly
   reduce the computation time, which can save expert's time in some MCDM
   problems.}},
