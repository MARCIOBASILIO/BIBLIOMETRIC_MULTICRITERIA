@article{ ISI:000405300400003,
Author = {Shin, Minchul and Mo, Jeonghoon and Lee, Kyungsik and Lee, Cheol},
Title = {{ON THE INTERVAL SELECTION AND INTERPRETATION OF THE PROBABILISTIC AHP
   FOR UNCERTAIN DECISION MAKING}},
Journal = {{INTERNATIONAL JOURNAL OF INDUSTRIAL ENGINEERING-THEORY APPLICATIONS AND
   PRACTICE}},
Year = {{2017}},
Volume = {{24}},
Number = {{1}},
Pages = {{32-43}},
Abstract = {{The Analytic Hierarchy Process (AHP) method, which is popularly used in
   multi-criteria decision making, can have a sensitivity issue when
   multiple participants have different opinions on the importance of
   criteria. The ranking of alternatives can be reversed with small changes
   in the weight of a criterion. We propose a new ranging method that can
   be used with probabilistic AHP to solve the sensitivity problem of the
   AHP. The equivalence concept is introduced to interpret the
   probabilistic output better. The usefulness of the proposed method is
   demonstrated through simulation.}},
