@inproceedings{ ISI:000424741600063,
Author = {Paul, U. and Falowo, O. E.},
Editor = {{Cornish, DR}},
Title = {{Efficient RAT-Selection for Group Calls using Intuitionistic Fuzzy
   TOPSIS in Heterogeneous Wireless Networks}},
Booktitle = {{2017 IEEE AFRICON}},
Series = {{Africon}},
Year = {{2017}},
Pages = {{365-370}},
Note = {{IEEE AFRICON Conference - Science, Technology and Innovation for Africa,
   Cape Town, SOUTH AFRICA, SEP 18-20, 2017}},
Organization = {{IEEE; mlab; IEEE Reg 8; IEEE S Africa Sect; IES; Univ Pretoria; SAiEE;
   IBM; Altair; CST}},
Abstract = {{The imminent deployment of fifth generation (5G) mobile networks
   promises introduction of new radio access technologies (RATs) which
   would co-exist with the current RATs. As such, RAT selection for
   subscribers running different applications becomes a crucial issue.
   Users with various requirements need to be connected to the best network
   in order to support their session activities. This paper proposes a RAT
   selection scheme by taking into account the extremely unpredictable and
   fuzzy nature of the heterogeneous wireless environment. It provides a
   new perspective for network selection by employing integrated group
   multiple criteria decision making methodology for group calls for
   different types of subscribers. In order to capture the dynamic and
   highly ambiguous nature of the heterogeneous wireless environment, we
   use Intuitionistic Fuzzy TOPSIS to develop a framework that selects the
   best network for different users running multiple applications. In
   addition to application requirements, we also consider a dynamic network
   parameter in form of network cost in the process of selecting the best
   network. Numerical results presented demonstrate the proposed scheme's
   ability to select the best RAT in several scenarios and its ability to
   overcome ranking abnormality that affects existing schemes in the field.}},
