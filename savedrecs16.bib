@article{ ISI:000398770700005,
Author = {Park, Jungkyu and Heo, Eunnyeong and Lee, Dongjun},
Title = {{Effective R\&D investment planning based on technology spillovers: the
   case of Korea}},
Journal = {{SCIENTOMETRICS}},
