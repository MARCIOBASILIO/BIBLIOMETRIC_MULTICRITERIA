Title = {{Problems in reconstruction projects, BIM uses and decision-making:
   Lithuanian case studies}},
Booktitle = {{INNOVATIVE SOLUTIONS IN CONSTRUCTION ENGINEERING AND MANAGEMENT:
   FLEXIBILITY IN SUSTAINABLE CONSTRUCTION}},
Series = {{Procedia Engineering}},
Year = {{2017}},
