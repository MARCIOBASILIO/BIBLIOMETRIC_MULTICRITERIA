@article{ ISI:000398343800001,
Author = {Peteiro-Barral, Diego and Remeseiro, Beatriz and Mendez, Rebeca and
   Penedo, Manuel G.},
Title = {{Evaluation of an automatic dry eye test using MCDM methods and rank
   correlation}},
