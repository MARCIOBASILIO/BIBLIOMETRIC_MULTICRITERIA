@article{ ISI:000401019200006,
Author = {Sanchez-Zamora, P. and Gallardo-Cobos, R. and Cena-Delgado, F.},
Title = {{Analyzing resilience factors in rural territories of Andalusia through
   techniques of Analytical Network Process (ANP)}},
Journal = {{ITEA-INFORMACION TECNICA ECONOMICA AGRARIA}},
Year = {{2017}},
Volume = {{113}},
Number = {{1}},
Pages = {{68-89}},
Month = {{MAR}},
Abstract = {{The economic crisis is one of the main elements that have determined the
   existence of different territorial dynamics in the Andalusian rural
   areas. To understand the current processes of change in these rural
   areas, it is appropriate to use the conceptual framework of territorial
   resilience. This paper analyzes the capacity of influence that factors
   associated with territorial dynamics have on the Andalusian rural
   counties resilience. To this end, a methodology for assessing these
   factors through multicriteria decision techniques-Analytical Network
   Process (ANP)-based on the opinions exposed by experts. The results show
   that factors such as Training and Access to ICT, Aid for rural
   development, Youth farmers and agricultural dynamism, and Agriculture
   and CAP aids contribute to territorial resilience. These could be taken
   into account in the public policies design allowing rural territories to
   better respond to economic pressure.}},
