@article{ ISI:000390417200012,
Author = {Rezaei-Malek, Mohammad and Razmi, Jafar and Tavakkoli-Moghaddam, Reza
   and Taheri-Moghaddam, Alireza},
Title = {{Towards a psychologically consistent cellular manufacturing system}},
Journal = {{INTERNATIONAL JOURNAL OF PRODUCTION RESEARCH}},
