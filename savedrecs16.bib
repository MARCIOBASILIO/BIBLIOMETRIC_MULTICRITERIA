@article{ ISI:000398087400030,
Author = {Chang, S. -C. and Tsai, P. -H.},
Title = {{Evaluating the Competitive Strategy of Tablet PC Industry by Using Fuzzy
   Group Decision Making Techniques}},
Journal = {{JOURNAL OF TESTING AND EVALUATION}},
