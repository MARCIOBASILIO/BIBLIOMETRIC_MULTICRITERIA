@article{ ISI:000399510900013,
Author = {Buyukozkan, Gulcin and Guleryuz, Sezin},
Title = {{Evaluation of Renewable Energy Resources in Turkey using an integrated
   MCDM approach with linguistic interval fuzzy preference relations}},
