@inproceedings{ ISI:000427148000003,
Author = {Baslyman, Malak and Amyot, Daniel},
Book-Group-Author = {{IEEE}},
Title = {{A Distance-based GRL Approach to Goal Model Refinement and Alternative
   Selection}},
Booktitle = {{2017 IEEE 25TH INTERNATIONAL REQUIREMENTS ENGINEERING CONFERENCE
