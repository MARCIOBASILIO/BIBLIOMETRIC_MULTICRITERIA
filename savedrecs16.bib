@article{ ISI:000396959700010,
Author = {Boujelben, Mohamed Ayman},
Title = {{A unicriterion analysis based on the PROMETHEE principles for
   multicriteria ordered clustering}},
Journal = {{OMEGA-INTERNATIONAL JOURNAL OF MANAGEMENT SCIENCE}},
Year = {{2017}},
Volume = {{69}},
Pages = {{126-140}},
Month = {{JUN}},
Abstract = {{We consider multicriteria clustering problems where the groups are
   defined by a total order. We assume that the pairwise comparisons
   between the alternatives are expressed by a valued preference model
   built by PROMETHEE. In order to analyze the characteristics of each
   cluster on the different criteria, we propose three concepts based on
   the PROMETHEE principles: the preference profile of a cluster, the
   similarity profile of a cluster and the inconsistency profile of a
   cluster. These notions are illustrated based on a real application on
   the supplier segmentation. (C) 2016 Elsevier Ltd. All rights reserved.}},
