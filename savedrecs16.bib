@article{ ISI:000413300600011,
Author = {Ramachandra, T. V. and Setturu, Bharath and Rajan, K. S. and Chandran,
   M. D. Subash},
Title = {{Modelling the forest transition in Central Western Ghats, India}},
Journal = {{SPATIAL INFORMATION RESEARCH}},
