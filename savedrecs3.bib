@article{ ISI:000561973100002,
Author = {Neeraj and Goraya, Major Singh and Singh, Damanpreet},
Title = {{A comparative analysis of prominently used MCDM methods in cloud
   environment}},
Journal = {{JOURNAL OF SUPERCOMPUTING}},
