@article{ ISI:000570336500001,
Author = {dos Santos Martins, Andrea Camila and Roberto Balbo, Antonio and Jones,
   Dylan and Nepomuceno, Leonardo and Martins Soler, Edilaine and Cassia
   Baptista, Edmea},
Title = {{A Hybrid Multi-Criteria Methodology for Solving the Sustainable Dispatch
   Problem}},
Journal = {{SUSTAINABILITY}},
Year = {{2020}},
Volume = {{12}},
