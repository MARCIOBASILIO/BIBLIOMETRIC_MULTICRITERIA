Author = {Bhowmik, Chiranjib and Kaviani, Mohamad Amin and Ray, Amitava and
   Ocampo, Lanndon},
Title = {{An Integrated Entropy-TOPSIS Methodology for Evaluating Green Energy
   Sources}},
