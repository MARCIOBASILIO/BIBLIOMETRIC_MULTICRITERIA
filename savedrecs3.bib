@article{ ISI:000548493400002,
Author = {Asadpourian, Zeinab and Rahimian, Mehdi and Gholamrezai, Saeed},
Title = {{SWOT-AHP-TOWS Analysis for Sustainable Ecotourism Development in the
   Best Area in Lorestan Province, Iran}},
