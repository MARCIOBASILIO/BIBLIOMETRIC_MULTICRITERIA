@article{ ISI:000547357800004,
Author = {Afshar, Mohammad Reza and Shahhosseini, Vahid and Sebt, Mohammad Hassan},
Title = {{An interval type-2 fuzzy MCDM model for work package subcontractor
   prequalification}},
