@article{ ISI:000576133700002,
Author = {Cheng, Minjiao and Wei, Wenchao},
Title = {{An AHP-DEA Approach of the Bike-Sharing Spots Selection Problem in the
   Free-Floating Bike-Sharing System}},
Journal = {{DISCRETE DYNAMICS IN NATURE AND SOCIETY}},
