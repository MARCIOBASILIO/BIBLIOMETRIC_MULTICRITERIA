@article{ ISI:000555012300001,
Author = {Lee, Zon-Yau and Chu, Mei-Tai and Wang, Yu-Ting and Chen, Kuan-Ju},
Title = {{Industry Performance Appraisal Using Improved MCDM for Next Generation
   of Taiwan}},
