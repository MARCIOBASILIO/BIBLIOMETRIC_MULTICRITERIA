@article{ ISI:000558993500001,
Author = {Din, Muhammad Salah Ud and Rehman, Muhammad Atif Ur and Ullah, Rehmat
   and Park, Chan-Won and Kim, Byung Seo},
Title = {{Towards Network Lifetime Enhancement of Resource Constrained IoT Devices
   in Heterogeneous Wireless Sensor Networks}},
Journal = {{SENSORS}},
Year = {{2020}},
