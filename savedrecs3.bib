@article{ ISI:000568500800002,
Author = {Akram, Muhammad and Peng, Xindong and Sattar, Aqsa},
Title = {{Multi-criteria Decision-Making Model Using Complex Pythagorean Fuzzy
   Yager Aggregation Operators}},
Journal = {{ARABIAN JOURNAL FOR SCIENCE AND ENGINEERING}},
Year = {{2021}},
Volume = {{46}},
Number = {{2}},
Pages = {{1691-1717}},
Month = {{FEB}},
Abstract = {{Aggregation operators are indispensable tool to consolidate the sundry
   inputs into a single worthwhile output and widely used in distinct
   models of classical and fuzzy set theory to aggregate the data. Complex
   Pythagorean fuzzy model has extrapolated the existing models due to its
   trait to model the uncertainty of human judgment in a more precise way.
   In this research article, we investigate some aggregation operators
   based on Yager t-norm and s-norm, namely complex Pythagorean fuzzy Yager
   weighted averaging operator, complex Pythagorean fuzzy Yager ordered
   weighted averaging operator, complex Pythagorean fuzzy Yager weighted
   geometric operator and complex Pythagorean fuzzy Yager ordered weighted
   geometric operator. Further, we discuss some remarkable properties of
   proposed operators. Moreover, we promote the advancement of
   multi-criteria decision-making strategies by proposing an algorithm in
   CPF environment. Further, to demonstrate the significance of proposed
   operators in decision-making environment, we provide fully developed
   numerical example which involves selection of best alternative among the
   existing alternative. Finally, we check the effectiveness of proposed
   operators by validity test.}},
