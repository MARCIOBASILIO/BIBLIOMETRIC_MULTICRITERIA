@article{ ISI:000536312200009,
Author = {Dai, Wen-feng and Zhong, Qiu-yan and Qi, Chun-ze},
Title = {{Multi-stage multi-attribute decision-making method based on the prospect
   theory and triangular fuzzy MULTIMOORA}},
Journal = {{SOFT COMPUTING}},
Year = {{2020}},
Volume = {{24}},
Number = {{13}},
Pages = {{9429-9440}},
Month = {{JUL}},
Abstract = {{This paper proposed a novel multi-stage multi-attribute decision-making
   method, in which the period weights and attribute weights are completely
   unknown, and attribute values are triangular fuzzy numbers. At first,
   the triangular fuzzy prospect decision matrices of different periods are
   constructed and the period weights optimization model is built according
   to the time degree and the prospect value deviation of alternatives in
   different periods. The attribute weights are obtained by the maximum
   deviation method. Then, MULTIMOORA is extended into the triangular fuzzy
   environment. Based on the new form of MULTIMOORA and dominance theory,
   the final ranking of alternatives can be got. Finally, a numerical
   example is presented to illustrate the feasibility and effectiveness of
   the proposed method.}},
