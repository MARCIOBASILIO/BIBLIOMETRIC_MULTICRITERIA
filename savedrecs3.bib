@article{ ISI:000564909700001,
Author = {Riaz, Muhammad and Hashmi, Masooma Raza and Kalsoom, Humaira and
   Pamucar, Dragan and Chu, Yu-Ming},
Title = {{Linear Diophantine Fuzzy Soft Rough Sets for the Selection of
   Sustainable Material Handling Equipment}},
