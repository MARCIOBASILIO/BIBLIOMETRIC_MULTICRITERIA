@article{ ISI:000572977900008,
Author = {Agarwal, Sanchit Saran and Kansal, Mitthan Lal},
Title = {{Risk based initial cost assessment while planning a hydropower project}},
Journal = {{ENERGY STRATEGY REVIEWS}},
Year = {{2020}},
Volume = {{31}},
Month = {{SEP}},
Abstract = {{In the absence of perceived risks at planning stage, initial cost
   underestimation or overestimation is commonly existent in hydropower
   projects. This study suggests an expert fuzzy-TOPSIS multiple criteria
   decision-making (MCDM) methodology for the initial cost estimation of a
   hydropower project. Risk factors along with their likelihood and impact
   are analyzed in pre-feasibility stage to assess the cost intervals per
   MW capacity. Project developers can use this philosophy to screen
   projects and identify critical areas for detailed investigations. To
   demonstrate the methodology's effectiveness, a case study of 126 MW
   Myntdu Leshka hydropower project in Meghalaya, India, is adopted. This
   study contributes to the limited research available concerning the
   analysis of risk factors for the cost assessment of a hydropower
   project.}},
