@article{ ISI:000579427700001,
Author = {Campos, Priscila Celebrini de Oliveira and Paz, Taina da Silva Rocha and
   Lenz, Leticia and Qiu, Yangzi and Alves, Camila Nascimento and Simoni,
   Ana Paula Roem and Amorim, Jose Carlos Cesar and Lima, Gilson Brito
