@article{ ISI:000564590900001,
Author = {Aliyev, Rashad and Temizkan, Hasan and Aliyev, Rafig},
Title = {{Fuzzy Analytic Hierarchy Process-Based Multi-Criteria Decision Making
   for Universities Ranking}},
