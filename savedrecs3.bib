@article{ ISI:000559632600001,
Author = {Mahmood, Tahir and Ali, Zeeshan},
Title = {{Entropy measure and TOPSIS method based on correlation coefficient using
   complex q-rung orthopair fuzzy information and its application to
   multi-attribute decision making}},
Journal = {{SOFT COMPUTING}},
