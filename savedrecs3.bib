@article{ ISI:000527276900026,
Author = {Arandarenko, Mihail and Corrente, Salvatore and Jandric, Maja and
   Stamenkovic, Mladen},
Title = {{Multiple criteria decision aiding as a prediction tool for migration
   potential of regions}},
