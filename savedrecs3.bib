@article{ ISI:000564546700001,
Author = {Gong, Jia-Wei and Li, Qiang and Yin, Linsen and Liu, Hu-Chen},
Title = {{Undergraduate teaching audit and evaluation using an extended MABAC
   method underq-rung orthopair fuzzy environment}},
Journal = {{INTERNATIONAL JOURNAL OF INTELLIGENT SYSTEMS}},
