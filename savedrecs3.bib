@article{ ISI:000559514900001,
Author = {Ho, Juin-Hao and Lee, Gwo-Guang and Lu, Ming-Tsang},
Title = {{Exploring the Implementation of a Legal AI Bot for Sustainable
   Development in Legal Advisory Institutions}},
