@article{ ISI:000548486300003,
Author = {Delice, Elif Kilic and Can, Gulin Feryal},
Title = {{A new approach for ergonomic risk assessment integrating KEMIRA,
   best-worst and MCDM methods}},
Journal = {{SOFT COMPUTING}},
