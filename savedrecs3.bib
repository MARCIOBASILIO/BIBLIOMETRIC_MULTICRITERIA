@article{ ISI:000554305300001,
Author = {Kechagias, Evripidis P. and Gayialis, Sotiris P. and Konstantakopoulos,
   Grigorios D. and Papadopoulos, Georgios A.},
Title = {{An Application of a Multi-Criteria Approach for the Development of a
   Process Reference Model for Supply Chain Operations}},
