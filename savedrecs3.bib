@article{ ISI:000600423000039,
Author = {Sarkar, Showmitra Kumar},
Title = {{COVID-19 Susceptibility Mapping Using Multicriteria Evaluation}},
Journal = {{DISASTER MEDICINE AND PUBLIC HEALTH PREPAREDNESS}},
