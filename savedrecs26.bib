@article{ ISI:000291407800012,
Author = {Vandani, Behnam and Mousavi, S. Meysam and Tavakkoli-Moghaddam, Reza},
Title = {{Group decision making based on novel fuzzy modified TOPSIS method}},
Journal = {{APPLIED MATHEMATICAL MODELLING}},
Year = {{2011}},
Volume = {{35}},
Number = {{9}},
Pages = {{4257-4269}},
Month = {{SEP}},
Abstract = {{The aim of this paper is to present a novel fuzzy modified technique of
   order preference by a similarity to ideal solution (TOPSIS) method by a
   group of experts, which can select the best alternative by considering
   both conflicting quantitative and qualitative evaluation criteria in
   real-life applications. The proposed method satisfies the condition of
   being the closest to the fuzzy positive ideal solution and also being
   the farthest from the fuzzy negative ideal solution with multi-judges
   and multi-criteria. The performance rating values of alternatives versus
   conflicting criteria as well as the weights of criteria are described by
   linguistic variables and are transformed into triangular fuzzy numbers.
   Then a new collective index is introduced to discriminate among
   alternatives in the evaluation process with respect to subjective
   judgment and objective information. This paper shows that the proposed
   fuzzy modified TOPSIS method is a suitable decision making tool for the
