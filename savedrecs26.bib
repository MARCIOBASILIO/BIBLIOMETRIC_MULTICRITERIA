Affiliation = {{Yang, ZL (Corresponding Author), Liverpool John Moores Univ, Sch Engn, Liverpool L3 3AF, Merseyside, England.
   Yang, Z. L.; Bonsall, S.; Wang, J., Liverpool John Moores Univ, Sch Engn, Liverpool L3 3AF, Merseyside, England.}},
DOI = {{10.1016/j.eswa.2011.05.032}},
