Author = {Zakria, Ghulam and Guan, Zailin and Shao, Xinyu and Riaz, Yasser and
   Hameed, Uzma},
Title = {{Selection of Simulation Software for Manufacturing System: Application
   of Analytical Hierarchy Approach in Multi Criteria Decision Making}},
