@article{ ISI:000290237500069,
Author = {Kilincci, Ozcan and Onal, Suzan Ash},
Title = {{Fuzzy AHP approach for supplier selection in a washing machine company}},
Journal = {{EXPERT SYSTEMS WITH APPLICATIONS}},
