@article{ ISI:000297693000009,
Author = {Kang, He-Yau and Hung, Meng-Chan and Pearn, W. L. and Lee, Amy H. I. and
   Kang, Mei-Sung},
Title = {{An Integrated Multi-Criteria Decision Making Model for Evaluating Wind
   Farm Performance}},
