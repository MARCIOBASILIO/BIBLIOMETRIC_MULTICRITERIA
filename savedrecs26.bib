@article{ ISI:000287938600005,
Author = {Siddiqui, Zeeshan and Abdullah, Abdul Hanan and Khan, Muhammad Khurram
   and Alghathbar, Khaled},
Title = {{Analysis of enterprise service buses based on information security,
   interoperability and high-availability using Analytical Hierarchy
   Process (AHP) method}},
