@article{ ISI:000302509500008,
Author = {Stanujkic, Dragisa and Magdalinovic, Nedeljko and Stojanovic, Sanja and
   Jovanovic, Rodoljub},
Title = {{Extension of Ratio System Part of MOORA Method for Solving
   Decision-Making Problems with Interval Data}},
Journal = {{INFORMATICA}},
Year = {{2012}},
Volume = {{23}},
Number = {{1}},
Pages = {{141-154}},
Abstract = {{In some cases of using multi-criteria decision making methods for
   solving real-world problems ratings of alternatives cannot be determined
   precisely, and that is why they are expressed in the form of intervals.
   Therefore, the aim of this paper is to extend the MOORA method for
   solving decision making problems with interval data. By extending the
   ratio system part of MOORA method, an algorithm to determine the most
   preferable alternative among all possible alternatives, when performance
   ratings are given as intervals, is presented. Finally, an example is
   shown to highlight the proposed procedure, at the end of this paper.}},
Publisher = {{INST MATHEMATICS \& INFORMATICS}},
Address = {{AKADEMIJOS STR 4, VILNIUS LT-08663, LITHUANIA}},
Type = {{Article}},
