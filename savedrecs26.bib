@article{ ISI:000294084700094,
Author = {Ishizaka, Alessio and Labib, Ashraf},
Title = {{Review of the main developments in the analytic hierarchy process}},
Journal = {{EXPERT SYSTEMS WITH APPLICATIONS}},
Year = {{2011}},
Volume = {{38}},
Number = {{11}},
Pages = {{14336-14345}},
Month = {{OCT}},
Abstract = {{In this paper the authors review the developments of the analytic
   hierarchy process (AHP) since its inception. The focus of this paper is
   a neutral review on the methodological developments rather than
   reporting its applications that have appeared since its introduction. In
   particular, we discuss problem modelling, pair-wise comparisons,
