@article{ ISI:000286782400033,
Author = {Garfi, Marianna and Ferrer-Marti, Laia and Bonoli, Alessandra and
   Tondelli, Simona},
Title = {{Multi-criteria analysis for improving strategic environmental assessment
   of water programmes. A case study in semi-arid region of Brazil}},
Journal = {{JOURNAL OF ENVIRONMENTAL MANAGEMENT}},
