@article{ ISI:000308110200004,
Author = {Ishizaka, Alessio and Pearman, Craig and Nemery, Philippe},
Title = {{AHPSort: an AHP-based method for sorting problems}},
Journal = {{INTERNATIONAL JOURNAL OF PRODUCTION RESEARCH}},
