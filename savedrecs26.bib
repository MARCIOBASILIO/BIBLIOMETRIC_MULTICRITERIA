@article{ ISI:000298518200004,
Author = {Jin Hao and You Yuan and Ji Xiaodong and Li Yong and Wang Wenbo},
Title = {{Network Selection in Integrated Heterogeneous Wireless Network: A Novel
   MADM Based Approach}},
