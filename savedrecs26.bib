@article{ ISI:000296264000010,
Author = {Afzali, A. and Samani, J. M. V. and Rashid, M.},
Title = {{MUNICIPAL LANDFILL SITE SELECTION FOR ISFAHAN CITY BY USE OF FUZZY LOGIC
   AND ANALYTIC HIERARCHY PROCESS}},
Journal = {{IRANIAN JOURNAL OF ENVIRONMENTAL HEALTH SCIENCE \& ENGINEERING}},
