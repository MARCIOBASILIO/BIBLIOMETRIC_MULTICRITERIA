@article{ ISI:000291081800025,
Author = {Hougaard, Jens Leth and Nielsen, Kurt},
Title = {{Weighted Overlap Dominance - A procedure for interactive selection on
   multidimensional interval data}},
Journal = {{APPLIED MATHEMATICAL MODELLING}},
Year = {{2011}},
Volume = {{35}},
Number = {{8}},
Pages = {{3958-3969}},
Month = {{AUG}},
Abstract = {{We present an outranking procedure that supports selection of
   alternatives represented by multiple attributes with interval valued
   data. The procedure is interactive in the sense that the decision maker
   directs the search for preferred alternatives by providing weights of
   the different attributes as well as parameters related to risk attitude
   and weighted dominance. The outranking relation builds on pairwise
   comparisons between optimistic and pessimistic weighted values as well
   as weighted dominance relations supported by volume based measures. The
   suggested procedure is referred to as the Weighted Overlap Dominance
   procedure (WOD). (C) 2011 Elsevier Inc. All rights reserved.}},
