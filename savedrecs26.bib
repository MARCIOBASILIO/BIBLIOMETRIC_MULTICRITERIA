@article{ ISI:000312704800007,
Author = {Liou, James J. H. and Tzeng, Gwo-Hshiung},
Title = {{COMMENTS ON ``MULTIPLE CRITERIA DECISION MAKING (MCDM) METHODS IN
   ECONOMICS: AN OVERVIEW{''}}},
