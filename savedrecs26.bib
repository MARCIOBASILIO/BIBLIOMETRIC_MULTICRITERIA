@article{ ISI:000289393400016,
Author = {Amid, A. and Ghodsypour, S. H. and O'Brien, C.},
Title = {{A weighted max-min model for fuzzy multi-objective supplier selection in
   a supply chain}},
Journal = {{INTERNATIONAL JOURNAL OF PRODUCTION ECONOMICS}},
Year = {{2011}},
Volume = {{131}},
Number = {{1, SI}},
Pages = {{139-145}},
Month = {{MAY}},
Note = {{15th International Working Seminar on Production Economics, Innsbruck,
   AUSTRIA, MAR 03-07, 2008}},
Abstract = {{Supplier selection is one of the most important activities of purchasing
   departments. This importance is increased even more by new strategies in
   a supply chain. Supplier selection is a multi-criteria decision making
   problem in which criteria have different relative importance. In
   practice, for supplier selection problems, many input information are
   not known precisely. The fuzzy set theories can be employed due to the
   presence of vagueness and imprecision of information. l weighted max-min
   fuzzy model is developed to handle effectively the vagueness of input
   data an I different weights of criteria in this problem. Due to this
   model, the achievement level of objective functions matches the relative
   importance of the objective functions. In this paper, an analytic
   hierarchy process (AHP) is used to determine the weights of criteria.
   The proposed model can help the decision maker (DM) to find out the
   appropriate order to each supplier, and allows the purchasing manager(s)
   to manage supply chain performance on cost, quality and service. The
   model is explained by an illustrative example. (C) 2010 Elsevier B.V.
   All rights reserved.}},
