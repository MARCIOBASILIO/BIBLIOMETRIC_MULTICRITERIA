@article{ ISI:000437142800018,
Author = {Buyukozkan, Gulcin and Gocer, Fethullah and Feyzioglu, Orhan},
Title = {{Cloud computing technology selection based on interval-valued
   intuitionistic fuzzy MCDM methods}},
Journal = {{SOFT COMPUTING}},
Year = {{2018}},
Volume = {{22}},
Number = {{15, SI}},
Pages = {{5091-5114}},
