@article{ ISI:000441344400018,
Author = {Junian, Javad and Azizifar, Valiollah},
Title = {{The Evaluation of Temporary Shelter Areas Locations Using Geographic
   Information System and Analytic Hierarchy Process}},
Journal = {{CIVIL ENGINEERING JOURNAL-TEHRAN}},
