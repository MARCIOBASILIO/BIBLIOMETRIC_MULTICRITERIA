@article{ ISI:000431733600015,
Author = {Ren, Jingzheng and Ren, Xusheng and Dong, Lichun and Manzardo,
   Alessandro and He, Chang and Pan, Ming},
Title = {{Multiactor multicriteria decision making for life cycle sustainability
   assessment under uncertainties}},
Journal = {{AICHE JOURNAL}},
