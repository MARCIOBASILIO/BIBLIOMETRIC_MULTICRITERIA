@article{ ISI:000438775200041,
Author = {Buyukozkan, Gulcin and Gocer, Fethullah},
Title = {{An extension of ARAS methodology under Interval Valued Intuitionistic
   Fuzzy environment for Digital Supply Chain}},
Journal = {{APPLIED SOFT COMPUTING}},
Year = {{2018}},
