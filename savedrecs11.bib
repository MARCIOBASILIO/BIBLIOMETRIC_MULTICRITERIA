@article{ ISI:000444364400034,
Author = {Sangkakool, Tachaya and Techato, Kuaanan and Zaman, Rafia and
   Brudermann, Thomas},
Title = {{Prospects of green roofs in urban Thailand - A multi-criteria decision
   analysis}},
Journal = {{JOURNAL OF CLEANER PRODUCTION}},
