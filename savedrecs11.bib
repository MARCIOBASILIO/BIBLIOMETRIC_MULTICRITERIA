@article{ ISI:000437672800019,
Author = {Mousseau, Vincent and Ozpeynirci, Ozgur and Ozpeynirci, Selin},
Title = {{Inverse multiple criteria sorting problem}},
Journal = {{ANNALS OF OPERATIONS RESEARCH}},
Year = {{2018}},
