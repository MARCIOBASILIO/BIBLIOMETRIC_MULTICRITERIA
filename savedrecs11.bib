@article{ ISI:000448561000066,
Author = {Wang, Jie and Wei, Guiwu and Lu, Mao},
Title = {{TODIM Method for Multiple Attribute Group Decision Making under 2-Tuple
   Linguistic Neutrosophic Environment}},
