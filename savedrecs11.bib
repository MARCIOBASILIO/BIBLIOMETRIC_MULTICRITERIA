@article{ ISI:000432102500044,
Author = {Sierra, Leonardo A. and Yepes, Victor and Pellicer, Eugenio},
Title = {{A review of multi-criteria assessment of the social sustainability of
   infrastructures}},
Journal = {{JOURNAL OF CLEANER PRODUCTION}},
Year = {{2018}},
Volume = {{187}},
Pages = {{496-513}},
Month = {{JUN 20}},
Abstract = {{Nowadays multi-criteria methods enable non-monetary aspects to be
   incorporated into the assessment of infrastructure sustainability. Yet
   evaluation of the social aspects is still neglected and the
   multi-criteria assessment of these social aspects is still an emerging
   topic. Therefore, the aim of this article is to review the current state
   of multi-criteria infrastructure assessment studies that include social
   aspects. The review includes an analysis of the social criteria,
   participation and assessment methods. The results identify mobility and
   access, safety and local development among the most frequent criteria.
   The Analytic Hierarchy Process and Simple Additive Weighting methods are
   the most frequently used. Treatments of equity, uncertainty, learning
   and consideration of the context, however, are not properly analyzed
   yet. Anyway, the methods for implementing the evaluation must guarantee
   the social effect on the result, improvement of the representation of
   the social context and techniques to facilitate the evaluation in the
   absence of information. (C) 2018 Elsevier Ltd. All rights reserved.}},
