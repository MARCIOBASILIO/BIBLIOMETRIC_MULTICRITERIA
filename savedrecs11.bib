@article{ ISI:000449750400025,
Author = {Saplioglu, M. and Aydin, M. M.},
Title = {{Choosing safe and suitable bicycle routes to integrate cycling and
   public transport systems}},
Journal = {{JOURNAL OF TRANSPORT \& HEALTH}},
