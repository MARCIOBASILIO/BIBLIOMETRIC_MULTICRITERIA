@article{ ISI:000448418300008,
Author = {Zolfani, Sarfaraz Hashemkhani and Yazdani, Morteza and Zavadskas,
   Edmundas Kazimieras},
Title = {{An extended stepwise weight assessment ratio analysis (SWARA) method for
   improving criteria prioritization process}},
Journal = {{SOFT COMPUTING}},
Year = {{2018}},
