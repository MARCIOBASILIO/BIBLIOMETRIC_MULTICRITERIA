@article{ ISI:000448097800074,
Author = {Erbas, Mehmet and Kabak, Mehmet and Ozceylan, Eren and Cetinkaya, Cihan},
Title = {{Optimal siting of electric vehicle charging stations: A GIS-based fuzzy
   Multi-Criteria Decision Analysis}},
Journal = {{ENERGY}},
Year = {{2018}},
