@article{ ISI:000432817200003,
Author = {Chen, Liuhao and Xu, Zeshui and Wang, Hai and Liu, Shousheng},
Title = {{An ordered clustering algorithm based on K-means and the PROMETHEE
   method}},
Journal = {{INTERNATIONAL JOURNAL OF MACHINE LEARNING AND CYBERNETICS}},
