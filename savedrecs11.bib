@article{ ISI:000446157900006,
Author = {Krejci, Jana and Ishizaka, Alessio},
Title = {{FAHPSort: A Fuzzy Extension of the AHPSort Method}},
Journal = {{INTERNATIONAL JOURNAL OF INFORMATION TECHNOLOGY \& DECISION MAKING}},
