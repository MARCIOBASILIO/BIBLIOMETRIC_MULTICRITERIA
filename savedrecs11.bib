@article{ ISI:000431913000069,
Author = {Montes, Rosana and Sanchez, Ana M. and Villar, Pedro and Herrera,
   Francisco},
Title = {{Teranga Go! : Carpooling Collaborative Consumption Community with
   multi-criteria hesitant fuzzy linguistic term set opinions to build
   confidence and trust}},
Journal = {{APPLIED SOFT COMPUTING}},
Year = {{2018}},
Volume = {{67}},
Pages = {{941-952}},
Month = {{JUN}},
Note = {{IEEE International Conference on Fuzzy Systems (FUZZ-IEEE) held as part
   of IEEE World Congress on Computational Intelligence (IEEE WCCI),
   Vancouver, CANADA, JUL 24-29, 2016}},
Organization = {{IEEE; IEEE Computat Intelligence Soc; Int Neural Network Soc;
   Evolutionary Programming Soc; IET; Gulf Univ Sci \& Technol; IEEE Big
   Data Initiat}},
Abstract = {{Boosting collaborative or participatory consumption is a priority for
   the European Commission. It is in line with the provisions of the Europe
   2020 Strategy, which proposes that consumption of goods and services
   should take place in accordance with smart, sustainable and inclusive
   growth. These have motivated us to develop an online community for
   collaborative consumption centered in the Senegalese community that
   travels by car from Europe to Africa named Teranga Go!. Carpooling
   relationships are based on the sense of a real existing community,
   social experiences among users, and connection through technology, where
   confidence is the key concept. To help creating values of confidence,
   trust and safety among the members of the Teranga Go! community, we have
   implemented an intelligent decision support system in the platform based
   on computing with words. The participants of a carpooling experience act
   as experts that assess the driver aptitudes and determine, together with
   the history of the driver, a linguistic value for the driver's karma
   which represents the collective opinion of people that have traveled
   with the driver. The karma is a public label attached to the site user
   profiles. A Multi-Expert Multi-Criteria Decision Making model is applied
   using Hesitant Fuzzy Linguistic Terms to represent the expert opinions.
   (C) 2017 Elsevier B.V. All rights reserved.}},
