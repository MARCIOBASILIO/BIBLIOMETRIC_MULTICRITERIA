@article{ ISI:000446675400008,
Author = {Liao, Huchang and Mi, Xiaomei and Xu, Zeshui and Xu, Jiuping and
   Herrera, Francisco},
Title = {{Intuitionistic Fuzzy Analytic Network Process}},
Journal = {{IEEE TRANSACTIONS ON FUZZY SYSTEMS}},
Year = {{2018}},
