@article{ ISI:000436897000005,
Author = {Xiao, Yangfan and Yi, Shanzhen and Tang, Zhongqian},
Title = {{A Spatially Explicit Multi-Criteria Analysis Method on Solving Spatial
   Heterogeneity Problems for Flood Hazard Assessment}},
Journal = {{WATER RESOURCES MANAGEMENT}},
