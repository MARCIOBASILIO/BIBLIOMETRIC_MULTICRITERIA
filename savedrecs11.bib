
@article{ ISI:000451975800001,
Author = {Pamucar, Dragan S. and Tarle, Snezana Pejcic and Parezanovic, Tanja},
Title = {{New hybrid multi-criteria decision-making DEMATEL-MAIRCA model:
   sustainable selection of a location for the development of multimodal
   logistics centre}},
Journal = {{ECONOMIC RESEARCH-EKONOMSKA ISTRAZIVANJA}},
Year = {{2018}},
Volume = {{31}},
