@article{ ISI:000441971300002,
Author = {Zarghami, Esmaeil and Sharghi, Ali and Olfat, Milad and Kousalari,
   Farzaneh Salehi},
Title = {{Using Multi-Criteria Decision-Making Method (MCDM) to Study Quality of
   Life Variables in the Design of Senior Residences in Iran}},
Journal = {{AGEING INTERNATIONAL}},
Year = {{2018}},
Volume = {{43}},
Number = {{3}},
Pages = {{279-296}},
Month = {{SEP}},
Abstract = {{This study conducted a meta-analysis on the related sources published in
   the last 15 years to obtain indicators relevant to residential space of
   elderly people effective on their quality of life. Using multi-criteria
   decision-making method (MCDM) and analytic hierarchy process (AHP) by 35
   experts, it evaluated the sub-components obtained from studies and the
   scoring of individual and collective residences for senior citizens by
   Expert Choice (EC) software. The results show that, among physical,
   perceptual, functional-skeletal and sociocultural factors, physical and
   sociocultural factors had the most and least significance, respectively.
   The results also indicated that the physical followed by perceptual
   factors have more weight and more impact in determining a residence that
   matches the quality of life of the elderly. Moreover, collective
   residences, as compared to individual ones, have a higher relative
   weight to be selected as the accommodation for elderly people.}},
