@article{ ISI:000439358200005,
Author = {Lazzerini, Beatrice and Pistolesi, Francesco},
Title = {{Multiobjective Personnel Assignment Exploiting Workers' Sensitivity to
   Risk}},
Journal = {{IEEE TRANSACTIONS ON SYSTEMS MAN CYBERNETICS-SYSTEMS}},
Year = {{2018}},
Volume = {{48}},
