@article{ ISI:000437142800003,
Author = {Topaloglu, Murside and Yarkin, Ferhat and Kaya, Tolga},
Title = {{Solid waste collection system selection for smart cities based on a
   type-2 fuzzy multi-criteria decision technique}},
Journal = {{SOFT COMPUTING}},
Year = {{2018}},
Volume = {{22}},
Number = {{15, SI}},
Pages = {{4879-4890}},
Month = {{AUG}},
