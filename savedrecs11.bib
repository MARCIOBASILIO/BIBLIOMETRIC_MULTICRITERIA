@article{ ISI:000446157900002,
Author = {De Souza, Leandro Pecanha and Simoes Gomes, Carlos Francisco and De
   Barros, Alexandre Pinheiro},
Title = {{Implementation of New Hybrid AHP-TOPSIS-2N Method in Sorting and
   Prioritizing 6 of an it CAPEX Project Portfolio}},
Journal = {{INTERNATIONAL JOURNAL OF INFORMATION TECHNOLOGY \& DECISION MAKING}},
Year = {{2018}},
Volume = {{17}},
