@article{ ISI:000284654800004,
Author = {El-Gayar, Omar F. and Fritz, Brian D.},
Title = {{A web-based multi-perspective decision support system for information
   security planning}},
Journal = {{DECISION SUPPORT SYSTEMS}},
Year = {{2010}},
Volume = {{50}},
Number = {{1}},
Pages = {{43-54}},
Month = {{DEC}},
Abstract = {{With the increasing exposure and vulnerability to cyber attacks, it
   becomes necessary to develop methodologies and systems that are capable
   of dealing with the complex and multifaceted nature of decision
   situations encountered in security planning and management. In this
   paper we present the theoretical basis, architecture and design of a
   web-based multi-perspective decision support system (DSS) and an
   underlying decision multi-criteria decision framework that is consistent
   with security and decision theory. The system is illustrated through a
   multi-stakeholder scenario that captures the complexity encountered in a
   multi-criteria security control selection decision problem. (C) 2010
   Elsevier B.V. All rights reserved.}},
