@article{ ISI:000279464900027,
Author = {Banias, Georgios and Achillas, Charisios and Vlachokostas, Christos and
   Moussiopoulos, Nicolas and Tarsenis, Sokratis},
Title = {{Assessing multiple criteria for the optimal location of a construction
   and demolition waste management facility}},
Journal = {{BUILDING AND ENVIRONMENT}},
