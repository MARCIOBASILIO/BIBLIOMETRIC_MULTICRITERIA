@article{ ISI:000271165700016,
Author = {El Hanandeh, Ali and El-Zein, Abbas},
Title = {{The development and application of multi-criteria decision-making tool
   with consideration of uncertainty: The selection of a management
   strategy for the bio-degradable fraction in the municipal solid waste}},
Journal = {{BIORESOURCE TECHNOLOGY}},
Year = {{2010}},
Volume = {{101}},
Number = {{2}},
Pages = {{555-561}},
Month = {{JAN}},
Abstract = {{A modified version of the multi-criteria decision aid, ELECTRE III has
   been developed to account for uncertainty in criteria weightings and
   threshold Values. The new procedure, called ELECTRE-SS, modifies the
   exploitation phase in ELECTRE III, through a new definition of the
   pre-order and the introduction of a ranking index (RI). The new approach
   accommodates cases where incomplete or uncertain preference data are
   present. The method is applied to a case of selecting a management
   strategy for the bio-degradable fraction in the municipal solid waste of
   Sydney. Ten alternatives are compared against 11 criteria. The results
   show that anaerobic digestion (AD) and composting of paper are less
   environmentally sound options than recycling. AD is likely to
   out-perform incineration where a market for heating does not exist.
   Moreover, landfilling can be a Sound alternative, when considering
   overall performance and conditions of uncertainty. (c) 2009 Elsevier
   Ltd. All rights reserved.}},
