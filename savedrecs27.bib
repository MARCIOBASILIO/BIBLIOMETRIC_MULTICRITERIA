@article{ ISI:000281339900134,
Author = {Hu, Fei-hu and Jiang, Jie and Liu, Le and Sun, Lin-yan and Ji,
   Ying-cheng},
Title = {{A new multi-perspective framework for multi-attribute decision making}},
Journal = {{EXPERT SYSTEMS WITH APPLICATIONS}},
Year = {{2010}},
Volume = {{37}},
Number = {{12}},
Pages = {{8575-8582}},
Month = {{DEC}},
Abstract = {{This paper proposes a new multi-perspective framework for
   multi-attribute decision making, and illustrates its mathematical
   descriptions and procedure. The main steps of the procedure are:
   construct the attribute set; construct the evaluation object set;
   construct evaluation perspectives with corresponding attributes and
   weights: select evaluation algorithms for different perspectives: and
   carry out cluster analysis on the multidimensional evaluation results.
   This framework can effectively solve the problem of attribute
   overlapping of different perspectives and proper algorithms can be
   applied to different perspectives according to their characteristics.
   Based on the business data of a tobacco company, a customer evaluation
   system is designed according to this framework. Results show that the
   framework is effective and practical. (C) 2010 Elsevier Ltd. All rights
   reserved.}},
