@article{ ISI:000281591100008,
Author = {Azadeh, Amir and Osanloo, M. and Ataei, M.},
Title = {{A new approach to mining method selection based on modifying the
   Nicholas technique}},
Journal = {{APPLIED SOFT COMPUTING}},
