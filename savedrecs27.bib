@article{ ISI:000298514700018,
Author = {Zavadskas, E. K. and Turskis, Z. and Tamosaitiene, J.},
Title = {{Selection of construction enterprises management strategy based on the
   SWOT and multi-criteria analysis}},
Journal = {{ARCHIVES OF CIVIL AND MECHANICAL ENGINEERING}},
