@article{ ISI:000296416400024,
Author = {Hosseini, Mirza Hasan and Karimzadegan, Davood and Sazvar, Azam},
Title = {{Identification of MIS Strategies Barriers in Higher Education
   Institutions through MCDM Approach (Case Study: Ferdowsi University of
   Mashhad)}},
