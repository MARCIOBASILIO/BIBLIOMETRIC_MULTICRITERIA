@inproceedings{ ISI:000315976500029,
Author = {Demir, Sara and Esbah, Hayriye and Akgun, Aliye Ahu},
Editor = {{Ozhan, E}},
Title = {{Multi Criteria Decision Making for Ecotourism}},
