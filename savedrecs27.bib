@article{ ISI:000280242500019,
Author = {Calizaya, Andres and Meixner, Oliver and Bengtsson, Lars and Berndtsson,
   Ronny},
Title = {{Multi-criteria Decision Analysis (MCDA) for Integrated Water Resources
   Management (IWRM) in the Lake Poopo Basin, Bolivia}},
Journal = {{WATER RESOURCES MANAGEMENT}},
