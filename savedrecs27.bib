@article{ ISI:000276860800012,
Author = {Chen, Hsing Hung and Lee, Amy H. I. and Kang, He-Yau},
Title = {{A model for strategic selection of feeder management systems: A case
   study}},
Journal = {{INTERNATIONAL JOURNAL OF ELECTRICAL POWER \& ENERGY SYSTEMS}},
Year = {{2010}},
Volume = {{32}},
Number = {{5}},
Pages = {{421-427}},
Month = {{JUN}},
Abstract = {{The move to integrating distribution management system (DMS) and feeder
   management system (FMS) in China is becoming the main trend in recent
   years, in addition to upgrading and rebuilding existing energy
   management system (EMS) and DMS. However, with increasing complexity in
   the social environments along with rapidly changing technologies, how to
   select a suitable contractor and a FMS project is becoming an important
