@inproceedings{ ISI:000302389400038,
Author = {Pedro, Luciana R. and Takahashi, Ricardo H. C.},
Editor = {{Takahashi, RHC and Deb, K and Wanner, EF and Greco, S}},
Title = {{Modeling Decision-Maker Preferences through Utility Function Level Sets}},
