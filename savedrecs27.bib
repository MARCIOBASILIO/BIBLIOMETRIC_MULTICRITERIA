@article{ ISI:000276532600074,
Author = {Ye, Jun},
Title = {{Using an improved measure function of vague sets for multicriteria fuzzy
   decision-making}},
Journal = {{EXPERT SYSTEMS WITH APPLICATIONS}},
Year = {{2010}},
Volume = {{37}},
Number = {{6}},
Pages = {{4706-4709}},
Month = {{JUN}},
Abstract = {{An improved accuracy function for vague sets is proposed by taking into
   account the effect of an unknown degree (hesitancy degree) of the vague
   values on the degree of suitability to which each alternative satisfies
   the decision-maker's requirement. The accuracy function is more
   reasonable than the existing accuracy functions that are not reasonable
   in some cases. Moreover, the improved decision-making method can improve
   existing techniques effectively and the process of decision-making needs
   not to be divided into several steps and needs not adding other rules
   and choosing parameters when we use the improved decision-making method.
   The improved method can be used to rank the decision alternative
   according to the decision criteria. Illustrative examples are given to
   verify the developed approach and to demonstrate its practicality and
   effectiveness. (C) 2009 Elsevier Ltd. All rights reserved.}},
