@inproceedings{ ISI:000299096403102,
Author = {Zhu, Qingjie and Chen, Jing},
Editor = {{Zhou, X}},
Title = {{RETRACTED: Multi-Objective Decision Making in GIS-based Multi-Criteria
   Evaluation for Land Use (Retracted Article)}},
Booktitle = {{2011 INTERNATIONAL CONFERENCE ON ENERGY AND ENVIRONMENTAL SCIENCE-ICEES
   2011}},
Series = {{Energy Procedia}},
Year = {{2011}},
Volume = {{11}},
Pages = {{3634-3640}},
Note = {{International Conference on Energy and Environmental Science (ICEES),
   Singapore, SINGAPORE, OCT, 2011}},
Organization = {{Singapore Inst Elect (SIE)}},
Abstract = {{Multi-objective decision making is common in environmental management,
   but it is not yet further developed within GIS. A compromise solution to
   the multi-objective problems under conditions of conflicting objectives
   with GIS-based Ordered Weighted Averaging (OWA) method is investigated.
   GIS-based OWA is a relatively new Multi-Criteria Evaluation (MCE)
   method, in which both criteria weights and order weights are considered.
   Calculation method of order weights based on Orness, and construction
   method of comparison matrix for criteria weights based on analytical
   hierarchy program (AHP), are introduced. Single objective multi-criteria
   evaluations with GIS-based OWA method for conflicting objectives are
   worked out. According to the data of influence factors in Tangshan City,
   criteria are established, images of influence factors are standardized,
   criteria weights and order weights are calculated. Single-objective
   suitability maps are obtained, and the multi-objective problem is
   solved. Finally, the calculating results are analyzed, and some advice
   is proposed. (C) 2011 Published by Elsevier Ltd. Selection and/or
   peer-review under responsibility of the Organizers of 2011 International
   Conference on Energy and Environmental Science.}},
Publisher = {{ELSEVIER SCIENCE BV}},
Address = {{SARA BURGERHARTSTRAAT 25, PO BOX 211, 1000 AE AMSTERDAM, NETHERLANDS}},
