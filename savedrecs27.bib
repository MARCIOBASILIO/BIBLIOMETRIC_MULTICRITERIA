@article{ ISI:000285930800012,
Author = {Erensal, Yasemin C. and Gurbuz, Tuncay and Albayrak, Y. Esra},
Title = {{A Multi-Criteria Decision Model for Architecturing Competence in Human
   Performance Technology}},
