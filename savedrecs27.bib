@article{ ISI:000282607800049,
Author = {Wei, Chiu-Chi and Chang, Houn-Wen},
Title = {{A new approach for selecting portfolio of new product development
   projects}},
Journal = {{EXPERT SYSTEMS WITH APPLICATIONS}},
