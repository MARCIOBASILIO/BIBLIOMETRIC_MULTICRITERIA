Title = {{Delineation of Groundwater Potential Zones for Hard Rock Region in
   Karnataka Using AHP and GIS}},
Booktitle = {{ADVANCES IN REMOTE SENSING AND GEO INFORMATICS APPLICATIONS}},
Series = {{Advances in Science Technology \& Innovation}},
