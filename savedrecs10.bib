@article{ ISI:000475760500011,
Author = {Keowsiri, Supachai and Leeprechanon, Nopbhorn},
Title = {{OPTIMAL BUDGET ALLOCATION STRATEGY BASED ON CONDITION BASED MAINTENANCE
   FOR POWER DISTRIBUTION SYSTEMS ASSETS: A CASE OF CIRCUIT BREAKERS}},
