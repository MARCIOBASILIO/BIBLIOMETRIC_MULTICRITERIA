@article{ ISI:000467056500008,
Author = {Senapati, Tapan and Yager, Ronald R.},
Title = {{Some New Operations Over Fermatean Fuzzy Numbers and Application of
   Fermatean Fuzzy WPM in Multiple Criteria Decision Making}},
Journal = {{INFORMATICA}},
Year = {{2019}},
Volume = {{30}},
Number = {{2}},
Pages = {{391-412}},
Abstract = {{Fermatean fuzzy sets (FFSs), proposed by Senapati and Yager (2019a), can
   handle uncertain information more easily in the process of decision
   making. They de fined basic operations over the Fermatean fuzzy sets.
   Here we shall introduce three new operations: subtraction, division, and
   Fermatean arithmetic mean operations over Fermatean fuzzy sets. We
   discuss their properties in details. Later, we develop a Fermatean fuzzy
   weighted product model to solve the multi-criteria decision-making
   problem. Finally, an illustrative example of selecting a suitable bridge
   construction method is given to verify the approach developed by us and
   to demonstrate its practicability and effectiveness.}},
