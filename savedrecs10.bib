@article{ ISI:000455518200018,
Author = {Khan, Muhammad Sajjad Ali and Abdullah, Saleem and Ali, Asad and Amin,
   Fazli and Hussain, Fawad},
Title = {{Pythagorean hesitant fuzzy Choquet integral aggregation operators and
   their application to multi-attribute decision-making}},
