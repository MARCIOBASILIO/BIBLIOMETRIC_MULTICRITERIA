Title = {{Evaluating and ranking suppliers by a multi-attribute decision making
   approach under uncertainty (case study: steel industry)}},
Booktitle = {{PROCEEDINGS OF 2019 15TH IRAN INTERNATIONAL INDUSTRIAL ENGINEERING
   CONFERENCE (IIIEC)}},
Year = {{2019}},
Pages = {{106-111}},
Note = {{15th Iran International Industrial Engineering Conference (IIIEC), Yazd
