@inproceedings{ ISI:000449742700015,
Author = {Jiri, Millek},
Editor = {{Lhotska, L and Sukupova, L and Lackovic, I and Ibbott, GS}},
Title = {{The Robustness of TOPSIS Results Using Sensitivity Analysis Based on
   Weight Tuning}},
Booktitle = {{WORLD CONGRESS ON MEDICAL PHYSICS AND BIOMEDICAL ENGINEERING 2018, VOL 2}},
Series = {{IFMBE Proceedings}},
