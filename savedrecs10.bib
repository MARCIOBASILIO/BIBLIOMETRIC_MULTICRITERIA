@inproceedings{ ISI:000445094400046,
Author = {Fellir, Fadoua and Nafil, Khalid and Touahni, Rajaa and Chung, Lawrence},
Editor = {{Silhavy, R}},
Title = {{Improving Case Based Software Effort Estimation Using a Multi-criteria
   Decision Technique}},
