@article{ ISI:000448494300013,
Author = {Doumpos, Michalis and Figueira, Jose Rui},
Title = {{A multicriteria outranking approach for modeling corporate credit
   ratings: An application of the ELECTRE TRI-NC method}},
Journal = {{OMEGA-INTERNATIONAL JOURNAL OF MANAGEMENT SCIENCE}},
Year = {{2019}},
Volume = {{82}},
Pages = {{166-180}},
Month = {{JAN}},
Abstract = {{Corporate credit ratings are widely used in financial services for risk
   management, investment, and financing decisions. In this study, the use
   of a recently developed multicriteria outranking approach, namely the
   ELECTRE TRI-NC method, is examined for constructing internal credit
   rating models in an expert-based judgmental framework. The models are
   constructed in a multicriteria classification (sorting) setting and the
   results are analyzed in terms of their internal properties as well as
   their deviations from risk rating categories defined by rating agencies
   (i.e. external benchmarking). A simulation/scenario analysis is
   conducted to examine the results and performance of the outranking
   models in relation to their parameters. Empirical results are provided
   for a sample of European firms rated by three leading rating agencies.
   (C) 2018 Elsevier Ltd. All rights reserved.}},
