@article{ ISI:000474715000091,
Author = {Nguyen Xuan Thao and Smarandache, Florentin},
Title = {{A new fuzzy entropy on Pythagorean fuzzy sets}},
Journal = {{JOURNAL OF INTELLIGENT \& FUZZY SYSTEMS}},
Year = {{2019}},
Volume = {{37}},
Number = {{1}},
Pages = {{1065-1074}},
Abstract = {{Pythagorean fuzzy sets are an extension of the intuitionistic fuzzy
   sets, and it also overcomes the limitations of the intuitionistic fuzzy
   sets. The entropy of a Pythagorean fuzzy set (PFS) is a measure of
   uncertainty related to the PFS. In this article, we exploit the concept
   of probability for defining the fuzzy entropy of Pythagorean fuzzy sets
   as an extension of the fuzzy entropy of Intuitionistic fuzzy sets
   (IFSs). Compared to some previous measures, the new measure is simpler,
   closer to the statistical significance and it reflects better fuzzy
   properties. After that, we give some numerical examples to compare our
   proposed entropy measure to some existing entropy of Pythagorean fuzzy
   sets. The results on numerical examples show that the proposed entropy
   measures seem to be more reliable for presenting the degree of fuzziness
   of a PFS and/or IFS. We also proposed a COPRAS multi-criteria
   decision-making method with weights calculated based on the proposed new
   entropy measure. The illustrated numerical example shows that the
   calculated results according to the proposed new method are similar to
   the calculation results according to some other existing methods.}},
