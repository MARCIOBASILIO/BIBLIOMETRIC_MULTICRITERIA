@article{ ISI:000457238100003,
Author = {Czekster, Ricardo M. and De Carvalho, Henrique Jung and Kessler,
   Gabriela Zucchetti and Kipper, Liane Mahlmann and Webber, Thais},
Title = {{Decisor: A Software Tool to Drive Complex Decisions with Analytic
   Hierarchy Process}},
Journal = {{INTERNATIONAL JOURNAL OF INFORMATION TECHNOLOGY \& DECISION MAKING}},
Year = {{2019}},
Volume = {{18}},
Number = {{1}},
Pages = {{65-86}},
Month = {{JAN}},
Abstract = {{Addressing consistent and reliable decision-making are crucial
   activities when choosing seemingly related alternatives for a set of
   criteria. Models and methods for aiding decisions such as analytic
   hierarchy process (AHP) were developed to handle quantified assessments
   of quality attributes, usually intangible and numerically hard to cope.
   We introduce the Decisor software tool to assist the use of AHP to drive
   complex decision modeling. We present a study of related tools
   describing advantages and drawbacks. The tool is user friendly, with
   intuitive data entry fields. Users may input alternatives and criteria,
   assigning weights from different judgment scales. Decisor's main
   characteristic is its simple interface where the tool computes priority
   vectors for all alternatives and its consistency rates. It is also
   possible to operate with negative weighting, inferring Benefits,
   Opportunities, Costs, and Risks. The tool implements Group Decisions,
   using the geometric mean to combine criteria comparisons from selected
   stakeholders.}},
