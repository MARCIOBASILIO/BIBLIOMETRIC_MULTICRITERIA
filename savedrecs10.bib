@article{ ISI:000449449100045,
Author = {Santos, Joao and Bressi, Sara and Cerezo, Veronique and Lo Presti,
   Davide},
Title = {{SUP\&R DSS: A sustainability-based decision support system for road
   pavements}},
Journal = {{JOURNAL OF CLEANER PRODUCTION}},
