@article{ ISI:000485697400047,
Author = {Xu, Zheng-Wang and Mei, Wei and Yu, Jia-Qi and Zhang, Jia-Rui and Yi,
   Yu-Chun},
Title = {{Research on the lightning disaster risk assessment of electronic
   information system with intuitionistic fuzzy information}},
Journal = {{JOURNAL OF INTELLIGENT \& FUZZY SYSTEMS}},
Year = {{2019}},
Volume = {{37}},
Number = {{2}},
Pages = {{2043-2050}},
Abstract = {{With the rapid development of science and technology, more and more
   electronic devices, such as communication equipment, navigation
   equipment, and various information systems based on computer network
   installed in which leading to greatly improved possibilities of
   electromagnetic interference. Take consideration of other natural
   factors, the electromagnetic environment is getting more and more
   complicated. Normal operation of the electronic information systems
   installed in the vehicle are greatly threatened under the severe
   electromagnetic environment, improve interference prediction and design
   capability, to avoid electromagnetic compatibility problems, and to
   strengthen the electromagnetic environmental adaptability of electronic
   information systems has become widespread concern. The problem of
   evaluating the lightning disaster risk of electronic information system
   with intuitionistic fuzzy information is the multiple attribute decision
   making (MADM) problems. In this paper, we shall investigate the multiple
   attribute decision making problems for lightning disaster risk
   assessment of electronic information system with intuitionistic fuzzy
   information. Then, we used the induced intuitionistic fuzzy Hamacher
   ordered weighted averaging (I-IFHOWA) operator to evaluate the lightning
   disaster risk of electronic information system. Finally, an illustrative
   example is given to verify the developed approach.}},
