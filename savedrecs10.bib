@article{ ISI:000483450900020,
Author = {Liu, Ruobing and Ruan, Chuan-Yang and Li, Deng-Feng and Xu, Hui},
Title = {{HESITANT FUZZY MULTI-ATTRIBUTE DECISION-MAKING METHOD BASED ON SIGNED
   CORRELATION AND PRIORITIZATION RELATIONSHIP}},
Journal = {{JOURNAL OF NONLINEAR AND CONVEX ANALYSIS}},
Year = {{2019}},
Volume = {{20}},
Number = {{6, SI}},
Pages = {{1241-1252}},
Abstract = {{To solve the excessive subjectivity problem caused by artificially added
   elements in hesitant fuzzy elements, we propose a hesitant fuzzy
   decision making method based on signed correlation that considers
   attribute priority. In order to consider the variance of the elements
   and the number of elements, we propose the hesitation degree of hesitant
   fuzzy set, and on this basis, hesitant fuzzy signed correlation
   measurement is defined. These are then applied to the multi-attribute
   decision-making problem in multi-sensor electronic reconnaissance.
   Comparative analyses are made via simulation to test the rationality and
   validity of the proposed method.}},
