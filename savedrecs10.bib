Title = {{Applicability of TOPSIS Model and Markowitz Model}},
Booktitle = {{14TH INTERNATIONAL SYMPOSIUM ON GEOMETRIC FUNCTION THEORY AND
   APPLICATIONS}},
Series = {{Journal of Physics Conference Series}},
Year = {{2019}},
