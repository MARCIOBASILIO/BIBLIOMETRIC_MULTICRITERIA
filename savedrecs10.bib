@article{ ISI:000488869500083,
Author = {Akbas, Serkan and Dalkilic, Turkan Erbay},
Title = {{DECISION MAKING FOR PORTFOLIO SELECTION BY FUZZY MULTI CRITERIA LINEAR
   PROGRAMMING}},
Journal = {{COMMUNICATIONS FACULTY OF SCIENCES UNIVERSITY OF ANKARA-SERIES A1
   MATHEMATICS AND STATISTICS}},
Year = {{2019}},
