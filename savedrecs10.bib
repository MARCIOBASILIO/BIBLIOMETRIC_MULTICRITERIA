@article{ ISI:000489941200056,
Author = {Liu, Peide and Shen, Mengjiao},
Title = {{An extended C-TODIM method with linguistic intuitionistic fuzzy numbers}},
Journal = {{JOURNAL OF INTELLIGENT \& FUZZY SYSTEMS}},
