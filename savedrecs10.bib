Title = {{Sustainable development strategy of rural built-up landscapes in
   Northeast China based on ANP approach}},
Booktitle = {{TECHNOLOGIES AND MATERIALS FOR RENEWABLE ENERGY, ENVIRONMENT AND
   SUSTAINABILITY (TMREES)}},
Series = {{Energy Procedia}},
