@article{ ISI:000450629700008,
Author = {Mishra, Arunodaya Raj and Rani, Pratibha},
Title = {{Interval-Valued Intuitionistic Fuzzy WASPAS Method: Application in
   Reservoir Flood Control Management Policy}},
