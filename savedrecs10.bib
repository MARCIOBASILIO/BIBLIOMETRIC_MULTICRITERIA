Title = {{A TOPSIS Approach of Ranking Classifiers for Stock Index Price Movement
   Prediction}},
Booktitle = {{SOFT COMPUTING IN DATA ANALYTICS, SCDA 2018}},
Series = {{Advances in Intelligent Systems and Computing}},
