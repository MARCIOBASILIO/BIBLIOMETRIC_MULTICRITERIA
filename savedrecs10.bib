@article{ ISI:000471765800007,
Author = {Liao, Zhiqiang and Liao, Huchang and Gou, Xunjie and Xu, Zeshui and
   Zavadskas, Edmundas Kazimieras},
Title = {{A HESITANT FUZZY LINGUISTIC CHOQUET INTEGRAL-BASED MULTIMOORA METHOD FOR
   MULTIPLE CRITERIA DECISION MAKING AND ITS APPLICATION IN TALENT
   SELECTION}},
Journal = {{ECONOMIC COMPUTATION AND ECONOMIC CYBERNETICS STUDIES AND RESEARCH}},
Year = {{2019}},
Volume = {{53}},
Number = {{2}},
Pages = {{113-130}},
Abstract = {{In qualitative multiple criteria decision making (MCDM) process, dealing
   with complex linguistic evaluations and modeling interaction phenomena
   among criteria are two important issues. The purpose of this study is to
   introduce two Choquet integral operators for HFLTSs to tackle
   interactions among criteria and then implement these operators to a
   well-known MCDM method, namely, the MULTIMOORA (multi-objective
   optimization by ratio analysis plus the full multiplicative form).
   Firstly, we introduce new operations for HFLTSs based on some linguistic
   scale functions. Then, we define two hesitant fuzzy linguistic Choquet
   integral operators and propose a novel score function for hesitant fuzzy
   linguistic elements. Afterwards, we improve the HFL-MULTINIOORA method
   to handle the hesitant fuzzy linguistic MCDM problems in which the
   criteria are inter-dependent. Finally, an illustration regarding the
   human resource development at Sichuan University is shown to verify the
   applicability and validity of the proposed method.}},
