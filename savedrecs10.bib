@article{ ISI:000454378000016,
Author = {Nie Ru-xin and Tian Zhang-peng and Wang Xiao-kang and Wang Jian-qiang
   and Wang Tie-li},
Title = {{Risk evaluation by FMEA of supercritical water gasification system using
   multi-granular linguistic distribution assessment}},
Journal = {{KNOWLEDGE-BASED SYSTEMS}},
