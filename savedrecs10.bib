Author = {Peng, Shitong and Li, Tao and Li, Mengyun and Guo, Yanchun and Shi,
   Junli and Tan, George Z. and Zhang, Hongchao},
Title = {{An integrated decision model of restoring technologies selection for
   engine remanufacturing practice}},
