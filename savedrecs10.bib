@article{ ISI:000456238600003,
Author = {Ozsahin, I. and Sharif, T. and Ozsahin, D. Uzun and Uzun, B.},
Title = {{Evaluation of solid-state detectors in medical imaging with fuzzy
   PROMETHEE}},
Journal = {{JOURNAL OF INSTRUMENTATION}},
