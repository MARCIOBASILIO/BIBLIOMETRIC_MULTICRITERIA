@article{ ISI:000454548400263,
Author = {Nayak, Suvendu Chandan and Tripathy, Chitaranjan},
Title = {{Deadline based task scheduling using multi-criteria decision-making in
   cloud environment}},
Journal = {{AIN SHAMS ENGINEERING JOURNAL}},
Year = {{2018}},
Volume = {{9}},
Number = {{4}},
Pages = {{3315-3324}},
Month = {{DEC}},
Abstract = {{Task scheduling within the deadline is more challenging due to different
   task parameters: start time, execution time, deadline and number of
   virtual machines (VMs). The OpenNebula platform supports Backfilling
   algorithm to schedule deadline based tasks. If there are similar types
   of tasks, then the scheduling performance should be enhanced. In this
   work, we propose, implementation of Multi-criteria Decision Making
   (MCDM) using VIKOR method to the existing backfilling algorithm. The
   VIKOR method is implemented as a decision maker to resolve conflicts
   among similar tasks. The mechanism finds the best ideal task among the
   similar tasks and ranks them for scheduling. Therefore, a number of
   tasks are scheduled. The performance is evaluated by analyzing the
   number of tasks scheduled, resource utilization and task rejection,
   which is quite better in comparison to the existing algorithm. (C) 2018
   Production and hosting by Elsevier B.V. on behalf of Ain Shams
   University.}},
