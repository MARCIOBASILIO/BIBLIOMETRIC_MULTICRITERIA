Title = {{Supplier Selection and Order Allocation Using TOPSIS and Linear
   Programming Method at PT. Sekarlima Surakarta}},
Booktitle = {{4TH INTERNATIONAL CONFERENCE ON INDUSTRIAL, MECHANICAL, ELECTRICAL, AND
   CHEMICAL ENGINEERING}},
Series = {{AIP Conference Proceedings}},
Year = {{2019}},
