@article{ ISI:000465453100049,
Author = {Ince, Murat and Yigit, Tuncay and Isik, Ali Hakan},
Title = {{A hybrid AHP-GA method for metadata-based learning object evaluation}},
Journal = {{NEURAL COMPUTING \& APPLICATIONS}},
