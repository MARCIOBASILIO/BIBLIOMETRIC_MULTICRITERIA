Author = {Dobre, Ciprian and Radulescu, Constanta Zoie and Bajenaru, Lidia},
Book-Group-Author = {{IEEE}},
Title = {{Parameters weighting in elderly monitoring based on multi-criteria
   methods}},
Booktitle = {{2019 22ND INTERNATIONAL CONFERENCE ON CONTROL SYSTEMS AND COMPUTER
   SCIENCE (CSCS)}},
Series = {{International Conference on Control Systems and Computer Science CSCS}},
