@article{ ISI:000447255700011,
Author = {Boostani, Abtin and Jolai, Fariborz and Bozorgi-Amiri, Ali},
Title = {{Optimal Location Selection of Temporary Accommodation Sites in Iran via
   a Hybrid Fuzzy Multiple-Criteria Decision Making Approach}},
