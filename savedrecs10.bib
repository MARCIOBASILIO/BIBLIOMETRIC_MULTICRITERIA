@article{ ISI:000452281800008,
Author = {Zhou, Xiao-Guang and Ding, Yang-Fan and Lu, Mi},
Title = {{Study on bond selection under intuitionistic fuzzy conditions}},
Journal = {{JOURNAL OF ALGORITHMS \& COMPUTATIONAL TECHNOLOGY}},
Year = {{2018}},
Volume = {{12}},
