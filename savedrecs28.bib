@article{ ISI:000263453100010,
Author = {Sayadi, Mohammad Kazem and Heydari, Majeed and Shahanaghi, Kamran},
Title = {{Extension of VIKOR method for decision making problem with interval
   numbers}},
Journal = {{APPLIED MATHEMATICAL MODELLING}},
Year = {{2009}},
Volume = {{33}},
Number = {{5}},
Pages = {{2257-2262}},
Month = {{MAY}},
Abstract = {{The VIKOR method was developed for multi-criteria optimization of
   complex systems. It determines the compromise ranking list and the
   compromise solution obtained with the initial (given) weights. This
   method focuses on ranking and selecting from a set of alternatives in
   the presence of conflicting criteria. It introduces the multi-criteria
   ranking index based on the particular measure of ``closeness{''} to the
   ``ideal{''} solution. The aim of this paper is to extend the VIKOR
   method for decision making problems with interval number. The extended
   VIKOR method's ranking is obtained through comparison of interval
   numbers and for doing the comparisons between intervals, we introduce x
   as optimism level of decision maker. Finally, a numerical example
   illustrates and clarifies the main results developed in this paper. (c)
   2008 Elsevier Inc. All rights reserved.}},
