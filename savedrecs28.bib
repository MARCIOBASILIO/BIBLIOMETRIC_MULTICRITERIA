@inproceedings{ ISI:000274849000015,
Author = {Dhouib, Souhail and Kouraichi, Sana and Loukil, Taicir and Chabchoub,
   Habib},
Editor = {{Krasnogor, N and MelianBatista, MB and Perez, JAM and MorenoVega, JM and Pelta, DA}},
Title = {{An Interactive Simulated Annealing Multi-agents Platform to Solve
   Hierarchical Scheduling Problems with Goals}},
Booktitle = {{NICSO 2008: NATURE INSPIRED COOPERATIVE STRATEGIES FOR OPTIMIZATION}},
Series = {{Studies in Computational Intelligence}},
Year = {{2009}},
