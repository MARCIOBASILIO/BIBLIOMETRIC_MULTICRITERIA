@article{ ISI:000265260800022,
Author = {Ding, Ji-Feng},
Title = {{FUZZY MCDM APPROACH FOR SELECTING STRATEGIC PARTNER: AN EMPIRICAL STUDY
   OF A CONTAINER SHIPPING COMPANY IN TAIWAN}},
