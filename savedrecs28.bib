@inproceedings{ ISI:000272995300112,
Author = {Han, Xiao-juan and Jiang, Xue},
Editor = {{Cao, B and Li, TF and Zhang, CY}},
Title = {{Fault Diagnosis of Pulverizing System Based on Fuzzy Decision-Making
   Fusion Method}},
Booktitle = {{FUZZY INFORMATION AND ENGINEERING, VOLUME 2}},
Series = {{Advances in Intelligent and Soft Computing}},
Year = {{2009}},
Volume = {{62}},
Pages = {{1045+}},
Note = {{3rd International Conference on Fuzzy Information and Engineering (ICFIE
   2009), Chongqing Univ Sci \& Technol, Chongqing, PEOPLES R CHINA, SEP
   26-29, 2009}},
Organization = {{Fuzzy Informat \& Engn Branch, China Operat Res Soc; Fuzzy Informat \&
   Engn Branch; Guangzhou Univ; Mazandaran Univ; Natl Nat Sci Fdn China;
   Fuzzy Optimizat \& Dec Making; Fuzzy Informat \& Engn}},
Abstract = {{Fault process of pulverizing system in power plant belongs to soft
   fault. The used diagnosis methods only can identify the fault type but
   not determine the specific location and the status of the equipment at
   present. A hybrid model based on fuzzy decision-making fusion was
   provided in this paper combined with grey relation analysis and neural
   network. First the diagnosis sub-network was established by each kind of
   faults of the pulverizing system using grey relation indexes to divided
   the fault status such as normal, slight, moderate and serious and then
   adopted single-layer neural network to carry on fault recognition, the
   output result of single-layer neural network was regarded as the fuzzy
   satisfaction degrees of multi-attribute decision-making model to be
   fused with the output of diagnosis sub-network in decision-making level.
   The validity and feasibility of the method was verified through actual
   data simulation of pulverizing system which is advantageous to get
   accurate identification to early fault, fault degree as well as fault
   tendency.}},
