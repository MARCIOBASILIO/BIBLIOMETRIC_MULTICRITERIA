@article{ ISI:000271709500009,
Author = {Boujelben, Mohamed Ayman and De Smet, Yves and Frikha, Ahmed and
   Chabchoub, Habib},
Title = {{Building a binary outranking relation in uncertain, imprecise and
   multi-experts contexts: The application of evidence theory}},
Journal = {{INTERNATIONAL JOURNAL OF APPROXIMATE REASONING}},
Year = {{2009}},
Volume = {{50}},
Number = {{8}},
Pages = {{1259-1278}},
Month = {{SEP}},
Abstract = {{We consider multicriteria decision problems where the actions are
   evaluated on a set of ordinal criteria. The evaluation of each
   alternative with respect to each criterion may be uncertain and/or
   imprecise and is provided by one or several experts. We model this
   evaluation as a basic belief assignment (BBA). In order to compare the
   different pairs of alternatives according to each criterion, the concept
   of first belief dominance is proposed. Additionally, criteria weights
   are also expressed by means of a BBA. A model inspired by ELECTRE I is
   developed and illustrated by a pedagogical example. (C) 2009 Elsevier
   Inc. All rights reserved.}},
