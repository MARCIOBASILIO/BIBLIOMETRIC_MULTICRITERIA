@article{ ISI:000264528600152,
Author = {Chang, Yu-Hern and Wey, Wann-Ming and Tseng, Hsiao-Yu},
Title = {{Using ANP priorities with goal programming for revitalization strategies
   in historic transport: A case study of the Alishan Forest Railway}},
Journal = {{EXPERT SYSTEMS WITH APPLICATIONS}},
Year = {{2009}},
Volume = {{36}},
Number = {{4}},
Pages = {{8682-8690}},
Month = {{MAY}},
Abstract = {{The main purpose of this research was to solve a real-world,
   multi-criteria, revitalization strategies project selection problem for
   the historic Alishan Forest Railway in Taiwan by using fuzzy Delphi,
   analytic network process (ANP), and zero-one goal programming (ZOGP).
   This required identifying the most cost-beneficial projects for
   maximizing the net benefit to the public and allocating resources most
   efficiently. To evaluate different revitalization strategies, we used a
   hierarchical network model based on various factors, and we present the
   interactions of those factors. The proposed ANP model consists of a
   control hierarchy and a network of connections between the clusters of
   alternatives, actors, and criteria. The strategic criteria were included
   into the model to rate benefits (B), opportunities (0), costs (C), and
   risks (R). A synthesis of alternatives was finally obtained using rated
   BOCR. In this paper, we suggest an improved methodology, one that uses
   an integrated approach and reflects the interdependencies between the
   evaluation criteria and candidate projects. The management of the
