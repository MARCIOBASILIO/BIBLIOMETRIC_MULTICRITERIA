@article{ ISI:000279181800004,
Author = {Zavadskas, Edmundas Kazimieras and Kaklauskas, Arturas and Vilutiene,
   Tatjana},
Title = {{MULTICRITERIA EVALUATION OF APARTMENT BLOCKS MAINTENANCE CONTRACTORS:
   LITHUANIAN CASE STUDY}},
Journal = {{INTERNATIONAL JOURNAL OF STRATEGIC PROPERTY MANAGEMENT}},
Year = {{2009}},
Volume = {{13}},
Number = {{4}},
Pages = {{319-338}},
Abstract = {{The paper presents the comparative analysis of dwelling maintenance
   contractors aimed at determining the degree of their utility for users
   and bidding price of services by applying the method of multicriteria
   complex proportional assessment. To compare the performance of various
   maintenance contractors, the data from 15 dwelling maintenance
   organizations was used. A questionnaire survey of dwelling owners was
   conducted. Contractors were evaluated by a set of 44 criteria
   characterizing them from various perspectives. The analysis was made
   taking into account the standpoints of building owners ( clients). The
   initial weights of qualitative criteria were calculated by expert
   methods. Then they were coordinated with the calculated values of
   quantitative criteria using the method of multicriteria complex
   proportional assessment. Multicriteria analysis of the performance of
