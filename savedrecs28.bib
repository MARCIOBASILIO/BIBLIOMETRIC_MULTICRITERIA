@article{ ISI:000271571000024,
Author = {Hung, Kuo-Chen and Julian, Peter and Chien, Terence and Jin, Warren
   Tsu-Huei},
Title = {{A decision support system for engineering design based on an enhanced
   fuzzy MCDM approach}},
