Author = {Lee, Wen-Shiung and Tzeng, Gwo-Hshiung and Guan, Jyh-Liang and Chien,
   Kuo-Ting and Huang, Juan-Ming},
Title = {{Combined MCDM techniques for exploring stock selection based on Gordon
   model}},
