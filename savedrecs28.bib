@article{ ISI:000259851900002,
Author = {Durbach, Ian},
Title = {{On the estimation of a satisficing model of choice using stochastic
   multicriteria acceptability analysis}},
Journal = {{OMEGA-INTERNATIONAL JOURNAL OF MANAGEMENT SCIENCE}},
