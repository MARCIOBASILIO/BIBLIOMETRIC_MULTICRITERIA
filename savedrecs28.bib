@article{ ISI:000268270600055,
Author = {Sun, Chia-Chi and Lin, Grace T. R. and Tzeng, Gwo-Hshiung},
Title = {{The evaluation of cluster policy by fuzzy MCDM: Empirical evidence from
   HsinChu Science Park}},
