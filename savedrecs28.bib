@article{ ISI:000267357600025,
Author = {Antonio Cortes-Aldana, Felix and Garcia-Melon, Monica and
   Fernandez-de-Lucio, Ignacio and Aragones-Beltran, Pablo and
   Poveda-Bautista, Rocio},
Title = {{University objectives and socioeconomic results: A multicriteria
   measuring of alignment}},
