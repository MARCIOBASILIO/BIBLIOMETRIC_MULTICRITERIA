Title = {{Towards the Neuropsychological Diagnosis of Alzheimer's Disease: A
   Hybrid Model in Decision Making}},
Booktitle = {{BEST PRACTICES FOR THE KNOWLEDGE SOCIETY: KNOWLEDGE, LEARNING,
   DEVELOPMENT AND TECHNOLOGY FOR ALL}},
Series = {{Communications in Computer and Information Science}},
