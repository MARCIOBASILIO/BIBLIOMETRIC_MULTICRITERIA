Title = {{The Analytic Hierarchy Process in the Transportation Sector}},
Booktitle = {{MULTIPLE CRITERIA DECISION MAKING FOR SUSTAINABLE ENERGY AND
   TRANSPORTATION SYSTEMS: PROCEEDINGS OF THE 19TH INTERNATIONAL CONFERENCE
   ON MULTIPLE CRITERIA DECISION MAKING}},
Series = {{Lecture Notes in Economics and Mathematical Systems}},
