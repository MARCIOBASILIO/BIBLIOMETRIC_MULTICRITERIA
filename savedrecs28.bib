@inproceedings{ ISI:000277450700320,
Author = {Chakraborty, Subrata and Yeh, Chung-Hsing},
Book-Group-Author = {{IEEE}},
Title = {{A Simulation Comparison of Normalization Procedures for TOPSIS}},
