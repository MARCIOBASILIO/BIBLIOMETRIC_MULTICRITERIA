@article{ ISI:000266722400070,
Author = {Mosadeghi, R. and Tomlinson, R. and Mirfenderesk, H. and Warnken, J.},
Title = {{Coastal Management Issues in Queensland and Application of the
   Multi-Criteria Decision Making Techniques}},
Journal = {{JOURNAL OF COASTAL RESEARCH}},
