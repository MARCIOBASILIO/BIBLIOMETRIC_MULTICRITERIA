@inproceedings{ ISI:000392561600078,
Author = {Medineckiene, Milena and Turskis, Zenonas and Zavadskas, Edmundas
   Kazimieras and Tamosaitiene, Jolanta},
Editor = {{Vainiunas, P and Zavadskas, EK}},
Title = {{MULTI-CRITERIA SELECTION OF THE ONE FLAT DWELLING HOUSE, TAKING INTO
   ACCOUNT THE CONSTRUCTION IMPACT ON ENVIRONMENT}},
Booktitle = {{MODERN BUILDING MATERIALS, STRUCTURES AND TECHNIQUES, 10TH INTERNATIONAL
   CONFERENCE 2010, VOL I}},
Year = {{2010}},
Pages = {{455-460}},
Note = {{10th International Conference on Modern Building Materials, Structures
   and Techniques, LITHUANIA, MAY 19-21, 2010}},
Organization = {{Vilnius Gediminas TU, Fac Civil Engn; Lithuanian Acad Sci; Int Assoc
   Bridge \& Structural Engn, Lithuanian Natl Grp; Assoc European Civil
   Engn Fac; European Council Civil Engn}},
Abstract = {{The article describes a multi-criteria selection of the one flat
   dwelling house, taking into account the construction ecological aspects,
   their impact on environment and their economic and social condition. A
   problem of a global climate change is discussed, how construction
   industry determinate the CO2 emission to an atmosphere, how a
   construction material manufacturing impacts to an environment. The best
   alternative selection of the one flat dwelling house is presented. The
   first house is masonry, built from standard materials, the second is the
   blockhouse, made mostly of wood-based materials, and the third is built
   of wood frame, using a wood-based materials and mineral based materials.
