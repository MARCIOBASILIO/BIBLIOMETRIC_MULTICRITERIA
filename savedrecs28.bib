@inproceedings{ ISI:000393085200009,
Author = {De, Arijit and Diaz, Elizabeth},
Editor = {{Filipe, J and Kacprzyk, J}},
Title = {{FUZZY ANP A Analytical Network Model for Result Merging for Metasearch
   using Fuzzy Linguistic Quantifiers}},
Booktitle = {{ICFC 2010/ ICNC 2010: PROCEEDINGS OF THE INTERNATIONAL CONFERENCE ON
   FUZZY COMPUTATION AND INTERNATIONAL CONFERENCE ON NEURAL COMPUTATION}},
Year = {{2010}},
Pages = {{73-78}},
