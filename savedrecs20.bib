@article{ ISI:000364502500014,
Author = {Yildiz, A. and Yayla, A. Y.},
Title = {{MULTI-CRITERIA DECISION-MAKING METHODS FOR SUPPLIER SELECTION: A
   LITERATURE REVIEW}},
