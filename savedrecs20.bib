@article{ ISI:000355926600005,
Author = {Lue, Alessandro and Colorni, Alberto},
Title = {{Conflict Analysis for Environmental Impact Assessment: A Case Study of a
   Transportation System in a Tourist Area}},
Journal = {{GROUP DECISION AND NEGOTIATION}},
Year = {{2015}},
Volume = {{24}},
Number = {{4, SI}},
Pages = {{613-632}},
Month = {{JUL}},
Abstract = {{The paper presents a conflict analysis in an environmental impact
   assessment. In order to cope with a problem of traffic congestion in a
   tourist area, alternative transportation services and measures of travel
   demand management are studied and compared using the multi-attribute
   value theory. The problem is characterized by the presence of different
   criteria and conflicting actors who have different interests (expressed
   by criteria weights) and different decision power, which have been
   elicited from the actors' representatives. A group viewpoint is
   generated through an aggregation of the different actors' viewpoints, in
   order to find a group compromise solution. A conflict analysis,
   conducted on the criteria weights, is performed to examine the level of
   agreement associated with the compromise solution.}},
