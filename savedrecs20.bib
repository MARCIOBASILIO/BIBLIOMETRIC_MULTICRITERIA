@article{ ISI:000362383800022,
Author = {Dragovic, N. and Vulevic, T.},
Title = {{Selecting the best bid in water resources management sector applying AHP
   method}},
Journal = {{BAUINGENIEUR}},
Year = {{2015}},
Volume = {{90}},
Pages = {{420-426}},
Month = {{SEP}},
Abstract = {{Soil erosion and torrential floods are one of the most serious
   environmental problems. Torrent control projects represent investment
   projects consisting of several phases whose implementation involved more
   participants. Project participants are one of the key factors that
   contribute that project complete on time, under budget and satisfactory
   quality standards. For successful project implementation very important
   is a role that belongs to the project designer, as well as owner
   decision about project designer selection. In this paper, we analyzed
   procedure for the best bid selection for water project designing in
   Serbia. The survey was based on the tender documentation from one public
   water management company (owner) used for establishing the criteria for
   bid evaluation. The best bid selection using the multi-criteria decision
   analysis method Analytic Hierarchy Process (AHP) is done on the example
   of the public procurement work - designing the Project of Kolubara River
   regulation. The method allows taking into account a number of criteria
   and their inter-comparison with the determination of priority vector
