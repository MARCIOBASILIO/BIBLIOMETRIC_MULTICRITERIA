@article{ ISI:000351972800009,
Author = {Chen, Ting-Yu},
Title = {{An interval type-2 fuzzy PROMETHEE method using a likelihood-based
   outranking comparison approach}},
Journal = {{INFORMATION FUSION}},
Year = {{2015}},
