@article{ ISI:000360706100005,
Author = {Hu, Shu-kung and Lu, Ming-Tsang and Tzeng, Gwo-Hshiung},
Title = {{Improving Mobile Commerce Adoption Using a New Hybrid Fuzzy MADM Model}},
