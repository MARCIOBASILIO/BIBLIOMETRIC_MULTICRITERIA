@article{ ISI:000436034800006,
Author = {Pozderec, Silvo and Bavec, Martina and Rozman, Crtomir and Vincec, Jozef
   and Pazek, Karmen},
Title = {{Multi-Criteria Assessment of Vegetable Production Business Alternatives}},
