@article{ ISI:000353952500002,
Author = {Kahraman, Cengiz and Onar, Sezi Cevik and Oztaysi, Basar},
Title = {{Fuzzy Multicriteria Decision-Making: A Literature Review}},
Journal = {{INTERNATIONAL JOURNAL OF COMPUTATIONAL INTELLIGENCE SYSTEMS}},
Year = {{2015}},
Volume = {{8}},
Number = {{4}},
Pages = {{637-666}},
Month = {{JUL 4}},
Abstract = {{Multicriteria decision-making (MCDM) refers to making decisions in the
   presence of multiple and usually conflicting criteria. Fuzzy
   decision-making is used where vague and incomplete data exist for the
   solution. Fuzzy multicriteria decision-making is one of the most popular
   problems handled by the researchers in the literature. In this paper, we
   survey the latest status of fuzzy multicriteria decision-making methods
   and classify these methods dividing into two parts: fuzzy multiattribute
   decision-making (MADM) and fuzzy multiobjective decision-making (MODM).
   Most of the publications are on fuzzy MADM since there are a plenty of
   classical multiattribute decision-making methods in the literature.
   Tabular and graphical illustrations for each method are given.}},
