@article{ ISI:000365406200042,
Author = {Ramkumar, M. and Jenamani, M.},
Title = {{Sustainability in Supply Chain Through E-Procurement-An Assessment
   Framework Based on DANP and Liberatore Score}},
Journal = {{IEEE SYSTEMS JOURNAL}},
