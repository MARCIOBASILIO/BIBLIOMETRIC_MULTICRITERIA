@article{ ISI:000357238500020,
Author = {Scott, James and Ho, William and Dey, Prasanta K. and Talluri, Srinivas},
Title = {{A decision support system for supplier selection and order allocation in
   stochastic, multi-stakeholder and multi-criteria environments}},
