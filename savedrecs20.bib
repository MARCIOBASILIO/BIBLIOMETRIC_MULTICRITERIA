@article{ ISI:000371606400046,
Author = {Zhang, Maocong and Liu, Peide and Shi, Lanlan},
Title = {{An extended multiple attribute group decision-making TODIM method based
   on the neutrosophic numbers}},
Journal = {{JOURNAL OF INTELLIGENT \& FUZZY SYSTEMS}},
Year = {{2016}},
