@article{ ISI:000445030000007,
Author = {Hatefi, S. M. and Torabi, S. A. and Pourreza, N.},
Title = {{A Common Weight Multi-criteria Decision analysis-data Envelopment
   Analysis Approach with Assurance Region for Weight Derivation from
   Pairwise Comparison Matrices}},
Journal = {{INTERNATIONAL JOURNAL OF ENGINEERING}},
Year = {{2015}},
Volume = {{28}},
Number = {{12}},
Pages = {{1746-1755}},
Month = {{DEC}},
Abstract = {{Deriving weights from a pairwise comparison matrix (PCM) is a subject
   for which a wide range of methods have been presented. This paper
   proposes a common weight multi criteria decision analysis-data
   envelopment analysis (MCDA-DEA) approach with assurance region for
   weight derivation from a PCM. The proposed method has a more
   discrimination power over the conventional methods for weight derivation
   from a pairwise comparison matrix. Furthermore, the proposed model has
   several merits over the competing approaches and lacks the drawbacks of
   the well-known Data Envelopment Analysis HP and data envelopment
   analysis / assurance region methods (DEA/AR) methods. Some numerical
   examples and a case study are taken from the literature in order to
   confirm the merits of the proposed method and its applications in multi
   criteria decision making. Results disclose the advantages of the
   proposed approach.}},
