@inproceedings{ ISI:000389515100009,
Author = {Balcerzak, Adam P. and Pietrzak, Michal Bernard},
Editor = {{Loster, T and Pavelka, T}},
Title = {{APPLICATION OF TOPSIS METHOD FOR ANALYSIS OF SUSTAINABLE DEVELOPMENT IN
   EUROPEAN UNION COUNTRIES}},
Booktitle = {{10TH INTERNATIONAL DAYS OF STATISTICS AND ECONOMICS}},
