Author = {Parajuli, Ranjan and Knudsen, Marie Trydeman and Dalgaard, Tommy},
Title = {{Multi-criteria assessment of yellow, green, and woody biomasses:
   pre-screening of potential biomasses as feedstocks for biorefineries}},
Journal = {{BIOFUELS BIOPRODUCTS \& BIOREFINING-BIOFPR}},
