@article{ ISI:000358968200008,
Author = {Akter, Aysha and Ahmed, Shoukat},
Title = {{Potentiality of rainwater harvesting for an urban community in
   Bangladesh}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{2015}},
Volume = {{528}},
Pages = {{84-93}},
Month = {{SEP}},
Abstract = {{Due to cost effectiveness, rainwater harvesting (RWH) systems are
   practicing already in some rural parts of Bangladesh but very few in
   urban areas. This paper aimed to evaluate the potentiality of RWH
   systems in the South Agrabad in Chittagong city with an average annual
   precipitation of 3000 mm, experiencing both water scarcity and urban
   flooding in the same year. The adopted approach was Analytic Hierarchy
   Process (AMP) based multicriteria decision analysis technique, and the
   evaluation criteria were roof area, slope, drainage density and runoff
   coefficient. Geospatial Hydrologic Modeling Extension supported
   hydrologic model viz. HEC-HMS used to simulate the precipitation-runoff
   process, the model outcomes showed RWH potentiality which could minimize
   stagnant storm water up to 26\% through supplementing city water supply
   annually up to 20 liter/person/day. Then, assigning suitable weightage
   to the evaluation criteria with their associated features in ArcGIS 9.3,
   the study area was reasonably divided into three potential zones i.e.
   good, moderate and poor covering 19\%, 64\% and 17\% of the total area
   respectively. Thus, this is envisaged AHP using HEC-HMS could provide
   important guidance to the decision supporting system not only for urban
   areas but also for the wide sub-basin/basin context. Crown Copyright (C)
   2015 Published by Elsevier B.V. All rights reserved.}},
