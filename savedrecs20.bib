@article{ ISI:000211266700005,
Author = {Vesyropoulos, Nikolaos and Georgiadis, Christos},
Title = {{QoS-Based Filters in Web Service Compositions: Utilizing Multi-Criteria
   Decision Analysis Methods}},
Journal = {{JOURNAL OF MULTI-CRITERIA DECISION ANALYSIS}},
