@article{ ISI:000361381400007,
Author = {Tsui, Che-Wei and Tzeng, Gwo-Hshiung and Wen, Ue-Pyng},
Title = {{A hybrid MCDM approach for improving the performance of green suppliers
   in the TFT-LCD industry}},
Journal = {{INTERNATIONAL JOURNAL OF PRODUCTION RESEARCH}},
Year = {{2015}},
Volume = {{53}},
Number = {{21, SI}},
