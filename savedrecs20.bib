@article{ ISI:000374169100008,
Author = {Asgari, Moloud Sadat and Abbasi, Abbas and Alimohamadlou, Moslem},
Title = {{Comparison of ANFIS and FAHP-FGP methods for supplier selection}},
Journal = {{KYBERNETES}},
