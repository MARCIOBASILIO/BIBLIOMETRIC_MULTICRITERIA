@article{ ISI:000359155800004,
Author = {Karaman, Atilla and Dagdeviren, Metin},
Title = {{Fuzzy alpha-discounting method for multi-criteria decision-making}},
Journal = {{JOURNAL OF THE CHINESE INSTITUTE OF ENGINEERS}},
Year = {{2015}},
Volume = {{38}},
Number = {{7}},
Pages = {{855-865}},
Month = {{OCT}},
Abstract = {{The alpha-Discounting Method was developed to be an alternative to and
   extension of the Analytical Hierarchy Process (AHP) to solve
   multi-criteria decision-making (MCDM) problems with non-commensurable
   and conflicting criteria. In contrast to the AHP, this method works not
   only for pairwise comparisons but also for n-wise comparisons if
   relative importance of criteria can be expressed in a system of linear
   homogenous equations. This method also has a comparative advantage as it
   can transform those MCDM problems, classified as inconsistent by the
   AHP, into a consistent form. This study briefly compares the two methods
   and then develops the Fuzzy -Discounting Method for Multi-Criteria
   Decision Making (F-DM MCDM). Two illustrative fuzzy MCDM problems from
   the literature have been solved to show how the F-DM MCDM works.}},
