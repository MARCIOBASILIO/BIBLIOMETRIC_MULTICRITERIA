@article{ ISI:000368088000023,
Author = {Chung, Kaie-Chin},
Title = {{APPLYING ANALYTICAL HIERARCHY PROCESS TO SUPPLIER SELECTION AND
   EVALUATION IN THE HOSPITALITY INDUSTRY: A MULTIOBJECTIVE APPROACH}},
