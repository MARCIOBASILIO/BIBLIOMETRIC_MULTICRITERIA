@article{ ISI:000367685400002,
Author = {Pereira, Debora Viana e Sousa and de Miranda Mota, Caroline Maria},
Title = {{Human Development Index Based on ELECTRE TRI-C Multicriteria Method: An
   Application in the City of Recife}},
Journal = {{SOCIAL INDICATORS RESEARCH}},
