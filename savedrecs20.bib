@article{ ISI:000361483600001,
Author = {Xenarios, Stefanos and Polatidis, Heracles},
Title = {{Alleviating climate change impacts in rural Bangladesh: a PROMETHEE
   outranking-based approach for prioritizing agricultural interventions}},
Journal = {{ENVIRONMENT DEVELOPMENT AND SUSTAINABILITY}},
