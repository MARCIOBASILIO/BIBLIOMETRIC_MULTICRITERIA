@article{ ISI:000359948800005,
Author = {Afful-Dadzie, Eric and Nabareseh, Stephen and Afful-Dadzie, Anthony and
   Oplatkova, Zuzana Kominkova},
Title = {{A fuzzy TOPSIS framework for selecting fragile states for support
   facility}},
