@article{ ISI:000371612000002,
Author = {Doumpos, Michael and Xidonas, Panagiotis and Xidonas, Sotirios and
   Siskos, Yannis},
Title = {{Development of a Robust Multicriteria Classification Model for
   Monitoring the Postoperative Behaviour of Heart Patients}},
Journal = {{JOURNAL OF MULTI-CRITERIA DECISION ANALYSIS}},
