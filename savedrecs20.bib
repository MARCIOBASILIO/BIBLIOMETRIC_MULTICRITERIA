@article{ ISI:000359365100011,
Author = {Duran, Orlando},
Title = {{SPARE PARTS CRITICALITY ANALYSIS USING A FUZZY AHP APPROACH}},
Journal = {{TEHNICKI VJESNIK-TECHNICAL GAZETTE}},
Year = {{2015}},
Volume = {{22}},
