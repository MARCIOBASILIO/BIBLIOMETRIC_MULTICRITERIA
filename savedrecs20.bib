@article{ ISI:000443472100002,
Author = {Stevic, Zeljko and Tanackov, Ilija and Vasiljevic, Marko and Novarlic,
   Boris and Stojic, Gordan},
Title = {{AN INTEGRATED FUZZY AHP AND TOPSIS MODEL FOR SUPPLIER EVALUATION}},
Journal = {{SERBIAN JOURNAL OF MANAGEMENT}},
Year = {{2016}},
Volume = {{11}},
Number = {{1}},
Pages = {{15-27}},
Abstract = {{In today's modern supply chains, the adequate suppliers' choice has
   strategic meaning for entire companies' business. The aim of this paper
   is to evaluate different suppliers using the integrated model that
   recognizes a combination of fuzzy AHP (Analytical Hierarchy Process) and
   the TOPSIS method. Based on six criteria, the expert team was formed to
   compare them, so determination of their significance is being done with
   fuzzy AHP method. Expert team also compares suppliers according to each
   criteria and on the base of triangular fuzzy numbers. Based on their
   inputs, TOPSIS method is used to estimate potential solutions. Suggested
   model accomplishes certain advantages in comparison with previously used
   traditional models which were used to make decisions about evaluation
   and choice of supplier.}},
