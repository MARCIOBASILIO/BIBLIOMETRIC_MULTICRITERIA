@inproceedings{ ISI:000389774600111,
Author = {Meenakshi and Singh, N. P.},
Book-Group-Author = {{IEEE}},
Title = {{A Comparative Study of Cooperative and Non-Cooperative Game Theory in
   Network Selection}},
Booktitle = {{2016 INTERNATIONAL CONFERENCE ON COMPUTATIONAL TECHNIQUES IN INFORMATION
   AND COMMUNICATION TECHNOLOGIES (ICCTICT)}},
Year = {{2016}},
Note = {{International Conference on Computational Techniques in Information and
   Communication Technologies (ICCTICT), New Delhi, INDIA, MAR 11-13, 2016}},
Abstract = {{Fifth generation wireless network is consisting of heterogeneous
   networks to provide end to end mobility and service to mobile users. So
   in heterogeneous wireless network, selection of the optimal network
   becomes an important task. In recent years, network selection has been
   mostly studied by using different mathematical methods. But in this
   paper, the network selection problem is modelled as the evolutionary and
   bankruptcy game in heterogeneous wireless networks. Analytical Hierarchy
   Process (AHP) is used to decide the weights of different attributes
   according to service requirement and evolutionary and bankruptcy game
   theory is used for ranking of the networks. Evolutionary game is used as
   the non-cooperative game for studying the behavior of selfish users and
   bankruptcy game is used as the cooperative game for achieving mutual
   advantage. The comparison between the cooperative game and
   non-cooperative game is studied in case of streaming, conversational and
   background traffic class. Simulation results show that different
   networks are selected for different traffic class according to user
   preference.}},
