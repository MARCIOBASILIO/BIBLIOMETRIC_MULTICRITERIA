@article{ ISI:000502633500003,
Author = {Zhang, Dong and Bao, Xin and Wu, Chong},
Title = {{An Extended TODIM Method Based on Novel Score Function and Accuracy
   Function under Intuitionistic Fuzzy Environment}},
Journal = {{INTERNATIONAL JOURNAL OF UNCERTAINTY FUZZINESS AND KNOWLEDGE-BASED
   SYSTEMS}},
