@article{ ISI:000545451500010,
Author = {Mirzaee, Seyed Moslem Seyed and Mirfakhredini, Seyed Heydar and
   Sadrabadi, Alireza Nasser and Sharifabadi, Ali Morovati},
Title = {{COMPILING THE STRATEGIES OF ALFA MINING COMPANY AND PRIORITIZING THEM BY
   WASPAS METHOD}},
