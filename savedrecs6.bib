@article{ ISI:000560581800001,
Author = {Almulhim, Tarifa S. and Barahona, Igor},
Title = {{An Integrated Approach for Fuzzy-Dynamic Multi-Attribute Group Decision
   Making With Application in Renewable Energy}},
