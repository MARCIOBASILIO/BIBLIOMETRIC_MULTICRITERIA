@article{ ISI:000502984600009,
Author = {Diaz, Fabian and Cilinskis, Einars},
Title = {{Use of Multi-Criteria TOPSIS Analysis to Define a Decarbonization Path
   in Colombia}},
Journal = {{ENVIRONMENTAL AND CLIMATE TECHNOLOGIES}},
Year = {{2019}},
Volume = {{23}},
Number = {{3, SI}},
Pages = {{110-128}},
Month = {{DEC}},
Abstract = {{In the past few years, Colombia has begun to implement new laws
   according to new global trends, however, carbon dioxide emissions have
   not decreased despite country commitments according to the Paris
   agreement and other international treaties. In this paper, a pathway to
   achieve full decarbonization of the energy system of the country has
   been analysed using a multi-criteria analysis tool and considering
   several options from previous studies, evaluating three different
   abatement scenarios obtained by different models. Then, a Strength,
   Weakness, Opportunities and Threats (SWOT) analysis is conducted in
   order to propose a set of environmental policies that aid in seeking
   greater reduction of greenhouse gas emissions (GHG) in sectors as
   agriculture, forestry, land use and transport. This set of measures is
   then summarized, and their abatement costs and GHG reduction potential
   are displayed. The outcomes show the country has great potential to
   exploit its renewable natural resources and indicates how a new
   electricity mix may not only decrease GHG emissions but also reduce the
   levelized cost of electricity for the end users.}},
