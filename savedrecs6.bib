@article{ ISI:000490988600001,
Author = {Ploskas, Nikolaos and Papathanasiou, Jason},
Title = {{A decision support system for multiple criteria alternative ranking
   using TOPSIS and VIKOR in fuzzy and nonfuzzy environments}},
Journal = {{FUZZY SETS AND SYSTEMS}},
