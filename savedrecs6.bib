@article{ ISI:000516823700077,
Author = {Akram, Muhammad and Shumaiza and Al-Kenani, Ahmad N.},
Title = {{Multi-Criteria Group Decision-Making for Selection of Green Suppliers
   under Bipolar Fuzzy PROMETHEE Process}},
