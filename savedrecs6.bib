@article{ ISI:000503052700004,
Author = {Cuoghi, Kaio Guilherme and Leoneti, Alexandre Bevilacqua},
Title = {{A group MCDA method for aiding decision-making of complex problems in
   public sector: The case of Belo Monte Dam}},
Journal = {{SOCIO-ECONOMIC PLANNING SCIENCES}},
Year = {{2019}},
Volume = {{68}},
Month = {{DEC}},
Note = {{International Symposium on the Analytic Hierarchy Process, London,
   ENGLAND, AUG, 2016}},
Abstract = {{We review the applications of Multiple Criteria Decision Analysis (MCDA)
   methods in energy sector and in Environmental Impact Assessment (EIA),
   finding a gap on the non-use of specific methods that deal with
   divergent opinions, such as a group MCDA method. This way we suggest the
   application of a group MCDA method to demonstrate how it may be used for
   aiding group decision-making in public sector. Aiming at analyzing the
   aforementioned problem, we simulate the choosing of the construction
   alternatives of Belo Monte Dam. The power plant project was marked by
   several conflicts among stakeholders due to the generation of
   diversified environmental, social and economic impacts. The results show
   that a group MCDA method may be used to aid public sector in the
   analysis of complex problems, by dividing them into several parts,
   allowing, therefore, a transparent decision-making process, as well as
   to solve gaps in the EIA methods.}},
