@inproceedings{ ISI:000559924500016,
Author = {Fernandes, Andre and Almeida, Rafael and da Silva, Miguel Mira},
Book-Group-Author = {{Assoc Informat Syst}},
Title = {{A Flexible Method for COBIT 2019 Process Selection}},
Booktitle = {{AMCIS 2020 PROCEEDINGS}},
Year = {{2020}},
Note = {{Conference of the Association-for-Information-Systems (AMCIS), ELECTR
   NETWORK, AUG 10-14, 2020}},
Organization = {{Assoc Informat Syst}},
Abstract = {{COBIT (Control Objectives for Information and Related Technologies)
   provides a framework that supports enterprises in achieving their
   objectives in the governance and management of enterprise IT. The
   current method for the selection and prioritisation of Management
   Objectives in COBIT 2019 does not provide enterprises with the
   flexibility to customise their Design Factors, which means that it is
   not possible to adapt the framework to their context. In this research,
   we propose an alternative method to the current one provided by COBIT
   2019, which aims to solve this problem. We use a multicriteria
   decision-making method called the Analytic Hierarchy Process (AHP) in
   combination with the COBIT 2019 Design Factors to help organisations
   establish their priorities for a better implementation of COBIT 2019. In
   the evaluation step, we conduct a simulation and compare the results
   from both the current method and our proposed method against the
   decision of domain experts.}},
