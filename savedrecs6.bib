@article{ ISI:000504505100004,
Author = {Moraga, Javier A. and Quezada, Luis E. and Palominos, Pedro I. and
   Oddershede, Astrid M. and Silva, Hernan A.},
Title = {{A quantitative methodology to enhance a strategy map}},
Journal = {{INTERNATIONAL JOURNAL OF PRODUCTION ECONOMICS}},
