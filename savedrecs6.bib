@article{ ISI:000545337200002,
Author = {Krajnak, Michal},
Title = {{Financial Statement according to National or International Financial
   Reporting Standards? A Decision Analysis Case Study from the Czech
   Republic at Industrial Companies}},
Journal = {{INZINERINE EKONOMIKA-ENGINEERING ECONOMICS}},
