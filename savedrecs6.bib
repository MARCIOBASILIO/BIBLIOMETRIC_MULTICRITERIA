@article{ ISI:000500535000005,
Author = {Abdullah, Aref M. and Ali, Hesham A. and Haikal, Amira Y.},
Title = {{A reliable, TOPSIS-based multi-criteria, and hierarchical load balancing
   method for computational grid}},
Journal = {{CLUSTER COMPUTING-THE JOURNAL OF NETWORKS SOFTWARE TOOLS AND
   APPLICATIONS}},
Year = {{2019}},
Volume = {{22}},
Number = {{4, SI}},
Pages = {{1085-1106}},
