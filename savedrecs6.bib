@article{ ISI:000506856200073,
Author = {Kahraman, Cengiz and Oztaysi, Basar and Onar, Sezi Cevik},
Title = {{Single \& interval-valued neutrosophic AHP methods: Performance analysis
   of outsourcing law firms}},
Journal = {{JOURNAL OF INTELLIGENT \& FUZZY SYSTEMS}},
