@article{ ISI:000505728800004,
Author = {Mensah, Emmanuel Kwasi and Rocca, Matteo},
Title = {{Light Robust Goal Programming}},
Journal = {{MATHEMATICAL AND COMPUTATIONAL APPLICATIONS}},
