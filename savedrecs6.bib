@article{ ISI:000577453400007,
Author = {Falak, J. and Kunjan, M. and Nagaraju, Dega and Narayanan, S.},
Title = {{Evaluation of Continuous Improvement Techniques using Hybrid MCDM
   Technique under Fuzzy Environment}},
Journal = {{MATERIALS TODAY-PROCEEDINGS}},
Year = {{2020}},
Volume = {{22}},
Number = {{4}},
Pages = {{1295-1305}},
Note = {{2nd International Conference on Materials Manufacturing and Modelling
   (ICMMM), VIT Univ, Vellore, INDIA, MAR 29-31, 2019}},
Abstract = {{In this paper, an attempt is made to evaluate the continuous improvement
   techniques using hybrid MCDM techniques under fuzzy environment. The
   objective of the paper is to demonstrate the selection of best
   continuous improvement technique using Fuzzy Analytic Hierarchy Process
   (Fuzzy-AHP) and VIKOR. Lean Continuous, 5S, Kaizen, Poka-Yoke, Jidoka
   (Automation), and Kanban (Pull System) are considered as six alternative
   continuous improvement techniques. Number of Defects, Level of waste
   elimination, Cost, effective use of resources, Lead time between order
   and product delivery, and Quality of product are considered as six main
   evaluation criteria. Under each main criterion, two sub criteria are
   considered. The weights of priority of main criteria as well as the
   sub-criteria are computed using Fuzzy-AHP whereas the evaluation
   ratings/ weights of priority of alternatives with respect to each sub
   criteria are determined using Fuzzy-VIKOR. Finally, by using the weights
   of priority of main criteria and the alternatives' evaluation ratings,
   the overall priority weights of alternatives are determined. Based on
   these overall priority weights, the alternatives are ranked. From the
   findings of the research, it is concluded that the results are more
   practical and realistic. (C) 2019 Elsevier Ltd. All rights reserved.}},
