@article{ ISI:000501366000001,
Author = {Hinduja, Akshay and Pandey, Manju},
Title = {{A Distance-based Method for Computing Priorities of Intuitionistic Fuzzy
   Preference Relation and Its Application in AHP}},
Journal = {{VISION-THE JOURNAL OF BUSINESS PERSPECTIVE}},
