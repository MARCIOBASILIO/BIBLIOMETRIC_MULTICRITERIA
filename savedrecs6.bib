@article{ ISI:000518472900006,
Author = {Henao, Diana and Lopez, Fernanda and Chud Pantoja, Vivian Lorena and
   Carlos Osorio, Juan},
Title = {{Multi-criteria prioritization for affiliation to a food bank in Colombia}},
Journal = {{LOGOS CIENCIA \& TECNOLOGIA}},
Year = {{2020}},
Volume = {{12}},
Number = {{1}},
Pages = {{58-70}},
Month = {{JAN-APR}},
Abstract = {{Food waste has increased worldwide. There is also a large part of the
   world's population that does not feed properly and could take advantage
   of these foods. Food banks are a strategy to minimize food waste and
   increase food security in vulnerable populations. However, the decision
   to assign food to those who request support is a problematic situation.
   The decision must consider multiple criteria that ensure that food goes
   to the organizations or populations that need it most. A multi-criteria
   decision model is proposed that integrates the AHP and TOPSIS methods so
   that a food bank can manage the available resources in the best possible
   way. The model uses the AHP to weigh the criteria that are considered
   relevant, and TOPSIS to assess these criteria in each of the
   alternatives that request support. It is a model that allows decision
   making in a sustained and dynamic way that was positively valued by a
   food bank in Colombia.}},
