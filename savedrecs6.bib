@article{ ISI:000506856200091,
Author = {Beskese, Ahmet and Camci, Alper and Temur, Gul Tekin and Erturk, Ercan},
Title = {{Wind turbine evaluation using the hesitant fuzzy AHP-TOPSIS method with
   a case in Turkey}},
Journal = {{JOURNAL OF INTELLIGENT \& FUZZY SYSTEMS}},
