@article{ ISI:000511403800004,
Author = {de Miranda Lammoglia, Jose Antonio and Brandalise, Nilson and Hernandez,
   Cecilia Toledo},
Title = {{ANALYTICAL HIERARCHY PROCESS - BOCR APPLIED FOR THE BEST LEAN PROJECT
