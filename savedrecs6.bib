@article{ ISI:000506856200082,
Author = {Abdullah, Lazim and Goh, Chunmin and Zamri, Nurnadiah and Othman, Mahmod},
Title = {{Application of interval valued intuitionistic fuzzy TOPSIS for flood
   management}},
