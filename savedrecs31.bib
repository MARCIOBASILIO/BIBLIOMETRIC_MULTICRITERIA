@article{ ISI:000184568700009,
Author = {Yurdakul, M and Cogun, C},
Title = {{Development of a multi-attribute selection procedure for non-traditional
   machining processes}},
Journal = {{PROCEEDINGS OF THE INSTITUTION OF MECHANICAL ENGINEERS PART B-JOURNAL OF
