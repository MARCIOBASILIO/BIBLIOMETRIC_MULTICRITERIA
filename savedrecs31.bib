@inproceedings{ ISI:000174948900027,
Author = {Islam, R},
Editor = {{Koksalan, M and Zionts, S}},
Title = {{Modification of the nominal group technique by using the analytic
   hierarchy process}},
Booktitle = {{MULTIPLE CRITERIA DECISION MAKING IN THE NEW MILLENNIUM}},
