Title = {{Evaluation of three multi-criteria decision-making methods in Ecosystem
   Management}},
Booktitle = {{Proceedings of the 8th International Conference on Environmental Science
   and Technology, Vol B, Poster Presentations}},
Series = {{Proceedings of the International Conference on Environmental Science and
   Technology}},
