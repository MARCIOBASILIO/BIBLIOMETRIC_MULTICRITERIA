@article{ ISI:A1996UQ49900002,
Author = {ElMikawi, M and Mosallam, AS},
Title = {{A methodology for evaluation of the use of advanced composites in
   structural civil engineering applications}},
Journal = {{COMPOSITES PART B-ENGINEERING}},
Year = {{1996}},
Volume = {{27}},
Number = {{3-4}},
Pages = {{203-215}},
Note = {{2nd International Conference for Composites Engineering, NEW ORLEANS,
   LA, AUG 21-24, 1995}},
Abstract = {{The objective of this research was to develop a tool for evaluating the
   potential use of advanced materials in structural civil engineering
   applications. A multi-criteria systematic approach based on the
   analytical hierarchy process (AHP) was developed to assist
   decision-makers in evaluating the use of advanced materials. This study
   provides decision-makers with the means to evaluate the use of various
   structural materials based on specific applications, optimizing the use
   of limited resources. One of the key elements of the model is the
   ability to handle complex problems and alternatives with multiple
   objectives. This tool was applied to test the use of advanced composite
   materials in the repair of deteriorated and damaged bridge columns. The
   purpose of the study is to respond to the need to repair, strengthen,
