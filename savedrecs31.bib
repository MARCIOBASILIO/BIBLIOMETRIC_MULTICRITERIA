@inproceedings{ ISI:000089141200029,
Author = {Koulouri, A and Belton, V},
Editor = {{Haimes, YY and Steuer, RE}},
Title = {{Fuzzy multi-criteria DSS: Their applicability and their practical value}},
Booktitle = {{RESEARCH AND PRACTICE IN MULTIPLE CRITERIA DECISION MAKING}},
