@article{ ISI:000171381900006,
Author = {Rekiek, B and De Lit, P and Pellichero, F and L'Eglise, T and Fouda, P
   and Falkenauer, E and Delchambre, A},
Title = {{A multiple objective grouping genetic algorithm for assembly line design}},
Journal = {{JOURNAL OF INTELLIGENT MANUFACTURING}},
Year = {{2001}},
Volume = {{12}},
Number = {{5-6}},
Pages = {{467-485}},
Abstract = {{The purpose of this paper is to describe some of the main problems
   concerning assembly line design. The focus will be on the following
   steps: (1) the input data preparation, (2) the elaboration of the
   logical layout of the line, which consists in the distribution of
   operations among stations along the line and an assignment of resources
   to the different stations, (3) finally the mapping phase using a
   simulation package to check the obtained results. This work presents a
   new method to tackle the hybrid assembly line design, dealing with
