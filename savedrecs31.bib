@article{ ISI:000089114800004,
Author = {Mahmoud, MR and Garcia, LA},
Title = {{Comparison of different multicriteria evaluation methods for the Red
   Bluff diversion dam}},
Journal = {{ENVIRONMENTAL MODELLING \& SOFTWARE}},
Year = {{2000}},
Volume = {{15}},
Number = {{5}},
Pages = {{471-478}},
Abstract = {{A computer program was created to conduct a comparative study of five
   Multi-Criteria Evaluation Methods (MCEMs), each of which is capable of
   evaluating alternatives in a resource management setting. The MCEMs
   included Weighted Average (WA), Preference Ranking Organization METHod
   for Enrichment Evaluations (PROMETHEE II), Compromise Programming (CP),
   ELimination and Choice Translating REality (ELECTRE II). and Analytical
   Hierarchy Process (AHP). In this study, each of the MCEMs is applied to
   an array of different management alternatives for andromodous fish
   migration through the Red Bluff Diversion Dam (RBDD) along the
   Sacramento River in California. Results are evaluated and MCEMs are
   ranked based on (1) the consistency of results, (2) thr amount of
   interaction required by the user and (3) the degree of usability by
   technical and nontechnical professionals. It is concluded that WA is the
   method of choice when applied within the boundaries of the present
   study. (C) 2000 Elsevier Science Ltd. All rights reserved.}},
