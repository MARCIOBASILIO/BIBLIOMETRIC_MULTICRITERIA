@inproceedings{ ISI:A1997BJ71L00006,
Author = {Roy, S and Tuchinda, C},
Editor = {{Enslow, P and Desrochers, P and Bonifacio, I}},
Title = {{The importance of social and economic parameters in telecommunication
   network planning: The case of Thailand}},
Booktitle = {{GLOBAL NETWORKING `97 - 21ST CENTURY COMMUNICATIONS NETWORKS, VOL 1:
   TECHNOLOGY; ECONOMICS}},
Year = {{1997}},
Pages = {{40-45}},
Note = {{Global Networking 97 Conference - 21st-Century Communications Networks,
   CALGARY, CANADA, JUN 15-18, 1997}},
Organization = {{Int Telecommun Soc; Int Council Comp Commun}},
Abstract = {{The purpose of this study is to develop a model to determine the
   relative importance of various criteria and subcriteria in
   telecommunication network planning. A multi criteria decision making
   approach is used to build the model. The proposed model is developed
   under an Analytic Hierarchy Process (AHP) framework with multi level
   criteria and sub criteria. These three main criteria has been broken
   into five various sub criteria A survey through questionnaire on the
   pairwise comparison of criteria and sub criteria has been conducted to
   provide qualitative data which will be used for the calculation of the
   priority weights. The questionnaires were distributed to four groups of
   people who deal with telecommunication network planning. Using the
   Expert Choice software package, under the AHP framework the relative
   weights of the criteria and the sub criteria has been calculated and
   ranked The results indicate that the equipment suppliers consider
   technical parameters to be the most important in telecommunication
   network planning where as the telecommunication network operators and
   the Govt. sector persons consider the financial parameters to be more
   important. Among the social and economic parameters education and rise
   in living standard are getting prime importance.}},
