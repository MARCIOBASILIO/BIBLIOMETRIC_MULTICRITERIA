@article{ ISI:000222229900008,
Author = {Huang, CJ and Sheu, DD},
Title = {{Fuzzy multi-attribute analysis for evaluation of manufacturing
   management performance of notebook computer plants - A case study}},
Journal = {{INTERNATIONAL JOURNAL OF INDUSTRIAL ENGINEERING-THEORY APPLICATIONS AND
   PRACTICE}},
Year = {{2004}},
Volume = {{11}},
Number = {{2}},
Pages = {{178-186}},
Month = {{JUN}},
Abstract = {{In this paper a complete performance index model for manufacturing
   management for notebook computer industry is established. We suggested
   14 most important performance indices, which include productivity,
   quality, cost/profitability and cycle time at the plant level.
   Comparative data from questionnaires and interviews were collected from
   seven major manufacturers in Taiwan; their production share is over 80\%
   of the total production quantity. Three methods were used to evaluate
   their manufacturing management performance, namely, analytic hierarchy
   process (AHP), fuzzy AHP analysis and fuzzy multi-attribute
   decision-making analysis. The results indicate the effectiveness of the
   proposed index model to evaluate the management performance of
   individual manufacturers.
   Significance: Both by volume and by value, Taiwan has become the largest
   manufacturing area for notebook computers in the world. It is therefore
   very meaningful to study the manufacturing management performance of the
   notebook computer companies. Due to significant competition among the
   Taiwanese notebook manufacturers, companies are concerned about their
   relative positions in manufacturing management performances. This study
   uses three ways to evaluate the relative performance of seven major
   notebook manufacturers in Taiwan.}},
