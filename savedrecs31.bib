@article{ ISI:000180837700003,
Author = {Chen, SJ and Chen, SM},
Title = {{A new method for handling multicriteria fuzzy decision-making problems
   using FN-IOWA operators}},
Journal = {{CYBERNETICS AND SYSTEMS}},
Year = {{2003}},
Volume = {{34}},
Number = {{2}},
Pages = {{109-137}},
Month = {{MAR}},
Abstract = {{We use fuzzy numbers to extend the traditional induced ordered weight
   averaging (IOWA) operator to present the fuzzy-number IOWA (FN-IOWA)
   operator, wherein fuzzy numbers are used to describe the argument values
   and the weights of the FN-IOWA operator, and the aggregation results are
   obtained by using fuzzy-number arithmetic operations. We also present a
   new method for ranking fuzzy numbers. Based on the proposed FN-IOWA
   operator and the proposed ranking method of fuzzy numbers, we present a
   new algorithm to deal with multicriteria fuzzy decision-making problems.
   The proposed algorithm can deal with multicriteria fuzzy decision-making
   problems in a more intelligent and more flexible manner.}},
