@article{ ISI:000081364300101,
Author = {Chiadamrong, N},
Title = {{An integrated fuzzy multi-criteria decision making method for
   manufacturing strategies selection}},
Journal = {{COMPUTERS \& INDUSTRIAL ENGINEERING}},
Year = {{1999}},
Volume = {{37}},
Number = {{1-2}},
Pages = {{433-436}},
Month = {{OCT}},
Abstract = {{Potential of many new manufacturing systems is being roadblocked in many
   cases by the wrongful use of traditional justification methods. Since,
   these new systems can offer not only the financial benefits but also
   longer-term strategic benefits. Thus, piece-by-piece traditional
   justification approaches, overemphasising short-term saving in direct
   manufacturing costs rather than longer term company strategic benefits,
   need to be modified to be able to evaluate those benefits which are
   long-term and perhaps intangible. The study also presents a further step
   by introducing the concept of fuzzy set theory in overcoming the
   precision-based evaluation. Results from this approach present a better
   understanding of each alternative and an easier approach to decision
   makers for evaluating issues which cannot be precisely defined. (C) 1999
   Elsevier Science Ltd. All rights reserved.}},
