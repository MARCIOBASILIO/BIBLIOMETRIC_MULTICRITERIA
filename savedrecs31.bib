@article{ ISI:000169403000007,
Author = {Mamlook, R and Akash, BA and Nijmeh, S},
Title = {{Fuzzy sets programming to perform evaluation of solar systems in Jordan}},
Journal = {{ENERGY CONVERSION AND MANAGEMENT}},
Year = {{2001}},
Volume = {{42}},
Number = {{14}},
Pages = {{1717-1726}},
Month = {{SEP}},
Abstract = {{This paper uses fuzzy set methodology to perform the comparison between
   different solar systems for various applications. The aim of the paper
   is to determine the order in which solar systems should be given higher
   priority to be used in Jordan. The systems considered are solar
   distillation, solar water heating, solar space heating and ventilation,
   solar water pumping, photovoltaics and solar electric power production.
   They were compared according to their benefits and costs. Based on
   benefit to cost ratios, the results show that solar distillation is
   found to be the best choice and should be given the highest priority in
   terms of research and development. It is followed by the solar pond for
   electric power production, then, solar water pumping followed by solar
   space heating and photovoltaics. Finally, solar water heating may not
   need further development. (C) 2001 Elsevier Science Ltd. All rights
   reserved.}},
