@inproceedings{ ISI:000222487500061,
Author = {Pochampally, KK and Gupta, SM},
Book-Group-Author = {{ieee}},
Title = {{Efficient design and effective marketing of a reverse supply chain: A
   fuzzy logic approach}},
Booktitle = {{PROCEEDINGS OF THE 2004 IEEE INTERNATIONAL SYMPOSIUM ON ELECTRONICS \&
   THE ENVIRONMENT, CONFERENCE RECORD}},
