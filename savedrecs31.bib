@article{ ISI:000073903300006,
Author = {Amador, F and Sumpsi, JM and Romero, C},
Title = {{A non-interactive methodology to assess farmers' utility functions: An
   application to large farms in Andalusia, Spain}},
Journal = {{EUROPEAN REVIEW OF AGRICULTURAL ECONOMICS}},
