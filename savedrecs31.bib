@article{ ISI:000184446100011,
Author = {Ji, P and Jiang, R},
Title = {{Scale transitivity in the AHP}},
Journal = {{JOURNAL OF THE OPERATIONAL RESEARCH SOCIETY}},
Year = {{2003}},
Volume = {{54}},
Number = {{8}},
Pages = {{896-905}},
Month = {{AUG}},
Abstract = {{Transitivity is important in multicriteria decision-making. The analytic
   hierarchy process (AHP), as one of the widely used decision analysis
   tools, is criticized since it suffers from scale intransitivity. This
   paper first reviews and compares different scales from different
   aspects, then discusses the transitivity of AHP scales and derives a
   scale based on the transitivity, so it is naturally transitive. Besides,
   two approaches are provided to determine the scale parameter for the
   derived transitive scale. In order to deal with the transitivity
   problem, the AHP provides a consistency index for testing pairwise
   comparison consistency. So, finally, this paper proposes a consistency
   measure to reflect the judgmental inconsistency.}},
