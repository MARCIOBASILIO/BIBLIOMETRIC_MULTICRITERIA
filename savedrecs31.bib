@inproceedings{ ISI:000185639500002,
Author = {Ganoulis, J},
Editor = {{Angelakis, AN and Paranychianakis, NV and Tsagarakis, KP}},
Title = {{Evaluating alternative strategies for wastewater recycling and reuse in
   the Mediterranean area}},
