@inproceedings{ ISI:000174948900006,
Author = {Brugha, CM},
Editor = {{Koksalan, M and Zionts, S}},
Title = {{Decision-maker centred MCDM: Some empirical tests and their implications}},
Booktitle = {{MULTIPLE CRITERIA DECISION MAKING IN THE NEW MILLENNIUM}},
Series = {{Lecture Notes in Economics and Mathematical Systems}},
Year = {{2001}},
Volume = {{507}},
