@article{ ISI:000182030100010,
Author = {Aleskerov, F and Ersel, H and Yolalan, R},
Title = {{Personnel allocation among bank branches using a two-stage
   multi-criterial approach}},
Journal = {{EUROPEAN JOURNAL OF OPERATIONAL RESEARCH}},
Year = {{2003}},
Volume = {{148}},
Number = {{1}},
Pages = {{116-125}},
Month = {{JUL 1}},
Abstract = {{This paper presents a normative two-stage approach for personnel
   allocation among branches of a large-scale commercial bank. It invokes
   and generalizes upon a decision rule named ``Condorcet practical
   rule{''} first introduced by Marquis de Condorcet (1743-1794) and used
   by Geneva's parliament during two years around 1792. It lay dormant for
   more than two centuries. The approach presented is based on a
   multi-criteria ordinal ranking model, taking into account a threshold of
   insensitivity within branch performance evaluations. An underlying
   homogeneity assumption is relaxed by introducing the concept of
   homogeneous branch subsets. using a new clustering algorithm. These
   subsets are based on workload patterns within the branches. Lastly the
   method is applied to real-life data from the largest private commercial
   bank (Yapi Kredi Bank) in Turkey. (C) 2002 Elsevier Science B.V. All
   rights reserved.}},
