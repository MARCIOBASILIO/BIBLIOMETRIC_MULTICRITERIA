@article{ ISI:000179907400009,
Author = {Haralambopoulos, DA and Polatidis, H},
Title = {{Renewable energy projects: structuring a multi-criteria group
   decision-making framework}},
Journal = {{RENEWABLE ENERGY}},
Year = {{2003}},
Volume = {{28}},
Number = {{6}},
Pages = {{961-973}},
Month = {{MAY}},
Abstract = {{This paper describes an applicable group decision-making framework for
   assisting with multi-criteria analysis in renewable energy projects,
   utilizing the PROMETHEE II outranking method. The proposed framework is
   tested in a case study concerning the exploitation of a geothermal
   resource, located in the island of Chios, Greece. The presented
   structure provides a serial, decomposed agenda and enhances overall
   process transparency. Additional, innovatory elements are the
   incorporation of differing levels of resource exploitation within the
   decision framework and the direct determination of the PROMETHEE
   preference thresholds. The developed methodology provides a
   user-friendly approach, promotes the synergy between different actors,
   and could pave a way towards consensus. (C) 2002 Elsevier Science Ltd.
   All rights reserved.}},
