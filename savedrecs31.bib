@article{ ISI:A1995TK30400009,
Author = {TAKEDA, E and TABATA, Y},
Title = {{EIGENVECTOR METHOD IN MULTICRITERIA OUTRANKING RELATIONS}},
Journal = {{CYBERNETICS AND SYSTEMS}},
Year = {{1995}},
