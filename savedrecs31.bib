@article{ ISI:000186856500022,
Author = {Vaillancourt, K and Waaub, JP},
Title = {{Equity in international greenhouse gases abatement scenarios: A
   multicriteria approach}},
Journal = {{EUROPEAN JOURNAL OF OPERATIONAL RESEARCH}},
