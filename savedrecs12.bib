@article{ ISI:000439018300006,
Author = {Gonzalez-Cela, Gerardo and Bellas, Roberto and Martinez, Javier and
   Touza, Ramon and Carreno, Rafael},
Title = {{Optimal Design of Spanish Navy F-110 Frigates Combat Information Center}},
Journal = {{NAVAL ENGINEERS JOURNAL}},
Year = {{2018}},
Volume = {{130}},
Number = {{1}},
Pages = {{79-90}},
Month = {{MAR 1}},
Abstract = {{The CIC (Combat Information Center) is a naval vessel room with unique
   and characteristic elements where operators perform searching,
   monitoring, control, analysis, assistance and critical decision
   functions. The CIC processes all the vessel information in real time and
   manages, coordinates and supervises all the actions to be taken in the
   ship. The CIC layout of the last built frigates of the Spanish Navy
   (F-100 series) is based on proven design. However, the latest design
   trends observed in other navies around the world, together with the
   evolution of the combat scenarios towards low/medium intensity hybrid
   threats, makes the Spanish Navy wonder whether the current F-100 CIC
   design can be extrapolated to the future F-110 frigates. Thus, the aim
   of this paper is to provide the Spanish Navy Staff with a decision tool
   that helps in determining the optimal distribution of the future F-110
   CIC. For this purpose, an Analytic Hierarchy Process (AHP) was firstly
   developed from experts' criteria. In order to reduce the subjectivity of
   this first model, a series of simulated evacuations of the CIC personnel
   were performed according to a statistical design of experiments. The
   results of these simulations were experimentally validated and then
   added to the multicriteria decision model, thus creating a less
   subjective decision tool.}},
