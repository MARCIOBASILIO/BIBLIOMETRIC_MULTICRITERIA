@inproceedings{ ISI:000468841500018,
Author = {Naji, Mehdi Amine and Mousrij, Ahmed},
Book-Group-Author = {{IEEE}},
Title = {{Maintenance success factors identification using the fuzzy AHP}},
Booktitle = {{2018 IEEE INTERNATIONAL CONFERENCE ON TECHNOLOGY MANAGEMENT, OPERATIONS
   AND DECISIONS (ICTMOD)}},
Series = {{IEEE Vehicular Technology Conference Proceedings}},
Year = {{2018}},
Pages = {{107-112}},
Note = {{IEEE International Conference on Technology Management, Operations and
   Decisions (ICTMOD), Marrakech, MOROCCO, NOV 21-23, 2018}},
Organization = {{IEEE}},
Abstract = {{To face up with the intense competition, companies have to achieve their
   objectives in the most optimal way, in other words, they must be
   performant. However, the performance of every company is related to the
   performance level of its maintenance function. Nevertheless, maintenance
   performance is characterized by a high level of complexity due to its
   multi-criteria nature. As a result, to improve the overall maintenance
   performance with the least cost and effort, it will be necessary to
   identify and select the maintenance's key success factors. For this
   purpose, the presented paper will use the fuzzy AHP which is a
   multi-criteria decision-making method combining between the conventional
   AHP and the fuzzy logic for weighting and ranking the different
