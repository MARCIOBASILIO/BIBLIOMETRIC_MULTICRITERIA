@article{ ISI:000432510200020,
Author = {Yu, Ying and Hou, Mingshan and Liu, Bojun},
Title = {{Evaluation and selection of missile's optimal initial launch azimuth
   under side window detection using a hybrid decision making method}},
Journal = {{AEROSPACE SCIENCE AND TECHNOLOGY}},
Year = {{2018}},
Volume = {{76}},
Pages = {{217-228}},
Month = {{MAY}},
Abstract = {{Initial launch azimuth of missile is a key factor that affects the
   guidance performance, especially for missile with side window seeker.
   The aim of this study is to select the optimal initial launch azimuth of
   missile with side window seeker to achieve a high hit rate by using a
   hybrid multi -attribute decision making (MADM) method. The hybrid
   decision-making method combines interval numbers and combination weight
   with Technique for Order Preference by Similarity to Ideal Solution
   (TOPSIS). The specific innovations of the hybrid MADM method includes
   developing a new method to measure the distance between interval
   numbers, a new normalization method based on ideal interval for
   attributes, and a new combination weight method based on attribute
   importance. It is found that the decision results are robust, have a
   reliability of up to 90\% or even 95\%, and the best azimuths of missile
   can achieve a hit rate as high as 90\% or even 100\%. This indicates
   that there is a great possibility of solving the guidance problem of
   side window detection based on proportional navigation by applying the
   optimal initial launch azimuth of missile. The optimal results can
   provide reference for the fire control system. (C) 2018 Elsevier Masson
   SAS. All rights reserved.}},
