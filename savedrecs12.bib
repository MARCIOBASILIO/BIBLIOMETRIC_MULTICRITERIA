@article{ ISI:000435195000003,
Author = {Pramanik, Surapati and Dalapati, Shyamal and Alam, Shariful and
   Smarandache, Florentin and Roy, Tapan Kumar},
Title = {{NC-Cross Entropy Based MADM Strategy in Neutrosophic Cubic Set
   Environment}},
Journal = {{MATHEMATICS}},
Year = {{2018}},
Volume = {{6}},
Number = {{5}},
Month = {{MAY}},
Abstract = {{The objective of the paper is to introduce a new cross entropy measure
   in a neutrosophic cubic set (NCS) environment, which we call NC-cross
   entropy measure. We prove its basic properties. We also propose weighted
   NC-cross entropy and investigate its basic properties. We develop a
   novel multi attribute decision-making (MADM) strategy based on a
   weighted NC-cross entropy measure. To show the feasibility and
   applicability of the proposed multi attribute decision-making strategy,
   we solve an illustrative example of the multi attribute decision-making
   problem.}},
