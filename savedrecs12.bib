@article{ ISI:000435185200039,
Author = {Abdel-Basset, Mohamed and Mohamed, Mai and Smarandache, Florentin},
Title = {{An Extension of Neutrosophic AHP-SWOT Analysis for Strategic Planning
   and Decision-Making}},
