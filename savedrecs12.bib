@article{ ISI:000430391700009,
Author = {Bai, Chengzu and Zhang, Ren and Shen, Shuang and Huang, Chaofan and Fan,
   Xin},
Title = {{Interval-valued probabilistic linguistic term sets in multi-criteria
   group decision making}},
Journal = {{INTERNATIONAL JOURNAL OF INTELLIGENT SYSTEMS}},
