@article{ ISI:000424952200017,
Author = {Shahabi, R. Shakoor and Basiri, M. H. and Kahag, Mahdi Rashidi},
Title = {{Ranking of productivity improvement strategies in Iran mineral sector
   based on integrated SWOT-FAHP-FTOPSIS analysis}},
Journal = {{ARABIAN JOURNAL OF GEOSCIENCES}},
