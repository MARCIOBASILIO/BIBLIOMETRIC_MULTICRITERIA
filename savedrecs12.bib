Author = {Gurbuz, Feyza and Toga, Gulhan},
Book-Group-Author = {{IEEE}},
Title = {{Usage Of The Facial Action Coding System To Predict Costumer Gender
   Profile: A Neuro Marketing Application In TURKEY<bold> </bold>}},
Booktitle = {{2018 2ND INTERNATIONAL SYMPOSIUM ON MULTIDISCIPLINARY STUDIES AND
   INNOVATIVE TECHNOLOGIES (ISMSIT)}},
