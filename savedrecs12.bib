@article{ ISI:000446157500004,
Author = {Tuysuz, Fatih},
Title = {{Simulated Hesitant Fuzzy Linguistic Term Sets-Based Approach for
   Modeling Uncertainty in AHP Method}},
Journal = {{INTERNATIONAL JOURNAL OF INFORMATION TECHNOLOGY \& DECISION MAKING}},
Year = {{2018}},
Volume = {{17}},
Number = {{3}},
Pages = {{801-817}},
Month = {{MAY}},
Abstract = {{One of the important points in decision-making is representing the
   uncertainty. There have been developed approaches which are based on
   either fuzzy sets theory or probability theory to model the inherent
   uncertainty and imprecision in the pairwise comparison process of
   analytic hierarchy process (AHP). This study proposes a hybrid approach
   that integrates fuzzy sets theory and simulation technique into AHP
   method to better represent the uncertainty in decision-making process.
   The aim of the proposed approach is to benefit the advantages of both
   probability theory and fuzzy sets theory which may provide a new
   paradigm for modeling uncertainty in multi-criteria decision-making
   (MCDM). An illustrative example is also presented to show the
   applicability of the proposed approach.}},
