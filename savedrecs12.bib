Title = {{The Framework of Risk-based Decision Support System (DSS plus R) for
   Forensic Investigation in Detecting Human Cadaver of Clandestine Graves}},
Booktitle = {{INTERNATIONAL CONFERENCE ON DESIGN, ENGINEERING AND COMPUTER SCIENCES}},
Series = {{IOP Conference Series-Materials Science and Engineering}},
