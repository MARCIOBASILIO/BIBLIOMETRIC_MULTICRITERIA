@article{ ISI:000461277900009,
Author = {Pramanik, Surapati and Mallick, Rama},
Title = {{VIKOR based MAGDM Strategy with Trapezoidal Neutrosophic Numbers}},
Journal = {{NEUTROSOPHIC SETS AND SYSTEMS}},
Year = {{2018}},
Volume = {{22}},
Pages = {{118-130}},
Abstract = {{VIseKriterijumska Optimizacija I Kompromisno Resenje (VIKOR) is a
   popular strategy for multi-attribute decision making (MADM). We extend
   the VIKOR strategy for MAGDM problems in trapezoidal neutrosophic number
   environment. In decision making situation, single-valued trapezoidal
   neutrosophic numbers are employed to express the attribute values. Then
   we develop an extended VIKOR strategy to deal with MAGDM in
   single-valued trapezoidal neutrosophic number environment. The influence
   of decision-making mechanism coefficient is presented. To illustrate and
   validate the proposed VIKOR strategy, an illustrative numerical example
   of MAGDM problem is solved in trapezoidal neutrosophic number
   environment.}},
