@article{ ISI:000413177900007,
Author = {Ishizaka, Alessio and Siraj, Sajid},
Title = {{Are multi-criteria decision-making tools useful? An experimental
   comparative study of three methods}},
Journal = {{EUROPEAN JOURNAL OF OPERATIONAL RESEARCH}},
Year = {{2018}},
Volume = {{264}},
Number = {{2}},
Pages = {{462-471}},
