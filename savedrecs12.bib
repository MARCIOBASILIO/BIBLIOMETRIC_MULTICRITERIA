@inproceedings{ ISI:000495071900346,
Author = {Huang, Chan and Deng, Xinyang and Jiang, Wen and Liu, Zhunga},
Book-Group-Author = {{IEEE}},
Title = {{A New Method for OWA Aggregation of Interval Values in Multi-Criteria
   Decision Making}},
