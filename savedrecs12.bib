@inproceedings{ ISI:000461023800023,
Author = {Liu, Chang},
Editor = {{Li, X and Xu, X}},
Title = {{Supplier Selection Evaluation of Shipbuilding Enterprises Based on
   Entropy Weight and Multi-attribute Decision Making}},
Booktitle = {{PROCEEDINGS OF THE FIFTH INTERNATIONAL FORUM ON DECISION SCIENCES}},
