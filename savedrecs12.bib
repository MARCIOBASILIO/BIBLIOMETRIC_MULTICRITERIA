@article{ ISI:000434397500150,
Author = {Hameed, Shahul K. M. and Kumar, Arun M. and Dhanapal, C. K.},
Title = {{SELECTION OF A SUITABLE METHOD FOR THE PREPARATION OF TADALAFIL MOUTH
   DISSOLVING TABLETS: MULTI-CRITERIA DECISION MAKING APPROACH}},
Journal = {{INDO AMERICAN JOURNAL OF PHARMACEUTICAL SCIENCES}},
