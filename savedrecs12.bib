@article{ ISI:000428221500004,
Author = {Ting, Choo-Yee and Ho, Chiung Ching and Yee, Hui Jia and Matsah, Wan
   Razali},
Title = {{Geospatial Analytics in Retail Site Selection and Sales Prediction}},
