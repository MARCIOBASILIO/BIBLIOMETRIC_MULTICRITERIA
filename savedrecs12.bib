Title = {{Urban Vulnerability Assessment: Towards a Cross-Scale Spatial
   Multi-criteria Approach}},
Booktitle = {{COMPUTATIONAL SCIENCE AND ITS APPLICATIONS - ICCSA 2018, PT III}},
Series = {{Lecture Notes in Computer Science}},
Year = {{2018}},
