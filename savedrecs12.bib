@article{ ISI:000433168800013,
Author = {Kishore, A. H. Nandhu and Jayanthi, V. E.},
Title = {{Multi Criteria Decision Making Methods to Predict the Prevalence of
   Coronary Artery Disease}},
Journal = {{JOURNAL OF MEDICAL IMAGING AND HEALTH INFORMATICS}},
Year = {{2018}},
Volume = {{8}},
Number = {{4}},
Pages = {{719-726}},
Month = {{MAY}},
Abstract = {{A novel medical diagnosis system is proposed to predict the prevalence
   of Coronary Artery Disease (CAD) using the inherent mathematical
   capabilities of Multi Criteria Decision Making (MCDM) methods. Analytic
   Hierarchy Processing (AHP), Technique for Order Preference by Similarity
   to Ideal Solution (TOPSIS) and Simple Additive Weighting (SAW) method
   are chosen for implementing the MCDM system. AHP is used to analyze the
   hierarchy structure of CAD diagnosis and determine the attribute weights
   based on the individual contribution of risk attributes for disease
   diagnosis as prescribed by an experienced cardiologist. The extended
   TOPSIS and SAW methods are designed for diagnosis of CAD using attribute
   weights obtained from AHP. The performance of the proposed system is
   evaluated using Cleveland heart disease dataset available online in
   University of California at Irvine (UCI) machine learning repository.
   MCDM methods provided accurate prediction results on the diagnosis of
   CAD similar to the medical experts.}},
