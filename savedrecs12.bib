@article{ ISI:000432042900002,
Author = {Yu, Xi and Zhang, Haiqing and Bouras, Abdelaziz and Ouzrout, Yacine and
   Sekhari, Aicha},
Title = {{Multi-Criteria Decision Making for PLM Maturity Analysis based on an
   Integrated Fuzzy AHP and VIKOR Methodology}},
Journal = {{JOURNAL OF ADVANCED MANUFACTURING SYSTEMS}},
