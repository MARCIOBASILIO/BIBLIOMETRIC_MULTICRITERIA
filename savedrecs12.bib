
@article{ ISI:000432236100019,
Author = {Mistage, O. and Bilotta, P.},
Title = {{Decision support method for GHG emission management in industries}},
Journal = {{INTERNATIONAL JOURNAL OF ENVIRONMENTAL SCIENCE AND TECHNOLOGY}},
