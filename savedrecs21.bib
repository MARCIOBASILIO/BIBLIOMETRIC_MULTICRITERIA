@article{ ISI:000346954500022,
Author = {Rostamzadeh, Reza and Govindan, Kannan and Esmaeili, Ahmad and Sabaghi,
   Mandi},
Title = {{Application of fuzzy VIKOR for evaluation of green supply chain
   management practices}},
