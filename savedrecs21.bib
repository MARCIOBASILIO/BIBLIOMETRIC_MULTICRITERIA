@inproceedings{ ISI:000380768300010,
Author = {Opricovic, Marija},
Editor = {{AlShammari, M and Masri, H}},
Title = {{VIKOR Method with Application to Borrowing Terms Selection}},
Booktitle = {{MULTIPLE CRITERIA DECISION MAKING IN FINANCE, INSURANCE AND INVESTMENT}},
