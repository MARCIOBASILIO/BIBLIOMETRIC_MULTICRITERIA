@inproceedings{ ISI:000380587100226,
Author = {Rahman, Mohammad Anwar and Ali, Ahad},
Book-Group-Author = {{IEEE}},
Title = {{Defining Service Quality Index Preferences in a Service Industry Using
   Analytic Hierarchy Process}},
