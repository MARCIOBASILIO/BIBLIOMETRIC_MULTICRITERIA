@article{ ISI:000356904100028,
Author = {Mardani, Abbas and Jusoh, Ahmad and Zavadskas, Edmundas Kazimieras},
Title = {{Fuzzy multiple criteria decision-making techniques and applications -
   Two decades review from 1994 to 2014}},
Journal = {{EXPERT SYSTEMS WITH APPLICATIONS}},
