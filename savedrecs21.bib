@article{ ISI:000346843900012,
Author = {Cebi, Ferhan and Otay, Irem},
Title = {{Multi-Criteria and Multi-Stage Facility Location Selection under
   Interval Type-2 Fuzzy Environment: A Case Study for a Cement Factory}},
Journal = {{INTERNATIONAL JOURNAL OF COMPUTATIONAL INTELLIGENCE SYSTEMS}},
Year = {{2015}},
Volume = {{8}},
Number = {{2}},
Pages = {{330-344}},
Month = {{MAR 4}},
Abstract = {{The study proposes a comprehensive and systematic approach for
   multi-criteria and multi-stage facility location selection problem. To
   handle with high and more uncertainty in the evaluation and selection
   processes, the problem is solved by using multi-criteria decision making
   technique with interval Type-2 fuzzy sets. The study contributes the
   facility location selection literature by introducing the application of
