@article{ ISI:000349361200022,
Author = {Xi, Xi and Poh, Kim Leng},
Title = {{A Novel Integrated Decision Support Tool for Sustainable Water Resources
   Management in Singapore: Synergies Between System Dynamics and Analytic
   Hierarchy Process}},
