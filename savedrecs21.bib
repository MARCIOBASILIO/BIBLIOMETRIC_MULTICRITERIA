@article{ ISI:000446932700004,
Author = {Labreuche, Christophe and Mayag, Brice and Duqueroie, Bertrand},
Title = {{Extension of the MACBETH approach to elicit an ordered weighted average
   operator}},
Journal = {{EURO JOURNAL ON DECISION PROCESSES}},
Year = {{2015}},
Volume = {{3}},
Number = {{1-2, SI}},
Pages = {{65-105}},
Month = {{JUN}},
Abstract = {{In the context of multi-criteria decision making, we propose a new
   approach to construct an ordered weighted average (OWA) operator in a
   general case, a convex OWA operator and a trapezoidal OWA operator. Our
   methodology, based on an interactive decision process, extends the
   MACBETH methodology and takes as input some preferences on a specific
   set of alternatives called binary alternatives. For each operator
   studied, we analyze the inconsistencies which can occur when the
   preferences given by the decision maker are not representable by the
