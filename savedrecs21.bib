@inproceedings{ ISI:000363796700010,
Author = {Sobrie, Olivier and Mousseau, Vincent and Pirlot, Marc},
Editor = {{Walsh, T}},
Title = {{Learning the Parameters of a Non Compensatory Sorting Model}},
Booktitle = {{ALGORITHMIC DECISION THEORY, ADT 2015}},
