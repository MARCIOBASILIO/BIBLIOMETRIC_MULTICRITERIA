@article{ ISI:000362781300001,
Author = {Younes, Mohammad K. and Basri, N. E. Ahmad and Nopiah, Z. M. and Basri,
   H. and Abushammala, Mohammed F. M.},
Title = {{Use of a Combination of MRSS-ANP for Making an Innovative Landfill
   Siting Decision Model}},
Journal = {{MATHEMATICAL PROBLEMS IN ENGINEERING}},
Year = {{2015}},
Volume = {{2015}},
Abstract = {{Landfill siting is a complex, multicriteria decision-making problem that
   needs an extensive evaluation of environmental, social, land use, and
   operational criteria. Integration of a median ranked sample set (MRSS)
   and an analytic network process (ANP) has been implemented to rank the
   associated criteria and select a suitable landfill site. It minimizes
   the uncertainty and the subjectivity of human judgments. Four groups of
   experts with different backgrounds participated in this study, and each
   group contained four experts. The respondent preferences were ranked in
   a 4-by-4 matrix to obtain the judgment sets for the MRSS. These sets
   were subsequently analyzed using ANP to obtain the priorities in the
   landfill siting criteria. The results show that land topology and
   distance from surface water are the most influential factors, with
   priorities of 0.18 and 0.17, respectively. The proposed integrated model
   may become a promising tool for the environmental planners and decision
   makers.}},
