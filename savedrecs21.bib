@article{ ISI:000347605100023,
Author = {Kadzinski, Milosz and Ciomek, Krzysztof and Slowinski, Roman},
Title = {{Modeling assignment-based pairwise comparisons within integrated
   framework for value-driven multiple criteria sorting}},
