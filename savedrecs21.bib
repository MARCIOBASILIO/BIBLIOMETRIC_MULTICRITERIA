Author = {Li, Jian-jun and Liu, Li-wei},
Editor = {{Qi, E and Su, Q and Shen, J and Wu, F and Dou, R}},
Title = {{An MCDM Model Based on KL-AHP and TOPSIS and its Application to Weapon
   System Evaluation}},
Booktitle = {{PROCEEDINGS OF THE 5TH INTERNATIONAL ASIA CONFERENCE ON INDUSTRIAL
   ENGINEERING AND MANAGEMENT INNOVATION (IEMI2014)}},
