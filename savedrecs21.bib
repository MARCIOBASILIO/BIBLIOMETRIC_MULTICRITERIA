@article{ ISI:000384668800003,
Author = {Carmen Carnero, Maria},
Title = {{METHODOLOGY FOR SELECTION OF OPTIMAL PORTFOLIO IN MAINTENANCE
   DEPARTMENTS}},
Journal = {{INTERNATIONAL JOURNAL OF INDUSTRIAL ENGINEERING-THEORY APPLICATIONS AND
   PRACTICE}},
