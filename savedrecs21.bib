@article{ ISI:000350217700033,
Author = {Kuo, Tsai Chi and Hsu, Chia-Wei and Li, Jie-Ying},
Title = {{Developing a Green Supplier Selection Model by Using the DANP with VIKOR}},
Journal = {{SUSTAINABILITY}},
