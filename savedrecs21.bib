@inproceedings{ ISI:000380430700068,
Author = {Chen, L. and Pan, W.},
Editor = {{Chong, WO and Chang, J and Parrish, K and Berardi, U}},
Title = {{A BIM-integrated Fuzzy Multi-criteria Decision Making Model for
   Selecting Low-carbon Building Measures}},
