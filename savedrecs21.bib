Author = {Xiong Xianming and Lu Yi and Nazam, Muhammad},
Editor = {{Yingying, S and Guiran, C and Zhen, L}},
Title = {{A New Method of Multi-criteria Group Decision Making Based on
   Hesitant-valued Intuitionistic Fuzzy and Osculating Value}},
