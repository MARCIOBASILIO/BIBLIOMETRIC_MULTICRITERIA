@inproceedings{ ISI:000366123600030,
Author = {Cimler, Richard and Mls, Karel and Gavalec, Martin},
Editor = {{Nunez, M and Nguyen, NT and Camacho, D and Trawinski, B}},
Title = {{Decision Support Smartphone Application Based on Interval AHP Method}},
