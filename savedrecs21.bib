@inproceedings{ ISI:000380496500031,
Author = {Wiratno, Stefanus Eko and Latiffianti, Effi and Wirawan, Kevin Karmadi},
Editor = {{Pujawan, N and Ciptomulyono, U and Baihaqi, I and Santosa, B}},
Title = {{Selection of business funding proposals using analytic network process:
   a case study at a venture capital company}},
Booktitle = {{INDUSTRIAL ENGINEERING AND SERVICE SCIENCE 2015, IESS 2015}},
Series = {{Procedia Manufacturing}},
Year = {{2015}},
Volume = {{4}},
Pages = {{237-243}},
Note = {{International Conference on Industrial Engineering and Service Science
   (IESS), Yogyakarta, INDONESIA, SEP 01-02, 2015}},
Organization = {{Islam Univ Indonesia; Rajamangala Univ Technol Suvarnabhumi}},
Abstract = {{Financing companies are regularly faced with decision-making in the
   business process. They are often challenged to carefully select the
   right proposals as it is one among decision-makings that contribute to
   the company financial performance. Unfortunately, many venture capital
   companies in Indonesia perform the selection process based on some quick
   analysis methods while the criteria to be considered are multiple. Some
   of the criteria are possibly dependent to each other. This research
   attempted to apply Analytic Network Process (ANP) to support the
   decision-making process in the observed company. As ANP does not work
   optimally in the case where the number of alternatives is large, we
   performed an initial selection process by employing two criteria as
   filters to reduce the available alternatives. The process continued by
   applying ANP to the ten selected criteria from which ranking of each
   alternative was produced. One-by-one elimination rank was also used as
   an approach in the analysis. The result has shown that ANP was capable
   of providing a good and structured decision preview in the
   decision-making process. Analysis also found that when several
   alternatives were removed from the calculation, the resulted ranks also
   changed. (C) 2015 The Authors. Published by Elsevier B.V.}},
