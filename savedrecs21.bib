Editor = {{Rozic, N and Begusic, D and Saric, M and Solic, P}},
Title = {{Fuzzy MADM Based Vertical Handover Algorithm for Enhancing Network
   Performances}},
Booktitle = {{2015 23RD INTERNATIONAL CONFERENCE ON SOFTWARE, TELECOMMUNICATIONS AND
   COMPUTER NETWORKS (SOFTCOM)}},
Series = {{International Conference on Software in Telecommunications and Computer
   Networks}},
