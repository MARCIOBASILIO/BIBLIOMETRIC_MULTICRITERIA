Title = {{A Fuzzy Decision Support Approach for Model-Based Tradeoff Analysis of
   Non-Functional Requirements}},
Booktitle = {{2015 12TH INTERNATIONAL CONFERENCE ON INFORMATION TECHNOLOGY - NEW
   GENERATIONS}},
Year = {{2015}},
Pages = {{112-121}},
