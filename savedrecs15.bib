@article{ ISI:000394594600004,
Author = {Lourenco, Joao Carlos and Soares, Joao Oliveira and Bana e Costa, Carlos
   A.},
Title = {{PORTFOLIO ROBUSTNESS EVALUATION: A CASE STUDY IN THE ELECTRICITY SECTOR}},
Journal = {{TECHNOLOGICAL AND ECONOMIC DEVELOPMENT OF ECONOMY}},
Year = {{2017}},
Volume = {{23}},
Number = {{1}},
Pages = {{59-80}},
Abstract = {{Managers continually face the task of allocating resources to projects
   when there is not enough money to fund them all. Portfolio Robustness
   Evaluation (PROBE) is a multicriteria decision support system developed
   to help managers to perform that difficult task. This paper presents a
   PROBE model, developed for an electricity distribution company, to
   select the best portfolio of projects, subject to budget constraints for
   different types of projects and various organisational units in multiple
   time periods. Projects requiring large-scale investments are analysed
   separately from the small-scale projects. The robustness of the selected
   portfolio of large-scale projects is analysed in an iterative process
   where broader uncertainty ranges are considered for the values of the
   projects, and also when an environmental impact criterion is added to
   the evaluation model.}},
Publisher = {{VILNIUS GEDIMINAS TECH UNIV}},
Address = {{SAULETEKIO AL 11, VILNIUS, LT-10223, LITHUANIA}},
