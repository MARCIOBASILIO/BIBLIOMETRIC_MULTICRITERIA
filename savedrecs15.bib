@article{ ISI:000389089700023,
Author = {Ozceylan, Eren and Cetinkaya, Cihan and Erbas, Mehmet and Kabak, Mehmet},
Title = {{Logistic performance evaluation of provinces in Turkey: A GIS-based
   multi-criteria decision analysis}},
Journal = {{TRANSPORTATION RESEARCH PART A-POLICY AND PRACTICE}},
Year = {{2016}},
Volume = {{94}},
Pages = {{323-337}},
Month = {{DEC}},
Abstract = {{Logistics performance evaluation of provinces is considered in this
   study. To do so, a three step solution approach is developed: (i)
   determination of 16 geographic and economic indicators, (ii) using
   geographic information system to assign a logistics score and (iii)
   prioritizing the indicators and ranking the provinces using
   multi-criteria decision analysis tools. Proposed methodology is applied
   to 81 provinces in Turkey as a case study. Results show that the
   provinces of Istanbul, Izmir, and Hatay are the pioneers. The proposed
   methodology provides the ability to analyze the impacts of indicators on
   logistics performance and create a logistics performance map of
   countries. (C) 2016 Elsevier Ltd. All rights reserved.}},
