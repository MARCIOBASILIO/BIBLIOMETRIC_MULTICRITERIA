@inproceedings{ ISI:000417344100035,
Author = {Molnar, Viktor and Horvath, Dora Diana},
Editor = {{Nemec, R and Chytilova, L}},
Title = {{Determination of Coefficients of Multi-Attribute Utility Function with
   Attribute Breakdown}},
