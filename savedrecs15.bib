@article{ ISI:000385937000001,
Author = {Yang, Wu-E and Ma, Chao-Qun and Han, Zhi-Qiu},
Title = {{Linguistic multi-criteria decision-making with representing semantics by
   programming}},
Journal = {{INTERNATIONAL JOURNAL OF SYSTEMS SCIENCE}},
Year = {{2017}},
Volume = {{48}},
Number = {{2}},
Pages = {{225-235}},
Abstract = {{A linguistic multi-criteria decision-making method is introduced. In
   this method, a maximising discrimination programming assigns the
   semanteme values to linguistic variables to represent their semantics.
   Incomplete preferences from using linguistic information are expressed
   by the constraints of the model. Such assignment can amplify the
   difference between alternatives. Thus, the discrimination of the
   decision model is increased, which facilitates the decision-maker to
   rank or order the alternatives for making a decision. We also discuss
   the parameter setting and its influence, and use an application example
   to illustrate the proposed method. Further, the results with three types
   of semantic structure highlight the ability of the method in handling
   different semantic structures.}},
