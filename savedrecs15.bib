@article{ ISI:000409170600003,
Author = {Chakraborty, Shankar and Bandhopadyay, Siddhartha},
Title = {{Cotton fibre selection and grading - a PROMETHEE-GAIA-based approach}},
Journal = {{INTERNATIONAL JOURNAL OF CLOTHING SCIENCE AND TECHNOLOGY}},
