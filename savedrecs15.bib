@inproceedings{ ISI:000455059100004,
Author = {Mascle, Christian},
Editor = {{Maier, A and Skec, S and Kim, H and Kokkolaras, M and Oehmen, J and Fadel, G and Salustri, F and VanDerLoos, M}},
Title = {{DESIGN FOR COMPLEX PRODUCT REBIRTH OR HOW TO PROTECT RESOURCES}},
Booktitle = {{DS87-1 PROCEEDINGS OF THE 21ST INTERNATIONAL CONFERENCE ON ENGINEERING
   DESIGN (ICED 17), VOL 1: RESOURCE SENSITIVE DESIGN, DESIGN RESEARCH
   APPLICATIONS AND CASE STUDIES}},
Series = {{International Conference on Engineering Design}},
