Editor = {{Li, Q and Wang, L and Zhou, M and Sun, L and Qiu, S and Liu, H}},
Title = {{An Vertical Handover Introduced by Adaptive Mechanism Based on Fuzzy
   Analytic Hierarchy Process for Heterogeneous Networks}},
Booktitle = {{2017 10TH INTERNATIONAL CONGRESS ON IMAGE AND SIGNAL PROCESSING,
   BIOMEDICAL ENGINEERING AND INFORMATICS (CISP-BMEI)}},
Year = {{2017}},
