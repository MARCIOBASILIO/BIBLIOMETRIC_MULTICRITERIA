Editor = {{EsztergarKiss, D and Matrai, T and Toth, J and Varga, I}},
Title = {{Combining Analytic Hierarchy Process (AHP) with role-playing games for
   stakeholder engagement in complex transport decisions}},
Booktitle = {{20TH EURO WORKING GROUP ON TRANSPORTATION MEETING, EWGT 2017}},
Series = {{Transportation Research Procedia}},
