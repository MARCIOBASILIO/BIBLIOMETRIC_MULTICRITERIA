@inproceedings{ ISI:000452901603059,
Author = {Wang, Junyao and Sun, Taiwei and Deng, Shuai and Li, Kaixiang and Zhao,
   Jun and Gao, Liuhua and Wang, Yongzhen},
Editor = {{Yan, J and Wu, J and Li, H}},
Title = {{A resilience analysis on energy system: a preliminary case study for
   solar-assisted CCS}},
