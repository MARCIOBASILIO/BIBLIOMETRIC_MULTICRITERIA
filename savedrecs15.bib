Editor = {{Oancea, G and Dragoi, MV}},
Title = {{A multi-criteria decision making approach for supplier selection in the
   flexible packaging industry}},
Booktitle = {{4TH INTERNATIONAL CONFERENCE ON COMPUTING AND SOLUTIONS IN MANUFACTURING
   ENGINEERING 2016 - COSME'16}},
Series = {{MATEC Web of Conferences}},
Year = {{2017}},
