@article{ ISI:000414196200003,
Author = {Rashahmadi, Samrand and Hasanzadeh, Rezgar and Mosalman, Sina},
Title = {{Improving the Mechanical Properties of Poly Methyl Methacrylate
   Nanocomposites for Dentistry Applications Reinforced with Different
   Nanoparticles}},
Journal = {{POLYMER-PLASTICS TECHNOLOGY AND ENGINEERING}},
Year = {{2017}},
Volume = {{56}},
Number = {{16}},
Pages = {{1730-1740}},
Abstract = {{Properties of poly methyl methacrylate are improved using different
   nanoparticles for denture applications and the best combination is
   selected using multi-criteria decision-making methods. For these
   purposes, poly methyl methacrylate is melt compounded with TiO2, SiO2,
   and Al2O3 nanoparticles and then injection molded. The results of
   mechanical tests revealed that by addition of TiO2 and SiO2, the impact
   strengths of poly methyl methacrylate were increased 229 and 62\%,
   respectively. Also, the results indicated a significant improvement in
   Young's modulus and hardness. The implementation of multi-criteria
   decision-making methods illustrated that TiO2 nanoparticles are the best
   candidate for improving the properties of poly methyl methacrylate for
   dental applications.
