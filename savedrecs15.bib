@article{ ISI:000403929500003,
Author = {de Oliveira Neto, Geraldo Cardoso and de Oliveira, Julio Cezar and
   Henriques Librantz, Andre Felipe},
Title = {{Selection of Logistic Service Providers for the transportation of
   refrigerated goods}},
Journal = {{PRODUCTION PLANNING \& CONTROL}},
Year = {{2017}},
Volume = {{28}},
Number = {{10}},
Pages = {{813-828}},
Abstract = {{The aim of this article is to propose a decision model based on Analytic
   Hierarchy Process (AHP) for the evaluation and selection of Logistics
   Service Providers (LSPs) for a refrigerated products company. The
   results indicated that quality, responsiveness and reliability are the
   most important criteria for selecting an LSP for the transportation of
   perishable refrigerated goods, considering contractor's profile, which
   is based on protection of its brand and reputation in the market. In
   addition, sensitivity analysis is performed to verify priority ranking
   stability. Therefore, this research illustrates how AHP model could be
   implemented to help the decision-making process for selecting
   third-party services in logistics for perishable refrigerated goods
   effectively.}},
