@article{ ISI:000393071300002,
Author = {Jung, Uk and Yim, Seongmin and Lim, Sunguk and Kim, Chongman},
Title = {{BRINGING KANO'S PERSPECTIVE TO AHP: THE 2D-AHP DECISION MODEL}},
Journal = {{MANAGEMENT AND PRODUCTION ENGINEERING REVIEW}},
