@article{ ISI:000423749300007,
Author = {Li, Zongmin and Liang, Xuedong and Yin, Haidong},
Title = {{A MULTI-CRITERIA GROUP DECISION MAKING METHOD FOR ELEVATOR SAFETY
   EVALUATION WITH HESITANT FUZZY JUDGMENTS}},
Journal = {{APPLIED AND COMPUTATIONAL MATHEMATICS}},
Year = {{2017}},
Volume = {{16}},
Number = {{3}},
Pages = {{296-312}},
Abstract = {{Elevator safety evaluation is without a doubt necessary, but difficult
   to perform. Elevator safety evaluation involves not only objective
   criteria, such as the durable years and running frequency, but also
   subjective criteria, such as the inspection results of critical
   components. When express their opinions, experts use different
   linguistic terms because of cognitive styles differences. Further, the
   weights are difficult to determine. Motivated by these difficulties,
   this paper first proposes evaluation index system of elevator safety,
   which concurrently considers objective and subjective criteria of
   management status and inspection of critical components. Then, a
   hesitant fuzzy judgment description is proposed to increase the
   flexibility of the linguistic information. A distance-based method and a
   weighted averaging operator are applied to determine the experts' and
   criteria weights, respectively. After that, a TOPSIS-based aggregation
   method is applied to obtain the evaluation results. Finally, a practical
   case study is used to test the feasibility.}},
