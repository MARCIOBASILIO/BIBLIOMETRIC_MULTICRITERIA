@article{ ISI:000390494800094,
Author = {Jayaraman, Raja and Colapinto, Cinzia and La Torre, Davide and Malik,
   Tufail},
Title = {{A Weighted Goal Programming model for planning sustainable development
   applied to Gulf Cooperation Council Countries}},
