@article{ ISI:000415593600013,
Author = {Wang, Ze-Ling and You, Jian-Xin and Liu, Hu-Chen and Wu, Song-Man},
Title = {{Failure Mode and Effect Analysis using Soft Set Theory and COPRAS Method}},
Journal = {{INTERNATIONAL JOURNAL OF COMPUTATIONAL INTELLIGENCE SYSTEMS}},
