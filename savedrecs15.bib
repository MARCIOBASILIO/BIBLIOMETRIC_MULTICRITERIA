@inproceedings{ ISI:000458522300013,
Author = {Donais, Francis Marleau and Abi-Zeid, Irene and Lavoie, Roxane},
Editor = {{Schoop, M and Kilgour, DM}},
Title = {{Building a Shared Model for Multi-criteria Group Decision Making
   Experience from a Case Study for Sustainable Transportation Planning in
   Quebec City}},
Booktitle = {{GROUP DECISION AND NEGOTIATION: A SOCIO-TECHNICAL PERSPECTIVE}},
Series = {{Lecture Notes in Business Information Processing}},
