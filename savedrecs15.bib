@article{ ISI:000456364500003,
Author = {Kumar, Rupesh},
Title = {{Multi-criteria decision and multivariate statistical approaches improve
   olive supply chains: a review}},
Journal = {{INTERNATIONAL JOURNAL OF VALUE CHAIN MANAGEMENT}},
Year = {{2017}},
Volume = {{8}},
Number = {{3}},
Pages = {{219-246}},
Abstract = {{The olive oil supply chain stages have been studied extensively in both
   EU and non-EU countries. Various decision-making approaches have been
   proposed to tackle the problem. This study reviews different individual
   and integrated approaches like multi-criteria decision analysis (MCDA),
