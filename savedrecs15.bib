@inproceedings{ ISI:000418854600009,
Author = {Cherif, Hedi and Ladhari, Talel},
Editor = {{Abraham, A and Haqiq, A and Alimi, AM and Mezzour, G and Rokbani, N and Muda, AK}},
Title = {{A New Hybrid Multi-criteria ABC Inventory Classification Model Based on
   Differential Evolution and Topsis}},
Booktitle = {{PROCEEDINGS OF THE 16TH INTERNATIONAL CONFERENCE ON HYBRID INTELLIGENT
   SYSTEMS (HIS 2016)}},
Series = {{Advances in Intelligent Systems and Computing}},
