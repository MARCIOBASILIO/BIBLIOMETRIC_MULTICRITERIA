@article{ ISI:000393085500017,
Author = {Yazdani, Morteza and Hashemkhani Zolfani, Sarfaraz and Zavadskas,
   Edmundas Kazimieras},
Title = {{NEW INTEGRATION OF MCDM METHODS AND QFD IN THE SELECTION OF GREEN
   SUPPLIERS}},
