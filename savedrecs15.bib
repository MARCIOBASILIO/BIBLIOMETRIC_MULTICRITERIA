@inproceedings{ ISI:000417344100024,
Author = {Guliak, Roman},
Editor = {{Nemec, R and Chytilova, L}},
Title = {{Two-factor Outranking Approach to MCDM Methods for the Lagging Regions
   Determination}},
Booktitle = {{PROCEEDINGS OF THE 12TH INTERNATIONAL CONFERENCE ON STRATEGIC MANAGEMENT
   AND ITS SUPPORT BY INFORMATION SYSTEMS (SMSIS)}},
Series = {{Proceedings of the International Conference on Strategic Management and
   its Support by Information Systems}},
Year = {{2017}},
Pages = {{219-230}},
Note = {{12th International Conference on Strategic Management and its Support by
   Information Systems (SMSIS), Ostrava, CZECH REPUBLIC, MAY 25-26, 2017}},
Organization = {{Tech Univ Ostrava, Fac Econ; Czech Soc Syst Integrat; Dept Syst Engn}},
Abstract = {{The determination of lagging regions is a problem of high importance,
   especially in the context of Regional Cohesion policy. MCDM methods are
   commonly applied as the possible tool for solving of this problem. The
   outranking elements were embedded into the selected MCDM methods (VIKOR,
   TOPSIS) to avoid highlighted drawbacks of original methods. Suggested
   two-factor outranking approach decreases the compensating effect and
   provides clear determination of lagging regions by the hybrid MCDM
   methods. To define well-grounded lagging cluster of regions the
   combination of MCDM and PrincipalComponent Analysis is proposed. The
   ranks obtained by the hybrid versions of MCDM methods according to the
   Kendall's Tau coefficient are found to be almost identical. However the
   VIKOR method appeared to be more distinctive and less sensitive one,
   producing more incomparable alternatives sharing the same rank. In
   addition, the patterns of the Visegrad countries were defined
   highlighting priorities in the development of NUTS 2 regions.}},
