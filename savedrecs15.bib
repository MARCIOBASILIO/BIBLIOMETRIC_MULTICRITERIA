@article{ ISI:000393777100031,
Author = {Banihabib, Mohammad Ebrahim and Shabestari, Mohammad Hadi},
Title = {{Fuzzy Hybrid MCDM Model for Ranking the Agricultural Water Demand
   Management Strategies in Arid Areas}},
Journal = {{WATER RESOURCES MANAGEMENT}},
Year = {{2017}},
