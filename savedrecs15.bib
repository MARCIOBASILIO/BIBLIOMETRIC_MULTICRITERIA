@article{ ISI:000461274900007,
Author = {Banerjee, Durga and Giri, Bibhas C. and Pramanik, Surapati and
   Smarandache, Florentin},
Title = {{GRA for Multi Attribute Decision Making in Neutrosophic Cubic Set
   Environment}},
Journal = {{NEUTROSOPHIC SETS AND SYSTEMS}},
Year = {{2017}},
Volume = {{15}},
Pages = {{60-69}},
Abstract = {{In this paper, multi attribute decision making problem based on grey
   relational analysis in neutrosophic cubic set environment is
   investigated. In the decision making situation, the attribute weights
   are considered as single valued neutrosophic sets. The neutrosophic
   weights are converted into crisp weights. Both positve and negative GRA
   coefficients, and weighted GRA coefficients are determined. Hamming
   distances for weighted GRA coefficients and standard (ideal) GRA
   coefficients are determined. The relative closeness coefficients are
   derived in order to rank the alternatives. The relative closeness
   coefficients are designed in ascending order. Finally, a numerical
   example is solved to demonstrate the applicability of the proposed
   approach.}},
