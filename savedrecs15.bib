@article{ ISI:000389891200004,
Author = {Saayman, Melville and Martin, Juan Carlos and Roman, Concepcion},
Title = {{There is no fuzziness when it comes to measuring service quality in
   national parks}},
Journal = {{TOURISM ECONOMICS}},
Year = {{2016}},
Volume = {{22}},
Number = {{6, SI}},
Pages = {{1207-1224}},
Month = {{DEC}},
Note = {{5th Conference of the International-Association-for-Tourism-Economics
   (IATE), Hong Kong Polytechn Univ, Hong Kong, PEOPLES R CHINA, JUN 30-JUL
   03, 2015}},
Organization = {{Leeds Beckett Univ; Tourism Econ Journal; United Nat World Tourism Org}},
Abstract = {{Nature-based tourism is the leitmotiv for South African National Parks
   (SANParks) and Kruger National Park is an icon within the system. The
   mission and vision of SANParks have evolved from pure conservationism to
   a more recreation- and customer-oriented agent. The aim of this article
   is to address the issue of service quality (SQ) provided to satisfy the
   visitors' needs, demands and expectations. A questionnaire with eight
   different dimensions and 31 SQ attributes is used to evaluate the SQ for
   the period April 2012 to March 2013. A method based on a fuzzy
   multi-criteria decision-making model is applied to dynamically evaluate
   the SQ in the park. Our results suggest that the SQ differs depending on
   which time of the year the park is visited, opening an important venue
   to park managers about whether a price discrimination policy could
   moderate this observed effect.}},
