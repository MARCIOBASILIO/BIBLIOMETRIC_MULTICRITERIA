@article{ ISI:000410686100233,
Author = {Srirangan, Arun Kumar and Sathiya, Paulraj},
Title = {{Optimisation of Process Parameters for Gas Tungsten Arc Welding of
   Incoloy 800HT Using TOPSIS}},
Journal = {{MATERIALS TODAY-PROCEEDINGS}},
Year = {{2017}},
Volume = {{4}},
Number = {{2, A}},
Pages = {{2031-2039}},
Note = {{5th International Conference on Materials Processing and
   Characterization (ICMPC), Gokaraju Rangaraju Inst Engn \& Technol,
   Hyderabad, INDIA, MAR 12-13, 2016}},
Abstract = {{Multi-Criteria Decision Making (MCDM) methods have received much
   attention from researchers and practitioners in evaluating, assessing
   and ranking alternatives across diverse industries. Among numerous MCDA/
   MCDM methods developed to solve real-world decision problems, the
   Technique for Order Preference by Similarity to Ideal Solution (TOPSIS)
   continues to work satisfactorily across different application areas.
   This paper proposes the TOPSIS-based Taguchi optimization approach to
   determine the optimal TIG welding process parameter for welding incoloy
   800HT using N82 filler wire. The experiment was conducted according to
   L9 orthogonal array. The input parameters considered were the welding
   current, voltage and welding speed with a constant gas flow rate of 15
   lpm. From applying the TOPSIS technique, it was found that weldment 4
   has the best weld mechanical properties with ultimate tensile strength
   at room temperature (628 MPa), Ultimate tensile strength at 750 degrees
   C (591 MPa) and Charpy V-notch impact energy (60J). Also the
   relationship between the input parameters and the output parameters was
   examined. Further microscopic and fractography analysis were made and
   correlated with the mechanical properties. 2214-7853 (C) 2017 Elsevier
   Ltd. All rights reserved.}},
