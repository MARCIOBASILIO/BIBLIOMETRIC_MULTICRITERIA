@article{ ISI:000407960800053,
Author = {Toma, Elena},
Title = {{RANKING EUROPEAN COP FARMS IN TERMS OF FINANCIAL VIABILITY THROUGH A
   PCA-TOPSIS APPROACH}},
Journal = {{SCIENTIFIC PAPERS-SERIES MANAGEMENT ECONOMIC ENGINEERING IN AGRICULTURE
   AND RURAL DEVELOPMENT}},
Year = {{2017}},
Volume = {{17}},
Number = {{2}},
Pages = {{381-386}},
Abstract = {{The aim of this paper is to assess the evolution of European farms from
   COP sector (in 2009 and 2013) in terms of financial viability. In our
   research approach, we applied a combination between the non-parametric
   method PCA, the multi-criteria decision analysis method TOPSIS and
   k-means cluster analysis. Our database comprised eight ratios,
   calculated based on FADN data from 94 regions specialised in COP
   production (23 countries). The results revealed the disparities created
   between farms during the 2009-2013 period due to different agricultural
   political conditions and to different recovery from financial crisis.}},
