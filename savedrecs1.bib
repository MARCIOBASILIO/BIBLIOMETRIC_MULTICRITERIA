Author = {Bakhshi, Mohammad and Yaghoobnezhad, Ahmad and Maram, Hashem Nikoo},
Title = {{Developing a Hybrid Approach to Credit Priority based on Accounting
   Variables (Using Analytical Network Process (ANP) and Multi-Criteria
   Decision-Making)}},
Journal = {{INTERNATIONAL JOURNAL OF NONLINEAR ANALYSIS AND APPLICATIONS}},
