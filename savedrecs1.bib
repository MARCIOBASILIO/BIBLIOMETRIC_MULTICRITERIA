@article{ ISI:000638915900001,
Author = {Vakilipour, Samira and Sadeghi-Niaraki, Abolghasem and Ghodousi, Mostafa
   and Choi, Soo-Mi},
Title = {{Comparison between Multi-Criteria Decision-Making Methods and Evaluating
   the Quality of Life at Different Spatial Levels}},
Journal = {{SUSTAINABILITY}},
