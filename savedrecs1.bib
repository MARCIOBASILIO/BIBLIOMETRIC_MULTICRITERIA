@article{ ISI:000634636400003,
Author = {Yuan, Xiaojian and Wu, Wenbing},
Title = {{The WASPAS and AHP optimization methods applied on vibro-diagnostic
   models for rotational machines}},
Journal = {{ENGINEERING WITH COMPUTERS}},
Abstract = {{The optimization of a vibro-diagnostic model (VDM) implies the
   definition of such a model, which fulfills the condition for the minimum
   number of vibration formats needed for the precise diagnostics of a
   broad range of defects on all types of rotational machines. In addition
   to the minimum number of sets, the VDM must satisfy other criteria such
   as providing the maximum possibility of detection of the existence of a
   certain dynamic problem cause in the machine systems, having economic
   justification and simplicity in use. In this paper, two multi-criteria
   decision-making methods were used for finding optimal VDM. The methods
   include weighted aggregated sum product assessment and analytic
   hierarchy process.}},
