@article{ ISI:000612175800019,
Author = {Wang, Jiajia and Ma, Xueling and Xu, Zeshui and Zhan, Jianming},
Title = {{Three-way multi-attribute decision making under hesitant fuzzy
   environments}},
Journal = {{INFORMATION SCIENCES}},
Year = {{2021}},
