@article{ ISI:000632938300007,
Author = {Chowdhury, Mousumi and Paul, Prabir Kumar},
Title = {{Identification of suitable sites for rainwater harvesting using fuzzy
   AHP and fuzzy gamma operator: a case study}},
Journal = {{ARABIAN JOURNAL OF GEOSCIENCES}},
