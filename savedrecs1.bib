@article{ ISI:000610450000001,
Author = {Zhao, Mengwei and Wei, Guiwu and Wei, Cun and Guo, Yanfeng},
Title = {{CPT-TODIM method for bipolar fuzzy multi-attribute group decision making
   and its application to network security service provider selection}},
Journal = {{INTERNATIONAL JOURNAL OF INTELLIGENT SYSTEMS}},
