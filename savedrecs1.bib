@article{ ISI:000624817400001,
Author = {Vehbi, Beser Oktay and Gunce, Kagan and Iranmanesh, Aminreza},
Title = {{Multi-Criteria Assessment for Defining Compatible New Use: Old
   Administrative Hospital, Kyrenia, Cyprus}},
Journal = {{SUSTAINABILITY}},
