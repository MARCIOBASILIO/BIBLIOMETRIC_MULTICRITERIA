@article{ ISI:000629420400002,
Author = {Yu, Dejian and Kou, Gang and Xu, Zeshui and Shi, Shunshun},
Title = {{Analysis of Collaboration Evolution in AHP Research: 1982-2018}},
Journal = {{INTERNATIONAL JOURNAL OF INFORMATION TECHNOLOGY \& DECISION MAKING}},
