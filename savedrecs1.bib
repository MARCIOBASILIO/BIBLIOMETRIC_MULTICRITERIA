@article{ ISI:000605494200001,
Author = {Patra, Pritam and Angira, Mahesh},
Title = {{Investigation into the Selection of Dielectric Material using MCDM
   Methodologies for MEMS Switches with High Power}},
Journal = {{TRANSACTIONS ON ELECTRICAL AND ELECTRONIC MATERIALS}},
Abstract = {{This paper presents MCDM techniques (TOPSIS and VIKOR) for determining
   the optimum dielectric material for MEMS technology based high power
   capacitive switches. These techniques are used for picking the best
   material from a given set of materials under some given criterion. The
   capacitance ratio and dielectric leakage current which are important
   parameters in obtaining the good RF-response of a switch have been taken
   into consideration along with other parameters like thermal expansion,
   heat dissipation and melting point for choosing the best material. Under
   high power condition, chosen material must have lowest value of TCE,
   highest possible thermal conductivity, highest melting point, along with
   highest value of electrical resistivity and relative permittivity for
   good RF response. The result of both the methods shows that AlN is the
   best material followed by TiO2.}},
