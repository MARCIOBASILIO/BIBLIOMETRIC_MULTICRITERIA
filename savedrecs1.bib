@article{ ISI:000606807200011,
Author = {Wu, Min-Chao and Mao, Jun-Jun and Yao, Ai-Ting and Wu, Tao},
Title = {{The novel entropy measurements of Z(+)-numbers and their application on
   multi-attribute decision making problem}},
Journal = {{JOURNAL OF INTELLIGENT \& FUZZY SYSTEMS}},
Year = {{2021}},
Volume = {{40}},
Number = {{1}},
Pages = {{131-148}},
Abstract = {{Z(+)-numbers, which carry more information than Z-numbers, are studied
   in this paper. Based on existed models, two more scientific and
   reasonable probability models of Z(+)-numbers are developed. In order to
   utilize Z(+)-numbers to solve practical problems, the a-cut set of
   Z(+)-numbers and corresponding utility function are proposed. Meanwhile,
   according to the structure of Z(+)-numbers, the entropy, cross-entropy
   and comprehensive cross-entropy are introduced to measure the
   uncertainty and fuzziness of Z(+)-numbers information. Furthermore, a
   linear programming model based on proposed three kinds of entropy is
   designed to obtain the weight vector of criteria in decision-making
   problems. Finally, we provide an example by selecting an optimal design
   of electricity vehicles charge station(DEVCS) combined the PROMETHEE
   method with Z(+)-numbers, and the feasibility of the proposed method are
   verified.}},
