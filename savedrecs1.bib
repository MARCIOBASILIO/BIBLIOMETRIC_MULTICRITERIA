@article{ ISI:000634223700001,
Author = {Vinogradova-Zinkevic, Irina and Podvezko, Valentinas and Zavadskas,
   Edmundas Kazimeras},
Title = {{Comparative Assessment of the Stability of AHP and FAHP Methods}},
Journal = {{SYMMETRY-BASEL}},
