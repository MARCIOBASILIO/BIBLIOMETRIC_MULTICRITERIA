@article{ ISI:000605157700029,
Author = {Akram, Muhammad and Ilyas, Farwa and Al-Kenani, Ahmad N.},
Title = {{Two-Phase Group Decision-Aiding System using ELECTRE III Method in
   Pythagorean Fuzzy Environment}},
Journal = {{ARABIAN JOURNAL FOR SCIENCE AND ENGINEERING}},
