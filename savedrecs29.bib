Author = {Li, Wu and Chen, Yonggang and Chen, Yang},
Book-Group-Author = {{IEEE}},
Title = {{Generalizing TOPSIS for Multi-criteria Group Decision-making with
   Weighted Ordinal Preferences}},
Booktitle = {{2008 7TH WORLD CONGRESS ON INTELLIGENT CONTROL AND AUTOMATION, VOLS 1-23}},
