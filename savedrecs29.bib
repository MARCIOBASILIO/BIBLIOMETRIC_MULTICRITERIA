Author = {Fan, Tuan-Fang and Liau, Churn-Jung and Liu, Duen-Ren},
Editor = {{Carvalho, JP and Kaymak, DU and Sousa, JMC}},
Title = {{Dominance-based Rough Set Analysis of Uncertain Data Tables}},
Booktitle = {{PROCEEDINGS OF THE JOINT 2009 INTERNATIONAL FUZZY SYSTEMS ASSOCIATION
   WORLD CONGRESS AND 2009 EUROPEAN SOCIETY OF FUZZY LOGIC AND TECHNOLOGY
   CONFERENCE}},
