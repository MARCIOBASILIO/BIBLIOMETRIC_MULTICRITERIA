@inproceedings{ ISI:000261490900099,
Author = {Su, Zhixin and Xia, Guoping},
Editor = {{Xia, G and Osaki, H}},
Title = {{STUDY ON MULTI-ATTRIBUTE DECISION MAKING METHOD BASED ON TOPSIS AND GREY
   RELATION ANALYSIS}},
Booktitle = {{ICIM 2008: PROCEEDINGS OF THE NINTH INTERNATIONAL CONFERENCE ON
   INDUSTRIAL MANAGEMENT}},
Year = {{2008}},
Pages = {{675-680}},
Note = {{9th International Conference on Industrial Management, Osaka, JAPAN, SEP
   16-18, 2008}},
Organization = {{Beihang Univ; Japan Ind Management Assoc, Chugoku Shikoku Branch}},
Abstract = {{In practice, multi-attribute evaluation has to be considered to obtain a
   reliable decision. But it' s difficult to ensure the result's
   reliability by traditional method because of its shortages. Therefore,
   this study presents a new multi-attribute decision-making method based
   on TOPSIS and grey relation analysis. We give the concept of
   combinational weight to determine the decision making attributes weights
   and define weighted Euclidean distance and weighted grey correlation
   degree to measure the similarity of location and shape among the
   selected alternative, the ideal alternative and negative ideal
   alternative, respectively. And then, a new relative similarity degree is
   established to achieve comprehensive evaluation among alternatives.
   Through a case of gas-building alternatives selection, we illustrate its
   application and demonstrate its feasibility and practicability. Results
   show that the proposed method is efficient and robust.}},
Publisher = {{CHINA AVIATION INDUSTRY PRESS}},
Address = {{NO 2 YARD, BEIYUAN RD, CHAOYANG, BEIJING, 100009, PEOPLES R CHINA}},
