@article{ ISI:000262600300010,
Author = {Godaliyadde, D. and Godaliyadde, D. D. K. and Phylip-Jones, G. and Yang,
   Z. L. and Batako, A. D. and Wang, J.},
Title = {{A Subjective Cost-Benefit Analysis Approach for Selecting Ship
   Propulsion Systems}},
Journal = {{MARINE TECHNOLOGY SOCIETY JOURNAL}},
Year = {{2008}},
Volume = {{42}},
Number = {{4}},
Pages = {{69-86}},
Month = {{WIN}},
Abstract = {{A ship propulsion system is the major contributor to the occurrence of
   Ship Hull Vibration (SHV), which can easily lead to marine tragedies.
   Wrongly selecting ship propulsion systems will also significantly
   increase ship owners' operational costs. Based on a hierarchical
   structure for modelling propulsion systems, a subjective novel
   cost-benefit criteria analysis approach is developed to select the most
   economical propulsion system with the consideration of vibration
   characteristics. The weight of all the criteria are estimated by
   utilizing an Analytical Hierarchy Process (AHP) technique. All the
   quantitative in the hierarchy are converted into the qualitative
   criteria by proposing a novel approach. Membership Functions (MFs) of
   continuous fuzzy sets are employed to estimate the fuzzy performance
   rating of all the criteria. By taking into account fuzzy performance
   rating of all the criteria, a fuzzy Technique for Order Preference by
   Similarity to Ideal Solution (TOPSIS) decision matrix is constructed.
   Finally, the most economical propulsion system, with consideration of
   vibration characteristics, is selected using the fuzzy TOPSIS method.
   The results of this paper reveal that the fuzzy TOPSIS is suitable for
   propulsion system selection.}},
