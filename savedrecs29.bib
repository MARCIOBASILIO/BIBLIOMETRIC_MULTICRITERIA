@inproceedings{ ISI:000259032600057,
Author = {Yang, Jun and Ai, Xin and Zhao, Tao},
Book-Group-Author = {{IEEE}},
Title = {{An evidential reasoning approach to risk assessments in composite
   generation and transmission system}},
Booktitle = {{2008 THIRD INTERNATIONAL CONFERENCE ON ELECTRIC UTILITY DEREGULATION AND
   RESTRUCTURING AND POWER TECHNOLOGIES, VOLS 1-6}},
Year = {{2008}},
