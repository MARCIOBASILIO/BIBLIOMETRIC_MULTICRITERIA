Editor = {{Wang, Q and Zhang, XD and Xu, B and Wu, BC}},
Title = {{A Selection Model for New Product Development Project Based on a New
   Multicriteria Sorting Procedure}},
Booktitle = {{PROCEEDINGS OF 2009 CONFERENCE ON SYSTEMS SCIENCE, MANAGEMENT SCIENCE \&
   SYSTEM DYNAMICS, VOL 2}},
