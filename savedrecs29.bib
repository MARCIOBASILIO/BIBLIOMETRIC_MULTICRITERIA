@article{ ISI:000259762700020,
Author = {Averna Valente, Roberta de Oliveira and Vettorazzi, Carlos Alberto},
Title = {{Definition of priority areas for forest conservation through the ordered
   weighted averaging method}},
Journal = {{FOREST ECOLOGY AND MANAGEMENT}},
