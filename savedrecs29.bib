@article{ ISI:000255690900003,
Author = {Caterino, Nicola and Iervolino, Iunio and Manfredi, Gaetano and Cosenza,
   Edoardo},
Title = {{Multi-Criteria Decision Making for seismic retrofitting of RC structures}},
Journal = {{JOURNAL OF EARTHQUAKE ENGINEERING}},
