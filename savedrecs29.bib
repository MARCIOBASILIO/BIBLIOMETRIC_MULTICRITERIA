Author = {Namin, E. Samimi and Shahriar, K. and Ataee-pour, M. and Dehghani, H.},
Title = {{A new model for mining method selection of mineral deposit based on
   fuzzy decision making}},
Journal = {{JOURNAL OF THE SOUTHERN AFRICAN INSTITUTE OF MINING AND METALLURGY}},
Year = {{2008}},
Volume = {{108}},
