@article{ ISI:000246975600026,
Author = {Doukas, Haris Ch. and Andreas, Botsikas M. and Psarras, John E.},
Title = {{Multi-criteria decision aid for the formulation of sustainable
   technological energy priorities using linguistic variables}},
