@inproceedings{ ISI:000263701400062,
Author = {Wang, Jian-Qiang and Sun, Teng},
Book-Group-Author = {{IEEE Computer Society}},
Title = {{Fuzzy Multiple Criteria Decision Making Method Based on Prospect Theory}},
