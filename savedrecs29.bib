Booktitle = {{PROCEEDINGS OF THE EIGHTH INTERNATIONAL CONFERENCE ON INFORMATION AND
   MANAGEMENT SCIENCES}},
Series = {{Series of Information and Management Sciences}},
Year = {{2009}},
