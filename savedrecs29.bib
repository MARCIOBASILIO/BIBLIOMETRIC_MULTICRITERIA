@article{ ISI:000262132000010,
Author = {Thiel, Tomasz},
Title = {{DETERMINATION OF THE RELATIVE IMPORTANCE OF CRITERIA WHEN THE NUMBER OF
   PEOPLE JUDGING IS A SMALL SAMPLE}},
