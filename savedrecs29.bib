@article{ ISI:000255858000004,
Author = {Lee, Kuo-Liang},
Title = {{Analyzing the competitive relations among the location in the
   Asia-Pacific region for developing the re-export type of global
   logistics hub}},
Journal = {{JOURNAL OF MARINE SCIENCE AND TECHNOLOGY-TAIWAN}},
Year = {{2007}},
Volume = {{15}},
Number = {{3}},
Pages = {{187-200}},
Month = {{SEP}},
Abstract = {{This research empirically investigates competitive relations among
   locations developing the re-export type of global logistics hubs (GLH)
   in the Asia-Pacific region from the perspective of logistics service
   providers. A quantitative SWOT analytical procedure that integrates the
   AHP method and the fuzzy theory of graded mean integration
   representations was utilized to empirically evaluate the Multiple
   Criteria Decision Making (MCDM) problem. Respondents located Shenzhen,
   Busan and Kaohsiung locate in the SO quadrant and Shanghai, HK and
   Singapore in the WT quadrant. Shanghai could be expected to move into
   the leading group in the future. Finally, the concept of Grand Strategy
   Matrix (GSM) was used to suggest a suitable competitive strategy for
   location on particular competitive conditions.}},
